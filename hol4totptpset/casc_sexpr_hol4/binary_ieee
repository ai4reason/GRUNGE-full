; binary_ieee.float_div_plus_infinity_finite
; ∀mode x r.     (float_value x = Float r) ⇒     ((mode float_div float_plus_infinity (:τ # χ)) x =      (clear_flags,       if x.Sign = 0w then float_plus_infinity (:τ # χ)       else float_minus_infinity (:τ # χ)))
(conjecture "thm_2Ebinary__ieee_2Efloat__div__plus__infinity__finite" (forall_tyvarl_term ("A_27t" "A_27w") (("c_2Ebool_2E_21" "ty_2Ebinary__ieee_2Erounding") (lambda "V0mode" "ty_2Ebinary__ieee_2Erounding" (("c_2Ebool_2E_21" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w")) (lambda "V1x" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w") (("c_2Ebool_2E_21" "ty_2Erealax_2Ereal") (lambda "V2r" "ty_2Erealax_2Ereal" ("c_2Emin_2E_3D_3D_3E" (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Efloat__value") (("c_2Ebinary__ieee_2Efloat__value" "A_27t" "A_27w") "V1x") ("c_2Ebinary__ieee_2EFloat" "V2r")) (("c_2Emin_2E_3D" ("ty_2Epair_2Eprod" "ty_2Ebinary__ieee_2Eflags" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w"))) (("c_2Ebinary__ieee_2Efloat__div" "A_27t" "A_27w") "V0mode" (("c_2Ebinary__ieee_2Efloat__plus__infinity" "A_27t" "A_27w") ("c_2Ebool_2Ethe__value" ("ty_2Epair_2Eprod" "A_27t" "A_27w"))) "V1x") (("c_2Epair_2E_2C" "ty_2Ebinary__ieee_2Eflags" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w")) "c_2Ebinary__ieee_2Eclear__flags" (("c_2Ebool_2ECOND" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w")) (("c_2Emin_2E_3D" ("ty_2Efcp_2Ecart" "ty_2Emin_2Ebool" "ty_2Eone_2Eone")) (("c_2Ebinary__ieee_2Efloat__Sign" "A_27t" "A_27w") "V1x") (("c_2Ewords_2En2w" "ty_2Eone_2Eone") "c_2Enum_2E0")) (("c_2Ebinary__ieee_2Efloat__plus__infinity" "A_27t" "A_27w") ("c_2Ebool_2Ethe__value" ("ty_2Epair_2Eprod" "A_27t" "A_27w"))) (("c_2Ebinary__ieee_2Efloat__minus__infinity" "A_27t" "A_27w") ("c_2Ebool_2Ethe__value" ("ty_2Epair_2Eprod" "A_27t" "A_27w")))))))))))))))
; binary_ieee.float_div_finite_minus_infinity
; ∀mode x r.     (float_value x = Float r) ⇒     ((mode float_div x) (float_minus_infinity (:τ # χ)) =      (clear_flags,       if x.Sign = 0w then float_minus_zero (:τ # χ)       else float_plus_zero (:τ # χ)))
(conjecture "thm_2Ebinary__ieee_2Efloat__div__finite__minus__infinity" (forall_tyvarl_term ("A_27t" "A_27w") (("c_2Ebool_2E_21" "ty_2Ebinary__ieee_2Erounding") (lambda "V0mode" "ty_2Ebinary__ieee_2Erounding" (("c_2Ebool_2E_21" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w")) (lambda "V1x" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w") (("c_2Ebool_2E_21" "ty_2Erealax_2Ereal") (lambda "V2r" "ty_2Erealax_2Ereal" ("c_2Emin_2E_3D_3D_3E" (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Efloat__value") (("c_2Ebinary__ieee_2Efloat__value" "A_27t" "A_27w") "V1x") ("c_2Ebinary__ieee_2EFloat" "V2r")) (("c_2Emin_2E_3D" ("ty_2Epair_2Eprod" "ty_2Ebinary__ieee_2Eflags" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w"))) (("c_2Ebinary__ieee_2Efloat__div" "A_27t" "A_27w") "V0mode" "V1x" (("c_2Ebinary__ieee_2Efloat__minus__infinity" "A_27t" "A_27w") ("c_2Ebool_2Ethe__value" ("ty_2Epair_2Eprod" "A_27t" "A_27w")))) (("c_2Epair_2E_2C" "ty_2Ebinary__ieee_2Eflags" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w")) "c_2Ebinary__ieee_2Eclear__flags" (("c_2Ebool_2ECOND" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w")) (("c_2Emin_2E_3D" ("ty_2Efcp_2Ecart" "ty_2Emin_2Ebool" "ty_2Eone_2Eone")) (("c_2Ebinary__ieee_2Efloat__Sign" "A_27t" "A_27w") "V1x") (("c_2Ewords_2En2w" "ty_2Eone_2Eone") "c_2Enum_2E0")) (("c_2Ebinary__ieee_2Efloat__minus__zero" "A_27t" "A_27w") ("c_2Ebool_2Ethe__value" ("ty_2Epair_2Eprod" "A_27t" "A_27w"))) (("c_2Ebinary__ieee_2Efloat__plus__zero" "A_27t" "A_27w") ("c_2Ebool_2Ethe__value" ("ty_2Epair_2Eprod" "A_27t" "A_27w")))))))))))))))
