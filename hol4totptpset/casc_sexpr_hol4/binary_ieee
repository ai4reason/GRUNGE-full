; binary_ieee.rounding_case_def
; (∀v0 v1 v2 v3.      (case roundTiesToEven of         roundTiesToEven => v0       | roundTowardPositive => v1       | roundTowardNegative => v2       | roundTowardZero => v3) = v0) ∧ (∀v0 v1 v2 v3.      (case roundTowardPositive of         roundTiesToEven => v0       | roundTowardPositive => v1       | roundTowardNegative => v2       | roundTowardZero => v3) = v1) ∧ (∀v0 v1 v2 v3.      (case roundTowardNegative of         roundTiesToEven => v0       | roundTowardPositive => v1       | roundTowardNegative => v2       | roundTowardZero => v3) = v2) ∧ ∀v0 v1 v2 v3.     (case roundTowardZero of        roundTiesToEven => v0      | roundTowardPositive => v1      | roundTowardNegative => v2      | roundTowardZero => v3) = v3
