; binary_ieee.float_add_def
; ∀mode x y.     (mode float_add x) y =     case (float_value x,float_value y) of       (Float r1,Float r2) =>         float_round_with_flags mode           (if (r1 = 0) ∧ (r2 = 0) ∧ (x.Sign = y.Sign) then x.Sign = 1w            else (mode = roundTowardNegative)) (r1 + r2)     | (Float r1,Infinity) => (clear_flags,y)     | (Float r1,NaN) =>       (check_for_signalling [y],float_some_qnan (FP_Add mode x y))     | (Infinity,Float v7) => (clear_flags,x)     | (Infinity,Infinity) =>       if x.Sign = y.Sign then (clear_flags,x)       else (invalidop_flags,float_some_qnan (FP_Add mode x y))     | (Infinity,NaN) =>       (check_for_signalling [y],float_some_qnan (FP_Add mode x y))     | (NaN,v1) =>       (check_for_signalling [x; y],float_some_qnan (FP_Add mode x y))
