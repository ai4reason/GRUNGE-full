; binary_ieee.float_round_to_integral_compute
; (∀m.      float_round_to_integral m (float_minus_infinity (:τ # χ)) =      float_minus_infinity (:τ # χ)) ∧ (∀m.      float_round_to_integral m (float_plus_infinity (:τ # χ)) =      float_plus_infinity (:τ # χ)) ∧ ∀m fp_op.     float_round_to_integral m (float_some_qnan fp_op) = float_some_qnan fp_op
