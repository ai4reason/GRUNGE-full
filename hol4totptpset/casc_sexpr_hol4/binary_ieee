; binary_ieee.float_sub_compute
; (∀mode x fp_op.      (mode float_sub float_some_qnan fp_op) x =      (check_for_signalling [x],       float_some_qnan (FP_Sub mode (float_some_qnan fp_op) x))) ∧ (∀mode x fp_op.      (mode float_sub x) (float_some_qnan fp_op) =      (check_for_signalling [x],       float_some_qnan (FP_Sub mode x (float_some_qnan fp_op)))) ∧ (∀mode.      (mode float_sub float_minus_infinity (:τ # χ))        (float_minus_infinity (:τ # χ)) =      (invalidop_flags,       float_some_qnan         (FP_Sub mode (float_minus_infinity (:τ # χ))            (float_minus_infinity (:τ # χ))))) ∧ (∀mode.      (mode float_sub float_minus_infinity (:τ # χ))        (float_plus_infinity (:τ # χ)) =      (clear_flags,float_minus_infinity (:τ # χ))) ∧ (∀mode.      (mode float_sub float_plus_infinity (:τ # χ))        (float_plus_infinity (:τ # χ)) =      (invalidop_flags,       float_some_qnan         (FP_Sub mode (float_plus_infinity (:τ # χ))            (float_plus_infinity (:τ # χ))))) ∧ ∀mode.     (mode float_sub float_plus_infinity (:τ # χ))       (float_minus_infinity (:τ # χ)) =     (clear_flags,float_plus_infinity (:τ # χ))
