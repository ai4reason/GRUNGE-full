; binary_ieee.largest
; largest (:τ # χ) = &(2 ** (UINT_MAX (:χ) − 1)) * (2 − 1 / &dimword (:τ)) / &(2 ** INT_MAX (:χ))
(conjecture "thm_2Ebinary__ieee_2Elargest" (forall_tyvarl_term ("A_27t" "A_27w") (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Ebinary__ieee_2Elargest" "A_27t" "A_27w") ("c_2Ebool_2Ethe__value" ("ty_2Epair_2Eprod" "A_27t" "A_27w"))) ("c_2Ereal_2E_2F" ("c_2Erealax_2Ereal__mul" ("c_2Ereal_2Ereal__of__num" ("c_2Earithmetic_2EEXP" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT2" "c_2Earithmetic_2EZERO")) ("c_2Earithmetic_2E_2D" (("c_2Ewords_2EUINT__MAX" "A_27w") ("c_2Ebool_2Ethe__value" "A_27w")) ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT1" "c_2Earithmetic_2EZERO"))))) ("c_2Ereal_2Ereal__sub" ("c_2Ereal_2Ereal__of__num" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT2" "c_2Earithmetic_2EZERO"))) ("c_2Ereal_2E_2F" ("c_2Ereal_2Ereal__of__num" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT1" "c_2Earithmetic_2EZERO"))) ("c_2Ereal_2Ereal__of__num" (("c_2Ewords_2Edimword" "A_27t") ("c_2Ebool_2Ethe__value" "A_27t")))))) ("c_2Ereal_2Ereal__of__num" ("c_2Earithmetic_2EEXP" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT2" "c_2Earithmetic_2EZERO")) (("c_2Ewords_2EINT__MAX" "A_27w") ("c_2Ebool_2Ethe__value" "A_27w"))))))))
; binary_ieee.threshold
; threshold (:τ # χ) = &(2 ** (UINT_MAX (:χ) − 1)) * (2 − 1 / &(2 * dimword (:τ))) / &(2 ** INT_MAX (:χ))
(conjecture "thm_2Ebinary__ieee_2Ethreshold" (forall_tyvarl_term ("A_27t" "A_27w") (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Ebinary__ieee_2Ethreshold" "A_27t" "A_27w") ("c_2Ebool_2Ethe__value" ("ty_2Epair_2Eprod" "A_27t" "A_27w"))) ("c_2Ereal_2E_2F" ("c_2Erealax_2Ereal__mul" ("c_2Ereal_2Ereal__of__num" ("c_2Earithmetic_2EEXP" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT2" "c_2Earithmetic_2EZERO")) ("c_2Earithmetic_2E_2D" (("c_2Ewords_2EUINT__MAX" "A_27w") ("c_2Ebool_2Ethe__value" "A_27w")) ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT1" "c_2Earithmetic_2EZERO"))))) ("c_2Ereal_2Ereal__sub" ("c_2Ereal_2Ereal__of__num" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT2" "c_2Earithmetic_2EZERO"))) ("c_2Ereal_2E_2F" ("c_2Ereal_2Ereal__of__num" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT1" "c_2Earithmetic_2EZERO"))) ("c_2Ereal_2Ereal__of__num" ("c_2Earithmetic_2E_2A" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT2" "c_2Earithmetic_2EZERO")) (("c_2Ewords_2Edimword" "A_27t") ("c_2Ebool_2Ethe__value" "A_27t"))))))) ("c_2Ereal_2Ereal__of__num" ("c_2Earithmetic_2EEXP" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT2" "c_2Earithmetic_2EZERO")) (("c_2Ewords_2EINT__MAX" "A_27w") ("c_2Ebool_2Ethe__value" "A_27w"))))))))
; binary_ieee.largest_is_top
; 1 < dimindex (:χ) ⇒ (largest (:τ # χ) = float_to_real (float_top (:τ # χ)))
