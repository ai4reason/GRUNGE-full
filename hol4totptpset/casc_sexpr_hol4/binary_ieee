; binary_ieee.float_value_case_cong
; ∀M M' f v v1.     (M = M') ∧ (∀a. (M' = Float a) ⇒ (f a = f' a)) ∧     ((M' = Infinity) ⇒ (v = v')) ∧ ((M' = NaN) ⇒ (v1 = v1')) ⇒     (float_value_CASE M f v v1 = float_value_CASE M' f' v' v1')
