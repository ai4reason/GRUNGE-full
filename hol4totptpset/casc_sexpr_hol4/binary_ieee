; binary_ieee.float_mul_compute
; (∀mode x fp_op.      (mode float_mul float_some_qnan fp_op) x =      (check_for_signalling [x],       float_some_qnan (FP_Mul mode (float_some_qnan fp_op) x))) ∧ (∀mode x fp_op.      (mode float_mul x) (float_some_qnan fp_op) =      (check_for_signalling [x],       float_some_qnan (FP_Mul mode x (float_some_qnan fp_op)))) ∧ (∀mode.      (mode float_mul float_minus_infinity (:τ # χ))        (float_minus_infinity (:τ # χ)) =      (clear_flags,float_plus_infinity (:τ # χ))) ∧ (∀mode.      (mode float_mul float_minus_infinity (:τ # χ))        (float_plus_infinity (:τ # χ)) =      (clear_flags,float_minus_infinity (:τ # χ))) ∧ (∀mode.      (mode float_mul float_plus_infinity (:τ # χ))        (float_plus_infinity (:τ # χ)) =      (clear_flags,float_plus_infinity (:τ # χ))) ∧ ∀mode.     (mode float_mul float_plus_infinity (:τ # χ))       (float_minus_infinity (:τ # χ)) =     (clear_flags,float_minus_infinity (:τ # χ))
(conjecture "thm_2Ebinary__ieee_2Efloat__mul__compute" (forall_tyvarl_term ("A_27a" "A_27b" "A_27c" "A_27d" "A_27t" "A_27w") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "ty_2Ebinary__ieee_2Erounding") (lambda "V0mode" "ty_2Ebinary__ieee_2Erounding" (("c_2Ebool_2E_21" ("ty_2Ebinary__ieee_2Efloat" "A_27a" "A_27b")) (lambda "V1x" ("ty_2Ebinary__ieee_2Efloat" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Ebinary__ieee_2Efp__op" "A_27a" "A_27b")) (lambda "V2fp__op" ("ty_2Ebinary__ieee_2Efp__op" "A_27a" "A_27b") (("c_2Emin_2E_3D" ("ty_2Epair_2Eprod" "ty_2Ebinary__ieee_2Eflags" ("ty_2Ebinary__ieee_2Efloat" "A_27a" "A_27b"))) (("c_2Ebinary__ieee_2Efloat__mul" "A_27a" "A_27b") "V0mode" (("c_2Ebinary__ieee_2Efloat__some__qnan" "A_27a" "A_27b") "V2fp__op") "V1x") (("c_2Epair_2E_2C" "ty_2Ebinary__ieee_2Eflags" ("ty_2Ebinary__ieee_2Efloat" "A_27a" "A_27b")) (("c_2Ebinary__ieee_2Echeck__for__signalling" "A_27a" "A_27b") (("c_2Elist_2ECONS" ("ty_2Ebinary__ieee_2Efloat" "A_27a" "A_27b")) "V1x" ("c_2Elist_2ENIL" ("ty_2Ebinary__ieee_2Efloat" "A_27a" "A_27b")))) (("c_2Ebinary__ieee_2Efloat__some__qnan" "A_27a" "A_27b") (("c_2Ebinary__ieee_2EFP__Mul" "A_27a" "A_27b") "V0mode" (("c_2Ebinary__ieee_2Efloat__some__qnan" "A_27a" "A_27b") "V2fp__op") "V1x")))))))))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "ty_2Ebinary__ieee_2Erounding") (lambda "V3mode" "ty_2Ebinary__ieee_2Erounding" (("c_2Ebool_2E_21" ("ty_2Ebinary__ieee_2Efloat" "A_27c" "A_27d")) (lambda "V4x" ("ty_2Ebinary__ieee_2Efloat" "A_27c" "A_27d") (("c_2Ebool_2E_21" ("ty_2Ebinary__ieee_2Efp__op" "A_27c" "A_27d")) (lambda "V5fp__op" ("ty_2Ebinary__ieee_2Efp__op" "A_27c" "A_27d") (("c_2Emin_2E_3D" ("ty_2Epair_2Eprod" "ty_2Ebinary__ieee_2Eflags" ("ty_2Ebinary__ieee_2Efloat" "A_27c" "A_27d"))) (("c_2Ebinary__ieee_2Efloat__mul" "A_27c" "A_27d") "V3mode" "V4x" (("c_2Ebinary__ieee_2Efloat__some__qnan" "A_27c" "A_27d") "V5fp__op")) (("c_2Epair_2E_2C" "ty_2Ebinary__ieee_2Eflags" ("ty_2Ebinary__ieee_2Efloat" "A_27c" "A_27d")) (("c_2Ebinary__ieee_2Ec