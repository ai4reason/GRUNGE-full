; binary_ieee.float_to_int_def
; ∀mode x.     float_to_int mode x =     case float_value x of       Float r =>         SOME           (case mode of            roundTiesToEven =>              (let                 f = flr r ;                 df = abs (r − real_of_int f)               in                 if df < 1 / 2 ∨ (df = 1 / 2) ∧ EVEN (Num (ABS f)) then f                 else clg r)          | roundTowardPositive => clg r          | roundTowardNegative => flr r          | roundTowardZero => if x.Sign = 1w then clg r else flr r)     | Infinity => NONE     | NaN => NONE
