; binary_ieee.float_infinity_negate_abs
; (float_negate (float_plus_infinity (:τ # χ)) = float_minus_infinity (:τ # χ)) ∧ (float_negate (float_minus_infinity (:τ # χ)) = float_plus_infinity (:τ # χ)) ∧ (float_abs (float_plus_infinity (:τ # χ)) = float_plus_infinity (:τ # χ)) ∧ (float_abs (float_minus_infinity (:τ # χ)) = float_plus_infinity (:τ # χ))
