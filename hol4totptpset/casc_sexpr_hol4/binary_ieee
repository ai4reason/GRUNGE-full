; binary_ieee.float_sub_def
; ∀mode x y.     (mode float_sub x) y =     case (float_value x,float_value y) of       (Float r1,Float r2) =>         float_round_with_flags mode           (if (r1 = 0) ∧ (r2 = 0) ∧ x.Sign ≠ y.Sign then x.Sign = 1w            else (mode = roundTowardNegative)) (r1 − r2)     | (Float r1,Infinity) => (clear_flags,float_negate y)     | (Float r1,NaN) =>       (check_for_signalling [y],float_some_qnan (FP_Sub mode x y))     | (Infinity,Float v7) => (clear_flags,x)     | (Infinity,Infinity) =>       if x.Sign = y.Sign then         (invalidop_flags,float_some_qnan (FP_Sub mode x y))       else (clear_flags,x)     | (Infinity,NaN) =>       (check_for_signalling [y],float_some_qnan (FP_Sub mode x y))     | (NaN,v1) =>       (check_for_signalling [x; y],float_some_qnan (FP_Sub mode x y))
