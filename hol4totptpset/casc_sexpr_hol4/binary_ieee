; binary_ieee.rounding_case_eq
; ((case x of     roundTiesToEven => v0   | roundTowardPositive => v1   | roundTowardNegative => v2   | roundTowardZero => v3) = v) ⇔ (x = roundTiesToEven) ∧ (v0 = v) ∨ (x = roundTowardPositive) ∧ (v1 = v) ∨ (x = roundTowardNegative) ∧ (v2 = v) ∨ (x = roundTowardZero) ∧ (v3 = v)
