; binary_ieee.flags_component_equality
; ∀f1 f2.     (f1 = f2) ⇔     (f1.DivideByZero ⇔ f2.DivideByZero) ∧ (f1.InvalidOp ⇔ f2.InvalidOp) ∧     (f1.Overflow ⇔ f2.Overflow) ∧ (f1.Precision ⇔ f2.Precision) ∧     (f1.Underflow_BeforeRounding ⇔ f2.Underflow_BeforeRounding) ∧     (f1.Underflow_AfterRounding ⇔ f2.Underflow_AfterRounding)
(conjecture "thm_2Ebinary__ieee_2Eflags__component__equality" (("c_2Ebool_2E_21" "ty_2Ebinary__ieee_2Eflags") (lambda "V0f1" "ty_2Ebinary__ieee_2Eflags" (("c_2Ebool_2E_21" "ty_2Ebinary__ieee_2Eflags") (lambda "V1f2" "ty_2Ebinary__ieee_2Eflags" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Eflags") "V0f1" "V1f2") ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") ("c_2Ebinary__ieee_2Eflags__DivideByZero" "V0f1") ("c_2Ebinary__ieee_2Eflags__DivideByZero" "V1f2")) ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") ("c_2Ebinary__ieee_2Eflags__InvalidOp" "V0f1") ("c_2Ebinary__ieee_2Eflags__InvalidOp" "V1f2")) ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") ("c_2Ebinary__ieee_2Eflags__Overflow" "V0f1") ("c_2Ebinary__ieee_2Eflags__Overflow" "V1f2")) ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") ("c_2Ebinary__ieee_2Eflags__Precision" "V0f1") ("c_2Ebinary__ieee_2Eflags__Precision" "V1f2")) ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") ("c_2Ebinary__ieee_2Eflags__Underflow__BeforeRounding" "V0f1") ("c_2Ebinary__ieee_2Eflags__Underflow__BeforeRounding" "V1f2")) (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") ("c_2Ebinary__ieee_2Eflags__Underflow__AfterRounding" "V0f1") ("c_2Ebinary__ieee_2Eflags__Underflow__AfterRounding" "V1f2")))))))))))))
; binary_ieee.flags_updates_eq_literal
; ∀f b4 b3 b2 b1 b0 b.     f with     <|DivideByZero := b4; InvalidOp := b3; Overflow := b2; Precision := b1;       Underflow_BeforeRounding := b0; Underflow_AfterRounding := b|> =     <|DivideByZero := b4; InvalidOp := b3; Overflow := b2; Precision := b1;       Underflow_BeforeRounding := b0; Underflow_AfterRounding := b|>
