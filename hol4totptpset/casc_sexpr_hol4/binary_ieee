; binary_ieee.float_mul_add_def
; ∀mode x y z.     float_mul_add mode x y z =     (let        signP = x.Sign ⊕ y.Sign ;        infP = float_is_infinite x ∨ float_is_infinite y      in        if float_is_nan x ∨ float_is_nan y ∨ float_is_nan z then          (check_for_signalling [x; y; z],           float_some_qnan (FP_MulAdd mode x y z))        else if          float_is_infinite x ∧ float_is_zero y ∨          float_is_zero x ∧ float_is_infinite y ∨          float_is_infinite z ∧ infP ∧ signP ≠ z.Sign        then          (invalidop_flags,float_some_qnan (FP_MulAdd mode x y z))        else if float_is_infinite z ∧ (z.Sign = 0w) ∨ infP ∧ (signP = 0w) then          (clear_flags,float_plus_infinity (:τ # χ))        else if float_is_infinite z ∧ (z.Sign = 1w) ∨ infP ∧ (signP = 1w) then          (clear_flags,float_minus_infinity (:τ # χ))        else          (let             r1 = float_to_real x * float_to_real y and r2 = float_to_real z           in             float_round_with_flags mode               (if (r1 = 0) ∧ (r2 = 0) ∧ (signP = z.Sign) then signP = 1w                else (mode = roundTowardNegative)) (r1 + r2)))
