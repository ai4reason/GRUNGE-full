; binary_ieee.datatype_flags
; DATATYPE   (record flags DivideByZero InvalidOp Overflow Precision      Underflow_BeforeRounding Underflow_AfterRounding)
(conjecture "thm_2Ebinary__ieee_2Edatatype__flags" (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Ebinary__ieee_2Eflags" ("ty_2Emin_2Efun" "ty_2Emin_2Ebool" ("ty_2Emin_2Efun" "ty_2Emin_2Ebool" ("ty_2Emin_2Efun" "ty_2Emin_2Ebool" ("ty_2Emin_2Efun" "ty_2Emin_2Ebool" ("ty_2Emin_2Efun" "ty_2Emin_2Ebool" ("ty_2Emin_2Efun" "ty_2Emin_2Ebool" "ty_2Emin_2Ebool")))))))) (lambda "V0record" ("ty_2Emin_2Efun" "ty_2Ebinary__ieee_2Eflags" ("ty_2Emin_2Efun" "ty_2Emin_2Ebool" ("ty_2Emin_2Efun" "ty_2Emin_2Ebool" ("ty_2Emin_2Efun" "ty_2Emin_2Ebool" ("ty_2Emin_2Efun" "ty_2Emin_2Ebool" ("ty_2Emin_2Efun" "ty_2Emin_2Ebool" ("ty_2Emin_2Efun" "ty_2Emin_2Ebool" "ty_2Emin_2Ebool"))))))) (("c_2Ebool_2E_21" "ty_2Ebinary__ieee_2Eflags") (lambda "V1flags" "ty_2Ebinary__ieee_2Eflags" (("c_2Ebool_2E_21" "ty_2Emin_2Ebool") (lambda "V2DivideByZero" "ty_2Emin_2Ebool" (("c_2Ebool_2E_21" "ty_2Emin_2Ebool") (lambda "V3InvalidOp" "ty_2Emin_2Ebool" (("c_2Ebool_2E_21" "ty_2Emin_2Ebool") (lambda "V4Overflow" "ty_2Emin_2Ebool" (("c_2Ebool_2E_21" "ty_2Emin_2Ebool") (lambda "V5Precision" "ty_2Emin_2Ebool" (("c_2Ebool_2E_21" "ty_2Emin_2Ebool") (lambda "V6Underflow__BeforeRounding" "ty_2Emin_2Ebool" (("c_2Ebool_2E_21" "ty_2Emin_2Ebool") (lambda "V7Underflow__AfterRounding" "ty_2Emin_2Ebool" (("c_2Ebool_2EDATATYPE" "ty_2Emin_2Ebool") ("V0record" "V1flags" "V2DivideByZero" "V3InvalidOp" "V4Overflow" "V5Precision" "V6Underflow__BeforeRounding" "V7Underflow__AfterRounding")))))))))))))))))))
; binary_ieee.flags_11
; ∀a0 a1 a2 a3 a4 a5 a0' a1' a2' a3' a4' a5'.     (flags a0 a1 a2 a3 a4 a5 = flags a0' a1' a2' a3' a4' a5') ⇔     (a0 ⇔ a0') ∧ (a1 ⇔ a1') ∧ (a2 ⇔ a2') ∧ (a3 ⇔ a3') ∧ (a4 ⇔ a4') ∧     (a5 ⇔ a5')
