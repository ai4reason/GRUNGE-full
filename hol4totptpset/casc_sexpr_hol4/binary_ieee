; binary_ieee.top_properties
; ¬float_is_zero (float_top (:τ # χ)) ∧ float_is_finite (float_top (:τ # χ)) ∧ ¬float_is_nan (float_top (:τ # χ)) ∧ (float_is_normal (float_top (:τ # χ)) ⇔ dimindex (:χ) ≠ 1) ∧ (float_is_subnormal (float_top (:τ # χ)) ⇔ (dimindex (:χ) = 1)) ∧ ¬float_is_infinite (float_top (:τ # χ))
