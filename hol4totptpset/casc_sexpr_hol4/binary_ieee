; binary_ieee.float_sub_nan
; ∀mode x y.     (float_value x = NaN) ∨ (float_value y = NaN) ⇒     ((mode float_sub x) y =      (check_for_signalling [x; y],float_some_qnan (FP_Sub mode x y)))
(conjecture "thm_2Ebinary__ieee_2Efloat__sub__nan" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" "ty_2Ebinary__ieee_2Erounding") (lambda "V0mode" "ty_2Ebinary__ieee_2Erounding" (("c_2Ebool_2E_21" ("ty_2Ebinary__ieee_2Efloat" "A_27a" "A_27b")) (lambda "V1x" ("ty_2Ebinary__ieee_2Efloat" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Ebinary__ieee_2Efloat" "A_27a" "A_27b")) (lambda "V2y" ("ty_2Ebinary__ieee_2Efloat" "A_27a" "A_27b") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_5C_2F" (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Efloat__value") (("c_2Ebinary__ieee_2Efloat__value" "A_27a" "A_27b") "V1x") "c_2Ebinary__ieee_2ENaN") (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Efloat__value") (("c_2Ebinary__ieee_2Efloat__value" "A_27a" "A_27b") "V2y") "c_2Ebinary__ieee_2ENaN")) (("c_2Emin_2E_3D" ("ty_2Epair_2Eprod" "ty_2Ebinary__ieee_2Eflags" ("ty_2Ebinary__ieee_2Efloat" "A_27a" "A_27b"))) (("c_2Ebinary__ieee_2Efloat__sub" "A_27a" "A_27b") "V0mode" "V1x" "V2y") (("c_2Epair_2E_2C" "ty_2Ebinary__ieee_2Eflags" ("ty_2Ebinary__ieee_2Efloat" "A_27a" "A_27b")) (("c_2Ebinary__ieee_2Echeck__for__signalling" "A_27a" "A_27b") (("c_2Elist_2ECONS" ("ty_2Ebinary__ieee_2Efloat" "A_27a" "A_27b")) "V1x" (("c_2Elist_2ECONS" ("ty_2Ebinary__ieee_2Efloat" "A_27a" "A_27b")) "V2y" ("c_2Elist_2ENIL" ("ty_2Ebinary__ieee_2Efloat" "A_27a" "A_27b"))))) (("c_2Ebinary__ieee_2Efloat__some__qnan" "A_27a" "A_27b") (("c_2Ebinary__ieee_2EFP__Sub" "A_27a" "A_27b") "V0mode" "V1x" "V2y")))))))))))))
; binary_ieee.float_sub_finite
; ∀mode x y r1 r2.     (float_value x = Float r1) ∧ (float_value y = Float r2) ⇒     ((mode float_sub x) y =      float_round_with_flags mode        (if (r1 = 0) ∧ (r2 = 0) ∧ x.Sign ≠ y.Sign then x.Sign = 1w         else (mode = roundTowardNegative)) (r1 − r2))
