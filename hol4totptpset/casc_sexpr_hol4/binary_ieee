; binary_ieee.datatype_rounding
; DATATYPE   (rounding roundTiesToEven roundTowardPositive roundTowardNegative      roundTowardZero)
(conjecture "thm_2Ebinary__ieee_2Edatatype__rounding" (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Ebinary__ieee_2Erounding" ("ty_2Emin_2Efun" "ty_2Ebinary__ieee_2Erounding" ("ty_2Emin_2Efun" "ty_2Ebinary__ieee_2Erounding" ("ty_2Emin_2Efun" "ty_2Ebinary__ieee_2Erounding" "ty_2Emin_2Ebool"))))) (lambda "V0rounding" ("ty_2Emin_2Efun" "ty_2Ebinary__ieee_2Erounding" ("ty_2Emin_2Efun" "ty_2Ebinary__ieee_2Erounding" ("ty_2Emin_2Efun" "ty_2Ebinary__ieee_2Erounding" ("ty_2Emin_2Efun" "ty_2Ebinary__ieee_2Erounding" "ty_2Emin_2Ebool")))) (("c_2Ebool_2EDATATYPE" "ty_2Emin_2Ebool") ("V0rounding" "c_2Ebinary__ieee_2EroundTiesToEven" "c_2Ebinary__ieee_2EroundTowardPositive" "c_2Ebinary__ieee_2EroundTowardNegative" "c_2Ebinary__ieee_2EroundTowardZero")))))
; binary_ieee.rounding_distinct
; roundTiesToEven ≠ roundTowardPositive ∧ roundTiesToEven ≠ roundTowardNegative ∧ roundTiesToEven ≠ roundTowardZero ∧ roundTowardPositive ≠ roundTowardNegative ∧ roundTowardPositive ≠ roundTowardZero ∧ roundTowardNegative ≠ roundTowardZero
(conjecture "thm_2Ebinary__ieee_2Erounding__distinct" ("c_2Ebool_2E_2F_5C" ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Erounding") "c_2Ebinary__ieee_2EroundTiesToEven" "c_2Ebinary__ieee_2EroundTowardPositive")) ("c_2Ebool_2E_2F_5C" ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Erounding") "c_2Ebinary__ieee_2EroundTiesToEven" "c_2Ebinary__ieee_2EroundTowardNegative")) ("c_2Ebool_2E_2F_5C" ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Erounding") "c_2Ebinary__ieee_2EroundTiesToEven" "c_2Ebinary__ieee_2EroundTowardZero")) ("c_2Ebool_2E_2F_5C" ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Erounding") "c_2Ebinary__ieee_2EroundTowardPositive" "c_2Ebinary__ieee_2EroundTowardNegative")) ("c_2Ebool_2E_2F_5C" ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Erounding") "c_2Ebinary__ieee_2EroundTowardPositive" "c_2Ebinary__ieee_2EroundTowardZero")) ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Erounding") "c_2Ebinary__ieee_2EroundTowardNegative" "c_2Ebinary__ieee_2EroundTowardZero"))))))))
; binary_ieee.rounding_nchotomy
; ∀a.     (a = roundTiesToEven) ∨ (a = roundTowardPositive) ∨     (a = roundTowardNegative) ∨ (a = roundTowardZero)
(conjecture "thm_2Ebinary__ieee_2Erounding__nchotomy" (("c_2Ebool_2E_21" "ty_2Ebinary__ieee_2Erounding") (lambda "V0a" "ty_2Ebinary__ieee_2Erounding" ("c_2Ebool_2E_5C_2F" (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Erounding") "V0a" "c_2Ebinary__ieee_2EroundTiesToEven") ("c_2Ebool_2E_5C_2F" (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Erounding") "V0a" "c_2Ebinary__ieee_2EroundTowardPositive") ("c_2Ebool_2E_5C_2F" (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Erounding") "V0a" "c_2Ebinary__ieee_2EroundTowardNegative") (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Erounding") "V0a" "c_2Ebinary__ieee_2EroundTowardZero")))))))
; binary_ieee.rounding_Axiom
; ∀x0 x1 x2 x3.     ∃f.         (f roundTiesToEven = x0) ∧ (f roundTowardPositive = x1) ∧         (f roundTowardNegative = x2) ∧ (f roundTowardZero = x3)
(conjecture "thm_2Ebinary__ieee_2Erounding__Axiom" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" "A_27a") (lambda "V0x0" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V1x1" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V2x2" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V3x3" "A_27a" (("c_2Ebool_2E_3F" ("ty_2Emin_2Efun" "ty_2Ebinary__ieee_2Erounding" "A_27a")) (lambda "V4f" ("ty_2Emin_2Efun" "ty_2Ebinary__ieee_2Erounding" "A_27a") ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "A_27a") ("V4f" "c_2Ebinary__ieee_2EroundTiesToEven") "V0x0") ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "A_27a") ("V4f" "c_2Ebinary__ieee_2EroundTowardPositive") "V1x1") ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "A_27a") ("V4f" "c_2Ebinary__ieee_2EroundTowardNegative") "V2x2") (("c_2Emin_2E_3D" "A_27a") ("V4f" "c_2Ebinary__ieee_2EroundTowardZero") "V3x3"))))))))))))))))
; binary_ieee.rounding_induction
; ∀P.     P roundTiesToEven ∧ P roundTowardNegative ∧ P roundTowardPositive ∧     P roundTowardZero ⇒     ∀a. P a
(conjecture "thm_2Ebinary__ieee_2Erounding__induction" (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Ebinary__ieee_2Erounding" "ty_2Emin_2Ebool")) (lambda "V0P" ("ty_2Emin_2Efun" "ty_2Ebinary__ieee_2Erounding" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" ("V0P" "c_2Ebinary__ieee_2EroundTiesToEven") ("c_2Ebool_2E_2F_5C" ("V0P" "c_2Ebinary__ieee_2EroundTowardNegative") ("c_2Ebool_2E_2F_5C" ("V0P" "c_2Ebinary__ieee_2EroundTowardPositive") ("V0P" "c_2Ebinary__ieee_2EroundTowardZero")))) (("c_2Ebool_2E_21" "ty_2Ebinary__ieee_2Erounding") (lambda "V1a" "ty_2Ebinary__ieee_2Erounding" ("V0P" "V1a")))))))
; binary_ieee.rounding_case_cong
; ∀M M' v0 v1 v2 v3.     (M = M') ∧ ((M' = roundTiesToEven) ⇒ (v0 = v0')) ∧     ((M' = roundTowardPositive) ⇒ (v1 = v1')) ∧     ((M' = roundTowardNegative) ⇒ (v2 = v2')) ∧     ((M' = roundTowardZero) ⇒ (v3 = v3')) ⇒     ((case M of         roundTiesToEven => v0       | roundTowardPositive => v1       | roundTowardNegative => v2       | roundTowardZero => v3) =      case M' of        roundTiesToEven => v0'      | roundTowardPositive => v1'      | roundTowardNegative => v2'      | roundTowardZero => v3')
