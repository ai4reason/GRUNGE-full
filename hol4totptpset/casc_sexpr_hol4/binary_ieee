; binary_ieee.float_compare_case_cong
; ∀M M' v0 v1 v2 v3.     (M = M') ∧ ((M' = LT) ⇒ (v0 = v0')) ∧ ((M' = EQ) ⇒ (v1 = v1')) ∧     ((M' = GT) ⇒ (v2 = v2')) ∧ ((M' = UN) ⇒ (v3 = v3')) ⇒     ((case M of LT => v0 | EQ => v1 | GT => v2 | UN => v3) =      case M' of LT => v0' | EQ => v1' | GT => v2' | UN => v3')
(conjecture "thm_2Ebinary__ieee_2Efloat__compare__case__cong" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" "A_27a") (lambda "V0v0_27" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V1v1_27" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V2v2_27" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V3v3_27" "A_27a" (("c_2Ebool_2E_21" "ty_2Ebinary__ieee_2Efloat__compare") (lambda "V4M" "ty_2Ebinary__ieee_2Efloat__compare" (("c_2Ebool_2E_21" "ty_2Ebinary__ieee_2Efloat__compare") (lambda "V5M_27" "ty_2Ebinary__ieee_2Efloat__compare" (("c_2Ebool_2E_21" "A_27a") (lambda "V6v0" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V7v1" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V8v2" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V9v3" "A_27a" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Efloat__compare") "V4M" "V5M_27") ("c_2Ebool_2E_2F_5C" ("c_2Emin_2E_3D_3D_3E" (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Efloat__compare") "V5M_27" "c_2Ebinary__ieee_2ELT") (("c_2Emin_2E_3D" "A_27a") "V6v0" "V0v0_27")) ("c_2Ebool_2E_2F_5C" ("c_2Emin_2E_3D_3D_3E" (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Efloat__compare") "V5M_27" "c_2Ebinary__ieee_2EEQ") (("c_2Emin_2E_3D" "A_27a") "V7v1" "V1v1_27")) ("c_2Ebool_2E_2F_5C" ("c_2Emin_2E_3D_3D_3E" (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Efloat__compare") "V5M_27" "c_2Ebinary__ieee_2EGT") (("c_2Emin_2E_3D" "A_27a") "V8v2" "V2v2_27")) ("c_2Emin_2E_3D_3D_3E" (("c_2Emin_2E_3D" "ty_2Ebinary__ieee_2Efloat__compare") "V5M_27" "c_2Ebinary__ieee_2EUN") (("c_2Emin_2E_3D" "A_27a") "V9v3" "V3v3_27")))))) (("c_2Emin_2E_3D" "A_27a") (("c_2Ebinary__ieee_2Efloat__compare__CASE" "A_27a") "V4M" "V6v0" "V7v1" "V8v2" "V9v3") (("c_2Ebinary__ieee_2Efloat__compare__CASE" "A_27a") "V5M_27" "V0v0_27" "V1v1_27" "V2v2_27" "V3v3_27")))))))))))))))))))))))))
; binary_ieee.float_compare_case_eq
; ((case x of LT => v0 | EQ => v1 | GT => v2 | UN => v3) = v) ⇔ (x = LT) ∧ (v0 = v) ∨ (x = EQ) ∧ (v1 = v) ∨ (x = GT) ∧ (v2 = v) ∨ (x = UN) ∧ (v3 = v)
