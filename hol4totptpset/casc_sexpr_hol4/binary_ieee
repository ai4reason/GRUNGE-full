; binary_ieee.float_to_real
; ∀s e f.     float_to_real <|Sign := s; Exponent := e; Significand := f|> =     (let        r =          if e = 0w then 2 / &(2 ** INT_MAX (:χ)) * (&w2n f / &dimword (:τ))          else            &(2 ** w2n e) / &(2 ** INT_MAX (:χ)) *            (1 + &w2n f / &dimword (:τ))      in        if s = 1w then -r else r)
