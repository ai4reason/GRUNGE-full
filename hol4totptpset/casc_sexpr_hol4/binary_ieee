; binary_ieee.fp_op_induction
; ∀P.     (∀r f. P (FP_Sqrt r f)) ∧ (∀r f f0. P (FP_Add r f f0)) ∧     (∀r f f0. P (FP_Sub r f f0)) ∧ (∀r f f0. P (FP_Mul r f f0)) ∧     (∀r f f0. P (FP_Div r f f0)) ∧ (∀r f f0 f1. P (FP_MulAdd r f f0 f1)) ∧     (∀r f f0 f1. P (FP_MulSub r f f0 f1)) ⇒     ∀f. P f
(conjecture "thm_2Ebinary__ieee_2Efp__op__induction" (forall_tyvarl_term ("A_27t" "A_27w") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" ("ty_2Ebinary__ieee_2Efp__op" "A_27t" "A_27w") "ty_2Emin_2Ebool")) (lambda "V0P" ("ty_2Emin_2Efun" ("ty_2Ebinary__ieee_2Efp__op" "A_27t" "A_27w") "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "ty_2Ebinary__ieee_2Erounding") (lambda "V1r" "ty_2Ebinary__ieee_2Erounding" (("c_2Ebool_2E_21" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w")) (lambda "V2f" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w") ("V0P" (("c_2Ebinary__ieee_2EFP__Sqrt" "A_27t" "A_27w") "V1r" "V2f")))))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "ty_2Ebinary__ieee_2Erounding") (lambda "V3r" "ty_2Ebinary__ieee_2Erounding" (("c_2Ebool_2E_21" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w")) (lambda "V4f" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w") (("c_2Ebool_2E_21" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w")) (lambda "V5f0" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w") ("V0P" (("c_2Ebinary__ieee_2EFP__Add" "A_27t" "A_27w") "V3r" "V4f" "V5f0")))))))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "ty_2Ebinary__ieee_2Erounding") (lambda "V6r" "ty_2Ebinary__ieee_2Erounding" (("c_2Ebool_2E_21" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w")) (lambda "V7f" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w") (("c_2Ebool_2E_21" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w")) (lambda "V8f0" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w") ("V0P" (("c_2Ebinary__ieee_2EFP__Sub" "A_27t" "A_27w") "V6r" "V7f" "V8f0")))))))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "ty_2Ebinary__ieee_2Erounding") (lambda "V9r" "ty_2Ebinary__ieee_2Erounding" (("c_2Ebool_2E_21" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w")) (lambda "V10f" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w") (("c_2Ebool_2E_21" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w")) (lambda "V11f0" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w") ("V0P" (("c_2Ebinary__ieee_2EFP__Mul" "A_27t" "A_27w") "V9r" "V10f" "V11f0")))))))) ("c_2Eb