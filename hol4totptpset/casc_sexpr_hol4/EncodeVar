; c.EncodeVar.fixed_width
; :num -> (α -> bool) # (α -> bool list) # (bool list -> (α # bool list) option) -> bool
(constant_definition "c_2EEncodeVar_2Efixed__width" (forall_tyvarl_type ("A_27a") ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" ("ty_2Elist_2Elist" "ty_2Emin_2Ebool")) ("ty_2Emin_2Efun" ("ty_2Elist_2Elist" "ty_2Emin_2Ebool") ("ty_2Eoption_2Eoption" ("ty_2Epair_2Eprod" "A_27a" ("ty_2Elist_2Elist" "ty_2Emin_2Ebool")))))) "ty_2Emin_2Ebool"))))
; c.EncodeVar.of_length
; :num -> α list -> bool
(constant_definition "c_2EEncodeVar_2Eof__length" (forall_tyvarl_type ("A_27a") ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" ("ty_2Elist_2Elist" "A_27a") "ty_2Emin_2Ebool"))))
; EncodeVar.fixed_width_def
; ∀n c. fixed_width n c ⇔ ∀x. domain c x ⇒ (LENGTH (encoder c x) = n)
(axiom "thm_2EEncodeVar_2Efixed__width__def" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V0n" "ty_2Enum_2Enum" (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" ("ty_2Elist_2Elist" "ty_2Emin_2Ebool")) ("ty_2Emin_2Efun" ("ty_2Elist_2Elist" "ty_2Emin_2Ebool") ("ty_2Eoption_2Eoption" ("ty_2Epair_2Eprod" "A_27a" ("ty_2Elist_2Elist" "ty_2Emin_2Ebool"))))))) (lambda "V1c" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" ("ty_2Elist_2Elist" "ty_2Emin_2Ebool")) ("ty_2Emin_2Efun" ("ty_2Elist_2Elist" "ty_2Emin_2Ebool") ("ty_2Eoption_2Eoption" ("ty_2Epair_2Eprod" "A_27a" ("ty_2Elist_2Elist" "ty_2Emin_2Ebool")))))) (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2EEncodeVar_2Efixed__width" "A_27a") "V0n" "V1c") (("c_2Ebool_2E_21" "A_27a") (lambda "V2x" "A_27a" ("c_2Emin_2E_3D_3D_3E" (("c_2ECoder_2Edomain" "A_27a") "V1c" "V2x") (("c_2Emin_2E_3D" "ty_2Enum_2Enum") (("c_2Elist_2ELENGTH" "ty_2Emin_2Ebool") (("c_2ECoder_2Eencoder" "A_27a") "V1c" "V2x")) "V0n")))))))))))
; EncodeVar.of_length_def
; ∀l n. l ∈ of_length n ⇔ (LENGTH l = n)
(axiom "thm_2EEncodeVar_2Eof__length__def" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V0l" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V1n" "ty_2Enum_2Enum" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Ebool_2EIN" ("ty_2Elist_2Elist" "A_27a")) "V0l" (("c_2EEncodeVar_2Eof__length" "A_27a") "V1n")) (("c_2Emin_2E_3D" "ty_2Enum_2Enum") (("c_2Elist_2ELENGTH" "A_27a") "V0l") "V1n"))))))))
; EncodeVar.fixed_width_univ
