; ieee.roundmode_case_eq
; ((case x of     To_nearest => v0   | float_To_zero => v1   | To_pinfinity => v2   | To_ninfinity => v3) = v) ⇔ (x = To_nearest) ∧ (v0 = v) ∨ (x = float_To_zero) ∧ (v1 = v) ∨ (x = To_pinfinity) ∧ (v2 = v) ∨ (x = To_ninfinity) ∧ (v3 = v)
(conjecture "thm_2Eieee_2Eroundmode__case__eq" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" "ty_2Eieee_2Eroundmode") (lambda "V0x" "ty_2Eieee_2Eroundmode" (("c_2Ebool_2E_21" "A_27a") (lambda "V1v0" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V2v1" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V3v2" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V4v3" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V5v" "A_27a" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" "A_27a") (("c_2Eieee_2Eroundmode__CASE" "A_27a") "V0x" "V1v0" "V2v1" "V3v2" "V4v3") "V5v") ("c_2Ebool_2E_5C_2F" ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "ty_2Eieee_2Eroundmode") "V0x" "c_2Eieee_2ETo__nearest") (("c_2Emin_2E_3D" "A_27a") "V1v0" "V5v")) ("c_2Ebool_2E_5C_2F" ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "ty_2Eieee_2Eroundmode") "V0x" "c_2Eieee_2Efloat__To__zero") (("c_2Emin_2E_3D" "A_27a") "V2v1" "V5v")) ("c_2Ebool_2E_5C_2F" ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "ty_2Eieee_2Eroundmode") "V0x" "c_2Eieee_2ETo__pinfinity") (("c_2Emin_2E_3D" "A_27a") "V3v2" "V5v")) ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "ty_2Eieee_2Eroundmode") "V0x" "c_2Eieee_2ETo__ninfinity") (("c_2Emin_2E_3D" "A_27a") "V4v3" "V5v"))))))))))))))))))))
; ieee.is_closest
; ∀v s x a.     is_closest v s x a ⇔ a ∈ s ∧ ∀b. b ∈ s ⇒ abs (v a − x) ≤ abs (v b − x)
(axiom "thm_2Eieee_2Eis__closest" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Erealax_2Ereal")) (lambda "V0v" ("ty_2Emin_2Efun" "A_27a" "ty_2Erealax_2Ereal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V1s" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" "ty_2Erealax_2Ereal") (lambda "V2x" "ty_2Erealax_2Ereal" (("c_2Ebool_2E_21" "A_27a") (lambda "V3a" "A_27a" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Eieee_2Eis__closest" "A_27a") "V0v" "V1s" "V2x" "V3a") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" "A_27a") "V3a" "V1s") (("c_2Ebool_2E_21" "A_27a") (lambda "V4b" "A_27a" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27a") "V4b" "V1s") ("c_2Ereal_2Ereal__lte" ("c_2Ereal_2Eabs" ("c_2Ereal_2Ereal__sub" ("V0v" "V3a") "V2x")) ("c_2Ereal_2Eabs" ("c_2Ereal_2Ereal__sub" ("V0v" "V4b") "V2x"))))))))))))))))))
; ieee.closest
; ∀v p s x.     closest v p s x =     @a. is_closest v s x a ∧ ((∃b. is_closest v s x b ∧ p b) ⇒ p a)
(axiom "thm_2Eieee_2Eclosest" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Erealax_2Ereal")) (lambda "V0v" ("ty_2Emin_2Efun" "A_27a" "ty_2Erealax_2Ereal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V1p" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V2s" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" "ty_2Erealax_2Ereal") (lambda "V3x" "ty_2Erealax_2Ereal" (("c_2Emin_2E_3D" "A_27a") (("c_2Eieee_2Eclosest" "A_27a") "V0v" "V1p" "V2s" "V3x") (("c_2Emin_2E_40" "A_27a") (lambda "V4a" "A_27a" ("c_2Ebool_2E_2F_5C" (("c_2Eieee_2Eis__closest" "A_27a") "V0v" "V2s" "V3x" "V4a") ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2E_3F" "A_27a") (lambda "V5b" "A_27a" ("c_2Ebool_2E_2F_5C" (("c_2Eieee_2Eis__closest" "A_27a") "V0v" "V2s" "V3x" "V5b") ("V1p" "V5b")))) ("V1p" "V4a"))))))))))))))))
; ieee.round_def
; (∀X x.      round X To_nearest x =      if x ≤ -threshold X then minus_infinity X      else if x ≥ threshold X then plus_infinity X      else closest (valof X) (λa. EVEN (fraction a)) {a | is_finite X a} x) ∧ (∀X x.      round X float_To_zero x =      if x < -largest X then bottomfloat X      else if x > largest X then topfloat X      else        closest (valof X) (λx. T)          {a | is_finite X a ∧ abs (valof X a) ≤ abs x} x) ∧ (∀X x.      round X To_pinfinity x =      if x < -largest X then bottomfloat X      else if x > largest X then plus_infinity X      else closest (valof X) (λx. T) {a | is_finite X a ∧ valof X a ≥ x} x) ∧ ∀X x.     round X To_ninfinity x =     if x < -largest X then minus_infinity X     else if x > largest X then topfloat X     else closest (valof X) (λx. T) {a | is_finite X a ∧ valof X a ≤ x} x
