; option.IF_EQUALS_OPTION
; (((if P then SOME x else NONE) = NONE) ⇔ ¬P) ∧ (((if P then NONE else SOME x) = NONE) ⇔ P) ∧ (((if P then SOME x else NONE) = SOME y) ⇔ P ∧ (x = y)) ∧ (((if P then NONE else SOME x) = SOME y) ⇔ ¬P ∧ (x = y))
(conjecture "thm_2Eoption_2EIF__EQUALS__OPTION" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" "ty_2Emin_2Ebool") (lambda "V0P" "ty_2Emin_2Ebool" (("c_2Ebool_2E_21" "A_27a") (lambda "V1x" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V2y" "A_27a" ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) (("c_2Ebool_2ECOND" ("ty_2Eoption_2Eoption" "A_27a")) "V0P" (("c_2Eoption_2ESOME" "A_27a") "V1x") ("c_2Eoption_2ENONE" "A_27a")) ("c_2Eoption_2ENONE" "A_27a")) ("c_2Ebool_2E_7E" "V0P")) ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) (("c_2Ebool_2ECOND" ("ty_2Eoption_2Eoption" "A_27a")) "V0P" ("c_2Eoption_2ENONE" "A_27a") (("c_2Eoption_2ESOME" "A_27a") "V1x")) ("c_2Eoption_2ENONE" "A_27a")) "V0P") ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) (("c_2Ebool_2ECOND" ("ty_2Eoption_2Eoption" "A_27a")) "V0P" (("c_2Eoption_2ESOME" "A_27a") "V1x") ("c_2Eoption_2ENONE" "A_27a")) (("c_2Eoption_2ESOME" "A_27a") "V2y")) ("c_2Ebool_2E_2F_5C" "V0P" (("c_2Emin_2E_3D" "A_27a") "V1x" "V2y"))) (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) (("c_2Ebool_2ECOND" ("ty_2Eoption_2Eoption" "A_27a")) "V0P" ("c_2Eoption_2ENONE" "A_27a") (("c_2Eoption_2ESOME" "A_27a") "V1x")) (("c_2Eoption_2ESOME" "A_27a") "V2y")) ("c_2Ebool_2E_2F_5C" ("c_2Ebool_2E_7E" "V0P") (("c_2Emin_2E_3D" "A_27a") "V1x" "V2y"))))))))))))))
; option.IF_NONE_EQUALS_OPTION
; (((if P then X else NONE) = NONE) ⇔ P ⇒ IS_NONE X) ∧ (((if P then NONE else X) = NONE) ⇔ IS_SOME X ⇒ P) ∧ (((if P then X else NONE) = SOME x) ⇔ P ∧ (X = SOME x)) ∧ (((if P then NONE else X) = SOME x) ⇔ ¬P ∧ (X = SOME x))
(conjecture "thm_2Eoption_2EIF__NONE__EQUALS__OPTION" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" "ty_2Emin_2Ebool") (lambda "V0P" "ty_2Emin_2Ebool" (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27a")) (lambda "V1X" ("ty_2Eoption_2Eoption" "A_27a") (("c_2Ebool_2E_21" "A_27a") (lambda "V2x" "A_27a" ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) (("c_2Ebool_2ECOND" ("ty_2Eoption_2Eoption" "A_27a")) "V0P" "V1X" ("c_2Eoption_2ENONE" "A_27a")) ("c_2Eoption_2ENONE" "A_27a")) ("c_2Emin_2E_3D_3D_3E" "V0P" (("c_2Eoption_2EIS__NONE" "A_27a") "V1X"))) ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) (("c_2Ebool_2ECOND" ("ty_2Eoption_2Eoption" "A_27a")) "V0P" ("c_2Eoption_2ENONE" "A_27a") "V1X") ("c_2Eoption_2ENONE" "A_27a")) ("c_2Emin_2E_3D_3D_3E" (("c_2Eoption_2EIS__SOME" "A_27a") "V1X") "V0P")) ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) (("c_2Ebool_2ECOND" ("ty_2Eoption_2Eoption" "A_27a")) "V0P" "V1X" ("c_2Eoption_2ENONE" "A_27a")) (("c_2Eoption_2ESOME" "A_27a") "V2x")) ("c_2Ebool_2E_2F_5C" "V0P" (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) "V1X" (("c_2Eoption_2ESOME" "A_27a") "V2x")))) (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) (("c_2Ebool_2ECOND" ("ty_2Eoption_2Eoption" "A_27a")) "V0P" ("c_2Eoption_2ENONE" "A_27a") "V1X") (("c_2Eoption_2ESOME" "A_27a") "V2x")) ("c_2Ebool_2E_2F_5C" ("c_2Ebool_2E_7E" "V0P") (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) "V1X" (("c_2Eoption_2ESOME" "A_27a") "V2x")))))))))))))))
; option.OPTION_MAP_EQ_SOME
; ∀f x y. (OPTION_MAP f x = SOME y) ⇔ ∃z. (x = SOME z) ∧ (y = f z)
(conjecture "thm_2Eoption_2EOPTION__MAP__EQ__SOME" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V0f" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27a")) (lambda "V1x" ("ty_2Eoption_2Eoption" "A_27a") (("c_2Ebool_2E_21" "A_27b") (lambda "V2y" "A_27b" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27b")) (("c_2Eoption_2EOPTION__MAP" "A_27a" "A_27b") "V0f" "V1x") (("c_2Eoption_2ESOME" "A_27b") "V2y")) (("c_2Ebool_2E_3F" "A_27a") (lambda "V3z" "A_27a" ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) "V1x" (("c_2Eoption_2ESOME" "A_27a") "V3z")) (("c_2Emin_2E_3D" "A_27b") "V2y" ("V0f" "V3z"))))))))))))))
; option.OPTION_MAP_EQ_NONE
; ∀f x. (OPTION_MAP f x = NONE) ⇔ (x = NONE)
(conjecture "thm_2Eoption_2EOPTION__MAP__EQ__NONE" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V0f" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27a")) (lambda "V1x" ("ty_2Eoption_2Eoption" "A_27a") (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27b")) (("c_2Eoption_2EOPTION__MAP" "A_27a" "A_27b") "V0f" "V1x") ("c_2Eoption_2ENONE" "A_27b")) (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) "V1x" ("c_2Eoption_2ENONE" "A_27a")))))))))
; option.OPTION_MAP_EQ_NONE_both_ways
; ((OPTION_MAP f x = NONE) ⇔ (x = NONE)) ∧ ((NONE = OPTION_MAP f x) ⇔ (x = NONE))
(conjecture "thm_2Eoption_2EOPTION__MAP__EQ__NONE__both__ways" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "A_27a")) (lambda "V0f" ("ty_2Emin_2Efun" "A_27b" "A_27a") (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27b")) (lambda "V1x" ("ty_2Eoption_2Eoption" "A_27b") ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) (("c_2Eoption_2EOPTION__MAP" "A_27b" "A_27a") "V0f" "V1x") ("c_2Eoption_2ENONE" "A_27a")) (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27b")) "V1x" ("c_2Eoption_2ENONE" "A_27b"))) (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) ("c_2Eoption_2ENONE" "A_27a") (("c_2Eoption_2EOPTION__MAP" "A_27b" "A_27a") "V0f" "V1x")) (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27b")) "V1x" ("c_2Eoption_2ENONE" "A_27b"))))))))))
; option.OPTION_MAP_COMPOSE
; OPTION_MAP f (OPTION_MAP g x) = OPTION_MAP (f ∘ g) x
(conjecture "thm_2Eoption_2EOPTION__MAP__COMPOSE" (forall_tyvarl_term ("A_27a" "A_27b" "A_27c") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27c" "A_27b")) (lambda "V0f" ("ty_2Emin_2Efun" "A_27c" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27c")) (lambda "V1g" ("ty_2Emin_2Efun" "A_27a" "A_27c") (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27a")) (lambda "V2x" ("ty_2Eoption_2Eoption" "A_27a") (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27b")) (("c_2Eoption_2EOPTION__MAP" "A_27c" "A_27b") "V0f" (("c_2Eoption_2EOPTION__MAP" "A_27a" "A_27c") "V1g" "V2x")) (("c_2Eoption_2EOPTION__MAP" "A_27a" "A_27b") (("c_2Ecombin_2Eo" "A_27a" "A_27b" "A_27c") "V0f" "V1g") "V2x"))))))))))
; option.OPTION_MAP_CONG
; ∀opt1 opt2 f1 f2.     (opt1 = opt2) ∧ (∀x. (opt2 = SOME x) ⇒ (f1 x = f2 x)) ⇒     (OPTION_MAP f1 opt1 = OPTION_MAP f2 opt2)
(conjecture "thm_2Eoption_2EOPTION__MAP__CONG" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27a")) (lambda "V0opt1" ("ty_2Eoption_2Eoption" "A_27a") (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27a")) (lambda "V1opt2" ("ty_2Eoption_2Eoption" "A_27a") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V2f1" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V3f2" ("ty_2Emin_2Efun" "A_27a" "A_27b") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) "V0opt1" "V1opt2") (("c_2Ebool_2E_21" "A_27a") (lambda "V4x" "A_27a" ("c_2Emin_2E_3D_3D_3E" (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) "V1opt2" (("c_2Eoption_2ESOME" "A_27a") "V4x")) (("c_2Emin_2E_3D" "A_27b") ("V2f1" "V4x") ("V3f2" "V4x")))))) (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27b")) (("c_2Eoption_2EOPTION__MAP" "A_27a" "A_27b") "V2f1" "V0opt1") (("c_2Eoption_2EOPTION__MAP" "A_27a" "A_27b") "V3f2" "V1opt2")))))))))))))
; option.IS_SOME_MAP
; IS_SOME (OPTION_MAP f x) ⇔ IS_SOME x
(conjecture "thm_2Eoption_2EIS__SOME__MAP" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V0f" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27a")) (lambda "V1x" ("ty_2Eoption_2Eoption" "A_27a") (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Eoption_2EIS__SOME" "A_27b") (("c_2Eoption_2EOPTION__MAP" "A_27a" "A_27b") "V0f" "V1x")) (("c_2Eoption_2EIS__SOME" "A_27a") "V1x"))))))))
; option.OPTION_JOIN_EQ_SOME
; ∀x y. (OPTION_JOIN x = SOME y) ⇔ (x = SOME (SOME y))
(conjecture "thm_2Eoption_2EOPTION__JOIN__EQ__SOME" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" ("ty_2Eoption_2Eoption" "A_27a"))) (lambda "V0x" ("ty_2Eoption_2Eoption" ("ty_2Eoption_2Eoption" "A_27a")) (("c_2Ebool_2E_21" "A_27a") (lambda "V1y" "A_27a" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) (("c_2Eoption_2EOPTION__JOIN" "A_27a") "V0x") (("c_2Eoption_2ESOME" "A_27a") "V1y")) (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" ("ty_2Eoption_2Eoption" "A_27a"))) "V0x" (("c_2Eoption_2ESOME" ("ty_2Eoption_2Eoption" "A_27a")) (("c_2Eoption_2ESOME" "A_27a") "V1y"))))))))))
; option.OPTION_MAP2_SOME
; (OPTION_MAP2 f o1 o2 = SOME v) ⇔ ∃x1 x2. (o1 = SOME x1) ∧ (o2 = SOME x2) ∧ (v = f x1 x2)
(conjecture "thm_2Eoption_2EOPTION__MAP2__SOME" (forall_tyvarl_term ("A_27a" "A_27b" "A_27c") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27b" "A_27c"))) (lambda "V0f" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27b" "A_27c")) (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27a")) (lambda "V1o1" ("ty_2Eoption_2Eoption" "A_27a") (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27b")) (lambda "V2o2" ("ty_2Eoption_2Eoption" "A_27b") (("c_2Ebool_2E_21" "A_27c") (lambda "V3v" "A_27c" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27c")) (("c_2Eoption_2EOPTION__MAP2" "A_27c" "A_27a" "A_27b") "V0f" "V1o1" "V2o2") (("c_2Eoption_2ESOME" "A_27c") "V3v")) (("c_2Ebool_2E_3F" "A_27a") (lambda "V4x1" "A_27a" (("c_2Ebool_2E_3F" "A_27b") (lambda "V5x2" "A_27b" ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) "V1o1" (("c_2Eoption_2ESOME" "A_27a") "V4x1")) ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27b")) "V2o2" (("c_2Eoption_2ESOME" "A_27b") "V5x2")) (("c_2Emin_2E_3D" "A_27c") "V3v" ("V0f" "V4x1" "V5x2")))))))))))))))))))
; option.OPTION_MAP2_NONE
; (OPTION_MAP2 f o1 o2 = NONE) ⇔ (o1 = NONE) ∨ (o2 = NONE)
(conjecture "thm_2Eoption_2EOPTION__MAP2__NONE" (forall_tyvarl_term ("A_27a" "A_27b" "A_27c") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27b" "A_27c"))) (lambda "V0f" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27b" "A_27c")) (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27a")) (lambda "V1o1" ("ty_2Eoption_2Eoption" "A_27a") (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27b")) (lambda "V2o2" ("ty_2Eoption_2Eoption" "A_27b") (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27c")) (("c_2Eoption_2EOPTION__MAP2" "A_27c" "A_27a" "A_27b") "V0f" "V1o1" "V2o2") ("c_2Eoption_2ENONE" "A_27c")) ("c_2Ebool_2E_5C_2F" (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) "V1o1" ("c_2Eoption_2ENONE" "A_27a")) (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27b")) "V2o2" ("c_2Eoption_2ENONE" "A_27b"))))))))))))
; option.OPTION_MAP2_cong
; ∀x1 x2 y1 y2 f1 f2.     (x1 = x2) ∧ (y1 = y2) ∧     (∀x y. (x2 = SOME x) ∧ (y2 = SOME y) ⇒ (f1 x y = f2 x y)) ⇒     (OPTION_MAP2 f1 x1 y1 = OPTION_MAP2 f2 x2 y2)
(conjecture "thm_2Eoption_2EOPTION__MAP2__cong" (forall_tyvarl_term ("A_27a" "A_27b" "A_27c") (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27a")) (lambda "V0x1" ("ty_2Eoption_2Eoption" "A_27a") (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27a")) (lambda "V1x2" ("ty_2Eoption_2Eoption" "A_27a") (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27b")) (lambda "V2y1" ("ty_2Eoption_2Eoption" "A_27b") (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27b")) (lambda "V3y2" ("ty_2Eoption_2Eoption" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27b" "A_27c"))) (lambda "V4f1" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27b" "A_27c")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27b" "A_27c"))) (lambda "V5f2" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27b" "A_27c")) ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) "V0x1" "V1x2") ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27b")) "V2y1" "V3y2") (("c_2Ebool_2E_21" "A_27a") (lambda "V6x" "A_27a" (("c_2Ebool_2E_21" "A_27b") (lambda "V7y" "A_27b" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) "V1x2" (("c_2Eoption_2ESOME" "A_27a") "V6x")) (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27b")) "V3y2" (("c_2Eoption_2ESOME" "A_27b") "V7y"))) (("c_2Emin_2E_3D" "A_27c") ("V4f1" "V6x" "V7y") ("V5f2" "V6x" "V7y"))))))))) (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27c")) (("c_2Eoption_2EOPTION__MAP2" "A_27c" "A_27a" "A_27b") "V4f1" "V0x1" "V2y1") (("c_2Eoption_2EOPTION__MAP2" "A_27c" "A_27a" "A_27b") "V5f2" "V1x2" "V3y2")))))))))))))))))
; option.OPTION_MAP_CASE
