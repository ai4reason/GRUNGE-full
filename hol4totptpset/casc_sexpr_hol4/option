; option.OPTION_ALL_MONO
; (∀x. P x ⇒ P' x) ⇒ OPTION_ALL P opt ⇒ OPTION_ALL P' opt
(conjecture "thm_2Eoption_2EOPTION__ALL__MONO" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V0P" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V1P_27" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27a")) (lambda "V2opt" ("ty_2Eoption_2Eoption" "A_27a") ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2E_21" "A_27a") (lambda "V3x" "A_27a" ("c_2Emin_2E_3D_3D_3E" ("V0P" "V3x") ("V1P_27" "V3x")))) ("c_2Emin_2E_3D_3D_3E" (("c_2Eoption_2EOPTION__ALL" "A_27a") "V0P" "V2opt") (("c_2Eoption_2EOPTION__ALL" "A_27a") "V1P_27" "V2opt")))))))))))
; option.OPTION_ALL_CONG
; ∀opt opt' P P'.     (opt = opt') ∧ (∀x. (opt' = SOME x) ⇒ (P x ⇔ P' x)) ⇒     (OPTION_ALL P opt ⇔ OPTION_ALL P' opt')
(conjecture "thm_2Eoption_2EOPTION__ALL__CONG" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27a")) (lambda "V0opt" ("ty_2Eoption_2Eoption" "A_27a") (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27a")) (lambda "V1opt_27" ("ty_2Eoption_2Eoption" "A_27a") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V2P" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V3P_27" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) "V0opt" "V1opt_27") (("c_2Ebool_2E_21" "A_27a") (lambda "V4x" "A_27a" ("c_2Emin_2E_3D_3D_3E" (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) "V1opt_27" (("c_2Eoption_2ESOME" "A_27a") "V4x")) (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") ("V2P" "V4x") ("V3P_27" "V4x")))))) (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Eoption_2EOPTION__ALL" "A_27a") "V2P" "V0opt") (("c_2Eoption_2EOPTION__ALL" "A_27a") "V3P_27" "V1opt_27")))))))))))))
; option.option_case_eq
; (option_CASE opt nc sc = v) ⇔ (opt = NONE) ∧ (nc = v) ∨ ∃x. (opt = SOME x) ∧ (sc x = v)
(conjecture "thm_2Eoption_2Eoption__case__eq" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27a")) (lambda "V0opt" ("ty_2Eoption_2Eoption" "A_27a") (("c_2Ebool_2E_21" "A_27b") (lambda "V1nc" "A_27b" (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V2sc" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" "A_27b") (lambda "V3v" "A_27b" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" "A_27b") (("c_2Eoption_2Eoption__CASE" "A_27a" "A_27b") "V0opt" "V1nc" "V2sc") "V3v") ("c_2Ebool_2E_5C_2F" ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) "V0opt" ("c_2Eoption_2ENONE" "A_27a")) (("c_2Emin_2E_3D" "A_27b") "V1nc" "V3v")) (("c_2Ebool_2E_3F" "A_27a") (lambda "V4x" "A_27a" ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Eoption_2Eoption" "A_27a")) "V0opt" (("c_2Eoption_2ESOME" "A_27a") "V4x")) (("c_2Emin_2E_3D" "A_27b") ("V2sc" "V4x") "V3v"))))))))))))))))
; option.option_Induct
; ∀P. (∀a. P (SOME a)) ∧ P NONE ⇒ ∀x. P x
(conjecture "thm_2Eoption_2Eoption__Induct" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" ("ty_2Eoption_2Eoption" "A_27a") "ty_2Emin_2Ebool")) (lambda "V0P" ("ty_2Emin_2Efun" ("ty_2Eoption_2Eoption" "A_27a") "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V1a" "A_27a" ("V0P" (("c_2Eoption_2ESOME" "A_27a") "V1a")))) ("V0P" ("c_2Eoption_2ENONE" "A_27a"))) (("c_2Ebool_2E_21" ("ty_2Eoption_2Eoption" "A_27a")) (lambda "V2x" ("ty_2Eoption_2Eoption" "A_27a") ("V0P" "V2x"))))))))
; option.option_CASES
; ∀opt. (∃x. opt = SOME x) ∨ (opt = NONE)
