; measure.ADDITIVE
; ∀m s t u.     additive m ∧ s ∈ measurable_sets m ∧ t ∈ measurable_sets m ∧     DISJOINT s t ∧ (u = s ∪ t) ⇒     (measure m u = measure m s + measure m t)
(conjecture "thm_2Emeasure_2EADDITIVE" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V1s" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V2t" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V3u" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Eadditive" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V1s" (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V2t" (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")) ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EDISJOINT" "A_27a") "V1s" "V2t") (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V3u" (("c_2Epred__set_2EUNION" "A_27a") "V1s" "V2t")))))) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Emeasure_2Emeasure" "A_27a") "V0m" "V3u") ("c_2Erealax_2Ereal__add" (("c_2Emeasure_2Emeasure" "A_27a") "V0m" "V1s") (("c_2Emeasure_2Emeasure" "A_27a") "V0m" "V2t"))))))))))))))
; measure.COUNTABLY_SUBADDITIVE
; ∀m f s.     countably_subadditive m ∧ f ∈ (𝕌(:num) -> measurable_sets m) ∧     summable (measure m ∘ f) ∧ (s = BIGUNION (IMAGE f 𝕌(:num))) ∧     s ∈ measurable_sets m ⇒     measure m s ≤ suminf (measure m ∘ f)
(conjecture "thm_2Emeasure_2ECOUNTABLY__SUBADDITIVE" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V1f" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V2s" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Ecountably__subadditive" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) "V1f" (("c_2Epred__set_2EFUNSET" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("c_2Epred__set_2EUNIV" "ty_2Enum_2Enum") (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m"))) ("c_2Ebool_2E_2F_5C" ("c_2Eseq_2Esummable" (("c_2Ecombin_2Eo" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Emeasure_2Emeasure" "A_27a") "V0m") "V1f")) ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V2s" (("c_2Epred__set_2EBIGUNION" "A_27a") (("c_2Epred__set_2EIMAGE" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V1f" ("c_2Epred__set_2EUNIV" "ty_2Enum_2Enum")))) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V2s" (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")))))) ("c_2Ereal_2Ereal__lte" (("c_2Emeasure_2Emeasure" "A_27a") "V0m" "V2s") ("c_2Eseq_2Esuminf" (("c_2Ecombin_2Eo" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Emeasure_2Emeasure" "A_27a") "V0m") "V1f"))))))))))))
; measure.ADDITIVE_SUM
; ∀m f n.     algebra (m_space m,measurable_sets m) ∧ positive m ∧ additive m ∧     f ∈ (𝕌(:num) -> measurable_sets m) ∧ (∀m n. m ≠ n ⇒ DISJOINT (f m) (f n)) ⇒     (sum (0,n) (measure m ∘ f) = measure m (BIGUNION (IMAGE f (count n))))
