(("c_2Emeasure_2Esigma" ("ty_2Epair_2Eprod" "A_27b" "A_27c")) (("c_2Epred__set_2ECROSS" "A_27b" "A_27c") (("c_2Emeasure_2Espace" "A_27b") "V1a1") (("c_2Emeasure_2Espace" "A_27c") "V2a2")) (("c_2Eutil__prob_2Eprod__sets" "A_27b" "A_27c") (("c_2Emeasure_2Esubsets" "A_27b") "V1a1") (("c_2Emeasure_2Esubsets" "A_27c") "V2a2"))))))))))))))))
; measure.MEASURABLE_RANGE_REDUCE
; ∀m f s.     measure_space m ∧     f ∈ measurable (m_space m,measurable_sets m) (s,POW s) ∧ s ≠ ∅ ⇒     f ∈     measurable (m_space m,measurable_sets m)       (s ∩ IMAGE f (m_space m),POW (s ∩ IMAGE f (m_space m)))
