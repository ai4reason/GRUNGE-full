set_2ESUBSET" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V4c" (("c_2Epred__set_2EIMAGE" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EPREIMAGE" "A_27a" "A_27b") "V2f") (("c_2Emeasure_2Esubsets" "A_27b") "V1b")))) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EBIGUNION" "A_27a") "V4c") (("c_2Epred__set_2EIMAGE" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EPREIMAGE" "A_27a" "A_27b") "V2f") (("c_2Emeasure_2Esubsets" "A_27b") "V1b")))))))))))))))
; measure.MEASURABLE_SIGMA_PREIMAGES
; ∀a b f.     sigma_algebra a ∧ sigma_algebra b ∧ f ∈ (space a -> space b) ∧     (∀s. s ∈ subsets b ⇒ PREIMAGE f s ∈ subsets a) ⇒     sigma_algebra (space a,IMAGE (PREIMAGE f) (subsets b))
(conjecture "thm_2Emeasure_2EMEASURABLE__SIGMA__PREIMAGES" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V0a" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V1b" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V2f" ("ty_2Emin_2Efun" "A_27a" "A_27b") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Esigma__algebra" "A_27a") "V0a") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Esigma__algebra" "A_27b") "V1b") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27b")) "V2f" (("c_2Epred__set_2EFUNSET" "A_27a" "A_27b") (("c_2Emeasure_2Espace" "A_27a") "V0a") (("c_2Emeasure_2Espace" "A_27b") "V1b"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V3s" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) "V3s" (("c_2Emeasure_2Esubsets" "A_27b") "V1b")) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EPREIMAGE" "A_27a" "A_27b") "V2f" "V3s") (("c_2Emeasure_2Esubsets" "A_27a") "V0a")))))))) (("c_2Emeasure_2Esigma__algebra" "A_27a") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Espace" "A_27a") "V0a") (("c_2Epred__set_2EIMAGE" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EPREIMAGE" "A_27a" "A_27b") "V2f") (("c_2Emeasure_2Esubsets" "A_27b") "V1b")))))))))))))
; measure.IN_MEASURABLE
; ∀a b f.     f ∈ measurable a b ⇔     sigma_algebra a ∧ sigma_algebra b ∧ f ∈ (space a -> space b) ∧     ∀s. s ∈ subsets b ⇒ PREIMAGE f s ∩ space a ∈ subsets a
(conjecture "thm_2Emeasure_2EIN__MEASURABLE" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V0a" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V1b" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V2f" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27b")) "V2f" (("c_2Emeasure_2Emeasurable" "A_27a" "A_27b") "V0a" "V1b")) ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Esigma__algebra" "A_27a") "V0a") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Esigma__algebra" "A_27b") "V1b") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27b")) "V2f" (("c_2Epred__set_2EFUNSET" "A_27a" "A_27b") (("c_2Emeasure_2Espace" "A_27a") "V0a") (("c_2Emeasure_2Espace" "A_27b") "V1b"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V3s" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) "V3s" (("c_2Emeasure_2Esubsets" "A_27b") "V1b")) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EINTER" "A_27a") (("c_2Epred__set_2EPREIMAGE" "A_27a" "A_27b") "V2f" "V3s") (("c_2Emeasure_2Espace" "A_27a") "V0a")) (("c_2Emeasure_2Esubsets" "A_27a") "V0a")))))))))))))))))
; measure.MEASURABLE_SIGMA
; ∀f a b sp.     sigma_algebra a ∧ subset_class sp b ∧ f ∈ (space a -> sp) ∧     (∀s. s ∈ b ⇒ PREIMAGE f s ∩ space a ∈ subsets a) ⇒     f ∈ measurable a (sigma sp b)
(conjecture "thm_2Emeasure_2EMEASURABLE__SIGMA" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V0f" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V1a" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (lambda "V2b" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V3sp" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Esigma__algebra" "A_27a") "V1a") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Esubset__class" "A_27b") "V3sp" "V2b") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27b")) "V0f" (("c_2Epred__set_2EFUNSET" "A_27a" "A_27b") (("c_2Emeasure_2Espace" "A_27a") "V1a") "V3sp")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V4s" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) "V4s" "V2b") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EINTER" "A_27a") (("c_2Epred__set_2EPREIMAGE" "A_27a" "A_27b") "V0f" "V4s") (("c_2Emeasure_2Espace" "A_27a") "V1a")) (("c_2Emeasure_2Esubsets" "A_27a") "V1a")))))))) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27b")) "V0f" (("c_2Emeasure_2Emeasurable" "A_27a" "A_27b") "V1a" (("c_2Emeasure_2Esigma" "A_27b") "V3sp" "V2b"))))))))))))))
; measure.MEASURABLE_SUBSET
; ∀a b. measurable a b ⊆ measurable a (sigma (space b) (subsets b))
(conjecture "thm_2Emeasure_2EMEASURABLE__SUBSET" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V0a" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V1b" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Epred__set_2ESUBSET" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (("c_2Emeasure_2Emeasurable" "A_27a" "A_27b") "V0a" "V1b") (("c_2Emeasure_2Emeasurable" "A_27a" "A_27b") "V0a" (("c_2Emeasure_2Esigma" "A_27b") (("c_2Emeasure_2Espace" "A_27b") "V1b") (("c_2Emeasure_2Esubsets" "A_27b") "V1b"))))))))))
; measure.MEASURABLE_LIFT
; ∀f a b. f ∈ measurable a b ⇒ f ∈ measurable a (sigma (space b) (subsets b))
(conjecture "thm_2Emeasure_2EMEASURABLE__LIFT" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V0f" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V1a" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V2b" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27b")) "V0f" (("c_2Emeasure_2Emeasurable" "A_27a" "A_27b") "V1a" "V2b")) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27b")) "V0f" (("c_2Emeasure_2Emeasurable" "A_27a" "A_27b") "V1a" (("c_2Emeasure_2Esigma" "A_27b") (("c_2Emeasure_2Espace" "A_27b") "V2b") (("c_2Emeasure_2Esubsets" "A_27b") "V2b")))))))))))))
; measure.IN_MEASURE_PRESERVING
; ∀m1 m2 f.     f ∈ measure_preserving m1 m2 ⇔     f ∈     measurable (m_space m1,measurable_sets m1)       (m_space m2,measurable_sets m2) ∧ measure_space m1 ∧ measure_space m2 ∧     ∀s.         s ∈ measurable_sets m2 ⇒         (measure m1 (PREIMAGE f s ∩ m_space m1) = measure m2 s)
(conjecture "thm_2Emeasure_2EIN__MEASURE__PRESERVING" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m1" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V1m2" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V2f" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27b")) "V2f" (("c_2Emeasure_2Emeasure__preserving" "A_27a" "A_27b") "V0m1" "V1m2")) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27b")) "V2f" (("c_2Emeasure_2Emeasurable" "A_27a" "A_27b") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Em__space" "A_27a") "V0m1") (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m1")) (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Em__space" "A_27b") "V1m2") (("c_2Emeasure_2Emeasurable__sets" "A_27b") "V1m2")))) ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m1") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27b") "V1m2") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V3s" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) "V3s" (("c_2Emeasure_2Emeasurable__sets" "A_27b") "V1m2")) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Emeasure_2Emeasure" "A_27a") "V0m1" (("c_2Epred__set_2EINTER" "A_27a") (("c_2Epred__set_2EPREIMAGE" "A_27a" "A_27b") "V2f" "V3s") (("c_2Emeasure_2Em__space" "A_27a") "V0m1"))) (("c_2Emeasure_2Emeasure" "A_27b") "V1m2" "V3s")))))))))))))))))
; measure.MEASURE_PRESERVING_LIFT
; ∀m1 m2 a f.     measure_space m1 ∧ measure_space m2 ∧     (measurable_sets m2 = subsets (sigma (m_space m2) a)) ∧     f ∈ measure_preserving m1 (m_space m2,a,measure m2) ⇒     f ∈ measure_preserving m1 m2
(conjecture "thm_2Emeasure_2EMEASURE__PRESERVING__LIFT" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m1" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V1m2" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (lambda "V2a" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V3f" ("ty_2Emin_2Efun" "A_27a" "A_27b") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m1") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27b") "V1m2") ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Emeasurable__sets" "A_27b") "V1m2") (("c_2Emeasure_2Esubsets" "A_27b") (("c_2Emeasure_2Esigma" "A_27b") (("c_2Emeasure_2Em__space" "A_27b") "V1m2") "V2a"))) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27b")) "V3f" (("c_2Emeasure_2Emeasure__preserving" "A_27a" "A_27b") "V0m1" (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Emeasure_2Em__space" "A_27b") "V1m2") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")) "V2a" (("c_2Emeasure_2Emeasure" "A_27b") "V1m2")))))))) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27b")) "V3f" (("c_2Emeasure_2Emeasure__preserving" "A_27a" "A_27b") "V0m1" "V1m2")))))))))))))
; measure.MEASURE_PRESERVING_SUBSET
; ∀m1 m2 a.     measure_space m1 ∧ measure_space m2 ∧     (measurable_sets m2 = subsets (sigma (m_space m2) a)) ⇒     measure_preserving m1 (m_space m2,a,measure m2) ⊆     measure_preserving m1 m2
