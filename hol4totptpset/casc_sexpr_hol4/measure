; measure.INF_MEASURE_LE
; ∀m s x.     algebra (m_space m,measurable_sets m) ∧ positive m ∧ increasing m ∧     x ∈     {r |      ∃f.          f ∈ (𝕌(:num) -> measurable_sets m) ∧          s ⊆ BIGUNION (IMAGE f 𝕌(:num)) ∧ measure m ∘ f sums r} ⇒     inf_measure m s ≤ x
(conjecture "thm_2Emeasure_2EINF__MEASURE__LE" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V1s" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" "ty_2Erealax_2Ereal") (lambda "V2x" "ty_2Erealax_2Ereal" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Ealgebra" "A_27a") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Em__space" "A_27a") "V0m") (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m"))) ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Epositive" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Eincreasing" "A_27a") "V0m") (("c_2Ebool_2EIN" "ty_2Erealax_2Ereal") "V2x" (("c_2Epred__set_2EGSPEC" "ty_2Erealax_2Ereal" "ty_2Erealax_2Ereal") (lambda "V3r" "ty_2Erealax_2Ereal" (("c_2Epair_2E_2C" "ty_2Erealax_2Ereal" "ty_2Emin_2Ebool") "V3r" (("c_2Ebool_2E_3F" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V4f" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) "V4f" (("c_2Epred__set_2EFUNSET" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("c_2Epred__set_2EUNIV" "ty_2Enum_2Enum") (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m"))) ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2ESUBSET" "A_27a") "V1s" (("c_2Epred__set_2EBIGUNION" "A_27a") (("c_2Epred__set_2EIMAGE" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V4f" ("c_2Epred__set_2EUNIV" "ty_2Enum_2Enum")))) ("c_2Eseq_2Esums" (("c_2Ecombin_2Eo" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Emeasure_2Emeasure" "A_27a") "V0m") "V4f") "V3r")))))))))))) ("c_2Ereal_2Ereal__lte" (("c_2Emeasure_2Einf__measure" "A_27a") "V0m" "V1s") "V2x"))))))))))
; measure.INF_MEASURE_CLOSE
; ∀m s e.     algebra (m_space m,measurable_sets m) ∧ positive m ∧ 0 < e ∧     s ⊆ m_space m ⇒     ∃f l.         f ∈ (𝕌(:num) -> measurable_sets m) ∧ s ⊆ BIGUNION (IMAGE f 𝕌(:num)) ∧         (∀m n. m ≠ n ⇒ DISJOINT (f m) (f n)) ∧ measure m ∘ f sums l ∧         l ≤ inf_measure m s + e
