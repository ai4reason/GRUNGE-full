; measure.MEASURABLE_COMP_STRONG
; âˆ€f g a b c.     f âˆˆ measurable a b âˆ§ sigma_algebra c âˆ§ g âˆˆ (space b -> space c) âˆ§     (âˆ€x. x âˆˆ subsets c â‡’ PREIMAGE g x âˆ© IMAGE f (space a) âˆˆ subsets b) â‡’     g âˆ˜ f âˆˆ measurable a c
(conjecture "thm_2Emeasure_2EMEASURABLE__COMP__STRONG" (forall_tyvarl_term ("A_27a" "A_27b" "A_27c") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V0f" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "A_27c")) (lambda "V1g" ("ty_2Emin_2Efun" "A_27b" "A_27c") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V2a" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V3b" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V4c" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27b")) "V0f" (("c_2Emeasure_2Emeasurable" "A_27a" "A_27b") "V2a" "V3b")) ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Esigma__algebra" "A_27c") "V4c") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27b" "A_27c")) "V1g" (("c_2Epred__set_2EFUNSET" "A_27b" "A_27c") (("c_2Emeasure_2Espace" "A_27b") "V3b") (("c_2Emeasure_2Espace" "A_27c") "V4c"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool")) (lambda "V5x" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool")) "V5x" (("c_2Emeasure_2Esubsets" "A_27c") "V4c")) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EINTER" "A_27b") (("c_2Epred__set_2EPREIMAGE" "A_27b" "A_27c") "V1g" "V5x") (("c_2Epred__set_2EIMAGE" "A_27a" "A_27b") "V0f" (("c_2Emeasure_2Espace" "A_27a") "V2a"))) (("c_2Emeasure_2Esubsets" "A_27b") "V3b")))))))) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27c")) (("c_2Ecombin_2Eo" "A_27a" "A_27c" "A_27b") "V1g" "V0f") (("c_2Emeasure_2Emeasurable" "A_27a" "A_27c") "V2a" "V4c")))))))))))))))
; measure.MEASURABLE_COMP_STRONGER
; âˆ€f g a b c t.     f âˆˆ measurable a b âˆ§ sigma_algebra c âˆ§ g âˆˆ (space b -> space c) âˆ§     IMAGE f (space a) âŠ† t âˆ§     (âˆ€s. s âˆˆ subsets c â‡’ PREIMAGE g s âˆ© t âˆˆ subsets b) â‡’     g âˆ˜ f âˆˆ measurable a c
