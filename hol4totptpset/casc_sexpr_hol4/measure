(("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EINTER" "A_27b") (("c_2Epred__set_2EPREIMAGE" "A_27b" "A_27c") "V1g" "V5x") (("c_2Epred__set_2EIMAGE" "A_27a" "A_27b") "V0f" (("c_2Emeasure_2Espace" "A_27a") "V2a"))) (("c_2Emeasure_2Esubsets" "A_27b") "V3b")))))))) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27c")) (("c_2Ecombin_2Eo" "A_27a" "A_27c" "A_27b") "V1g" "V0f") (("c_2Emeasure_2Emeasurable" "A_27a" "A_27c") "V2a" "V4c")))))))))))))))
; measure.MEASURABLE_COMP_STRONGER
; ∀f g a b c t.     f ∈ measurable a b ∧ sigma_algebra c ∧ g ∈ (space b -> space c) ∧     IMAGE f (space a) ⊆ t ∧     (∀s. s ∈ subsets c ⇒ PREIMAGE g s ∩ t ∈ subsets b) ⇒     g ∘ f ∈ measurable a c
