; measure.CLOSED_CDI_DISJOINT
; ∀p f.     closed_cdi p ∧ f ∈ (𝕌(:num) -> subsets p) ∧     (∀m n. m ≠ n ⇒ DISJOINT (f m) (f n)) ⇒     BIGUNION (IMAGE f 𝕌(:num)) ∈ subsets p
(conjecture "thm_2Emeasure_2ECLOSED__CDI__DISJOINT" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V1f" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Eclosed__cdi" "A_27a") "V0p") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) "V1f" (("c_2Epred__set_2EFUNSET" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("c_2Epred__set_2EUNIV" "ty_2Enum_2Enum") (("c_2Emeasure_2Esubsets" "A_27a") "V0p"))) (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V2m" "ty_2Enum_2Enum" (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V3n" "ty_2Enum_2Enum" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "ty_2Enum_2Enum") "V2m" "V3n")) (("c_2Epred__set_2EDISJOINT" "A_27a") ("V1f" "V2m") ("V1f" "V3n"))))))))) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EBIGUNION" "A_27a") (("c_2Epred__set_2EIMAGE" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V1f" ("c_2Epred__set_2EUNIV" "ty_2Enum_2Enum"))) (("c_2Emeasure_2Esubsets" "A_27a") "V0p")))))))))
; measure.CLOSED_CDI_INCREASING
; ∀p f.     closed_cdi p ∧ f ∈ (𝕌(:num) -> subsets p) ∧ (f 0 = ∅) ∧     (∀n. f n ⊆ f (SUC n)) ⇒     BIGUNION (IMAGE f 𝕌(:num)) ∈ subsets p
