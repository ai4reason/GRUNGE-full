; measure.fn_plus_def
; ∀f. fn_plus f = (λx. if 0 < f x then f x else 0)
(axiom "thm_2Emeasure_2Efn__plus__def" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V0f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (("c_2Emeasure_2Efn__plus" "A_27a") "V0f") (lambda "V1x" "A_27a" (("c_2Ebool_2ECOND" "ty_2Eextreal_2Eextreal") ("c_2Eextreal_2Eextreal__lt" ("c_2Eextreal_2Eextreal__of__num" "c_2Enum_2E0") ("V0f" "V1x")) ("V0f" "V1x") ("c_2Eextreal_2Eextreal__of__num" "c_2Enum_2E0"))))))))
; measure.fn_minus_def
