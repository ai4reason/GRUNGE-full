; measure.LAMBDA_SYSTEM_STRONG_ADDITIVE
; ∀g0 lam g l1 l2.     algebra g0 ∧ positive (space g0,subsets g0,lam) ∧ g ∈ subsets g0 ∧     DISJOINT l1 l2 ∧ l1 ∈ lambda_system g0 lam ∧ l2 ∈ lambda_system g0 lam ⇒     (lam ((l1 ∪ l2) ∩ g) = lam (l1 ∩ g) + lam (l2 ∩ g))
(conjecture "thm_2Emeasure_2ELAMBDA__SYSTEM__STRONG__ADDITIVE" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V0g0" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")) (lambda "V1lam" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V2g" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V3l1" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V4l2" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Ealgebra" "A_27a") "V0g0") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Epositive" "A_27a") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Emeasure_2Espace" "A_27a") "V0g0") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")) (("c_2Emeasure_2Esubsets" "A_27a") "V0g0") "V1lam"))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V2g" (("c_2Emeasure_2Esubsets" "A_27a") "V0g0")) ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EDISJOINT" "A_27a") "V3l1" "V4l2") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V3l1" (("c_2Emeasure_2Elambda__system" "A_27a") "V0g0" "V1lam")) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V4l2" (("c_2Emeasure_2Elambda__system" "A_27a") "V0g0" "V1lam"))))))) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") ("V1lam" (("c_2Epred__set_2EINTER" "A_27a") (("c_2Epred__set_2EUNION" "A_27a") "V3l1" "V4l2") "V2g")) ("c_2Erealax_2Ereal__add" ("V1lam" (("c_2Epred__set_2EINTER" "A_27a") "V3l1" "V2g")) ("V1lam" (("c_2Epred__set_2EINTER" "A_27a") "V4l2" "V2g")))))))))))))))))
; measure.LAMBDA_SYSTEM_ADDITIVE
; ∀g0 lam l1 l2.     algebra g0 ∧ positive (space g0,subsets g0,lam) ⇒     additive (space g0,lambda_system g0 lam,lam)
