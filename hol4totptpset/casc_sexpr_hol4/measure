; measure.additive_def
; ∀m.     additive m ⇔     ∀s t.         s ∈ measurable_sets m ∧ t ∈ measurable_sets m ∧ DISJOINT s t ⇒         (measure m (s ∪ t) = measure m s + measure m t)
(axiom "thm_2Emeasure_2Eadditive__def" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emeasure_2Eadditive" "A_27a") "V0m") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V1s" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V2t" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V1s" (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V2t" (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")) (("c_2Epred__set_2EDISJOINT" "A_27a") "V1s" "V2t"))) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Emeasure_2Emeasure" "A_27a") "V0m" (("c_2Epred__set_2EUNION" "A_27a") "V1s" "V2t")) ("c_2Erealax_2Ereal__add" (("c_2Emeasure_2Emeasure" "A_27a") "V0m" "V1s") (("c_2Emeasure_2Emeasure" "A_27a") "V0m" "V2t")))))))))))))
; measure.countably_additive_def
; ∀m.     countably_additive m ⇔     ∀f.         f ∈ (𝕌(:num) -> measurable_sets m) ∧         (∀m n. m ≠ n ⇒ DISJOINT (f m) (f n)) ∧         BIGUNION (IMAGE f 𝕌(:num)) ∈ measurable_sets m ⇒         measure m ∘ f sums measure m (BIGUNION (IMAGE f 𝕌(:num)))
