; measure.increasing_def
; ∀m.     increasing m ⇔     ∀s t.         s ∈ measurable_sets m ∧ t ∈ measurable_sets m ∧ s ⊆ t ⇒         measure m s ≤ measure m t
(axiom "thm_2Emeasure_2Eincreasing__def" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emeasure_2Eincreasing" "A_27a") "V0m") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V1s" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V2t" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V1s" (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V2t" (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")) (("c_2Epred__set_2ESUBSET" "A_27a") "V1s" "V2t"))) ("c_2Ereal_2Ereal__lte" (("c_2Emeasure_2Emeasure" "A_27a") "V0m" "V1s") (("c_2Emeasure_2Emeasure" "A_27a") "V0m" "V2t"))))))))))))
; measure.measure_space_def
