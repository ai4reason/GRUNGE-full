; measure.MEASURABLE_BIGUNION_PROPERTY
; âˆ€a b f.     sigma_algebra a âˆ§ sigma_algebra b âˆ§ f âˆˆ (space a -> space b) âˆ§     (âˆ€s. s âˆˆ subsets b â‡’ PREIMAGE f s âˆˆ subsets a) â‡’     âˆ€c.         c âŠ† subsets b â‡’         (PREIMAGE f (BIGUNION c) = BIGUNION (IMAGE (PREIMAGE f) c))
(conjecture "thm_2Emeasure_2EMEASURABLE__BIGUNION__PROPERTY" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V0a" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V1b" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V2f" ("ty_2Emin_2Efun" "A_27a" "A_27b") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Esigma__algebra" "A_27a") "V0a") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Esigma__algebra" "A_27b") "V1b") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27b")) "V2f" (("c_2Epred__set_2EFUNSET" "A_27a" "A_27b") (("c_2Emeasure_2Espace" "A_27a") "V0a") (("c_2Emeasure_2Espace" "A_27b") "V1b"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V3s" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) "V3s" (("c_2Emeasure_2Esubsets" "A_27b") "V1b")) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EPREIMAGE" "A_27a" "A_27b") "V2f" "V3s") (("c_2Emeasure_2Esubsets" "A_27a") "V0a")))))))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (lambda "V4c" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" (("c_2Epred__set_2ESUBSET" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) "V4c" (("c_2Emeasure_2Esubsets" "A_27b") "V1b")) (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EPREIMAGE" "A_27a" "A_27b") "V2f" (("c_2Epred__set_2EBIGUNION" "A_27b") "V4c")) (("c_2Epred__set_2EBIGUNION" "A_27a") (("c_2Epred__set_2EIMAGE" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EPREIMAGE" "A_27a" "A_27b") "V2f") "V4c")))))))))))))))
; measure.MEASUBABLE_BIGUNION_LEMMA
; âˆ€a b f.     sigma_algebra a âˆ§ sigma_algebra b âˆ§ f âˆˆ (space a -> space b) âˆ§     (âˆ€s. s âˆˆ subsets b â‡’ PREIMAGE f s âˆˆ subsets a) â‡’     âˆ€c.         COUNTABLE c âˆ§ c âŠ† IMAGE (PREIMAGE f) (subsets b) â‡’         BIGUNION c âˆˆ IMAGE (PREIMAGE f) (subsets b)
