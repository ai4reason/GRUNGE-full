; measure.MEASURABLE_I
; ∀a. sigma_algebra a ⇒ I ∈ measurable a a
(conjecture "thm_2Emeasure_2EMEASURABLE__I" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V0a" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) ("c_2Emin_2E_3D_3D_3E" (("c_2Emeasure_2Esigma__algebra" "A_27a") "V0a") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27a")) ("c_2Ecombin_2EI" "A_27a") (("c_2Emeasure_2Emeasurable" "A_27a" "A_27a") "V0a" "V0a")))))))
; measure.MEASURABLE_COMP
; ∀f g a b c. f ∈ measurable a b ∧ g ∈ measurable b c ⇒ g ∘ f ∈ measurable a c
(conjecture "thm_2Emeasure_2EMEASURABLE__COMP" (forall_tyvarl_term ("A_27a" "A_27b" "A_27c") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V0f" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "A_27c")) (lambda "V1g" ("ty_2Emin_2Efun" "A_27b" "A_27c") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V2a" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V3b" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V4c" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27b")) "V0f" (("c_2Emeasure_2Emeasurable" "A_27a" "A_27b") "V2a" "V3b")) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27b" "A_27c")) "V1g" (("c_2Emeasure_2Emeasurable" "A_27b" "A_27c") "V3b" "V4c"))) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27c")) (("c_2Ecombin_2Eo" "A_27a" "A_27c" "A_27b") "V1g" "V0f") (("c_2Emeasure_2Emeasurable" "A_27a" "A_27c") "V2a" "V4c")))))))))))))))
; measure.MEASURABLE_COMP_STRONG
; ∀f g a b c.     f ∈ measurable a b ∧ sigma_algebra c ∧ g ∈ (space b -> space c) ∧     (∀x. x ∈ subsets c ⇒ PREIMAGE g x ∩ IMAGE f (space a) ∈ subsets b) ⇒     g ∘ f ∈ measurable a c
