; measure.SIGMA_PROPERTY_DISJOINT_LEMMA
; ∀sp a p.     algebra (sp,a) ∧ a ⊆ p ∧ closed_cdi (sp,p) ⇒ subsets (sigma sp a) ⊆ p
(conjecture "thm_2Emeasure_2ESIGMA__PROPERTY__DISJOINT__LEMMA" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V0sp" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (lambda "V1a" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (lambda "V2p" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Ealgebra" "A_27a") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) "V0sp" "V1a")) ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2ESUBSET" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V1a" "V2p") (("c_2Emeasure_2Eclosed__cdi" "A_27a") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) "V0sp" "V2p")))) (("c_2Epred__set_2ESUBSET" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Emeasure_2Esubsets" "A_27a") (("c_2Emeasure_2Esigma" "A_27a") "V0sp" "V1a")) "V2p"))))))))))
; measure.SIGMA_PROPERTY_DISJOINT_WEAK
; ∀sp p a.     algebra (sp,a) ∧ a ⊆ p ∧ subset_class sp p ∧     (∀s. s ∈ p ⇒ sp DIFF s ∈ p) ∧     (∀f.          f ∈ (𝕌(:num) -> p) ∧ (f 0 = ∅) ∧ (∀n. f n ⊆ f (SUC n)) ⇒          BIGUNION (IMAGE f 𝕌(:num)) ∈ p) ∧     (∀f.          f ∈ (𝕌(:num) -> p) ∧ (∀m n. m ≠ n ⇒ DISJOINT (f m) (f n)) ⇒          BIGUNION (IMAGE f 𝕌(:num)) ∈ p) ⇒     subsets (sigma sp a) ⊆ p
