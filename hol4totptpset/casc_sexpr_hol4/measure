m" (("c_2Epair_2E_2C" "ty_2Enum_2Enum" "ty_2Enum_2Enum") "c_2Enum_2E0" "V2n") (("c_2Ecombin_2Eo" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Emeasure_2Emeasure" "A_27a") "V0m") "V1f")) (("c_2Emeasure_2Emeasure" "A_27a") "V0m" (("c_2Epred__set_2EBIGUNION" "A_27a") (("c_2Epred__set_2EIMAGE" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V1f" ("c_2Epred__set_2Ecount" "V2n"))))))))))))))
; measure.INCREASING_ADDITIVE_SUMMABLE
; ∀m f.     algebra (m_space m,measurable_sets m) ∧ positive m ∧ increasing m ∧     additive m ∧ f ∈ (𝕌(:num) -> measurable_sets m) ∧     (∀m n. m ≠ n ⇒ DISJOINT (f m) (f n)) ⇒     summable (measure m ∘ f)
