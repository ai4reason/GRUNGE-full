; measure.MEASURABLE_SUBSET
; âˆ€a b. measurable a b âŠ† measurable a (sigma (space b) (subsets b))
(conjecture "thm_2Emeasure_2EMEASURABLE__SUBSET" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V0a" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V1b" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Epred__set_2ESUBSET" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (("c_2Emeasure_2Emeasurable" "A_27a" "A_27b") "V0a" "V1b") (("c_2Emeasure_2Emeasurable" "A_27a" "A_27b") "V0a" (("c_2Emeasure_2Esigma" "A_27b") (("c_2Emeasure_2Espace" "A_27b") "V1b") (("c_2Emeasure_2Esubsets" "A_27b") "V1b"))))))))))
; measure.MEASURABLE_LIFT
; âˆ€f a b. f âˆˆ measurable a b â‡’ f âˆˆ measurable a (sigma (space b) (subsets b))
