f" "c_2Enum_2E0") ("c_2Epred__set_2EEMPTY" "A_27a")) (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V5n" "ty_2Enum_2Enum" (("c_2Epred__set_2ESUBSET" "A_27a") ("V4f" "V5n") ("V4f" ("c_2Enum_2ESUC" "V5n"))))))) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EBIGUNION" "A_27a") (("c_2Epred__set_2EIMAGE" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V4f" ("c_2Epred__set_2EUNIV" "ty_2Enum_2Enum"))) "V1p")))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V6f" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) "V6f" (("c_2Epred__set_2EFUNSET" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("c_2Epred__set_2EUNIV" "ty_2Enum_2Enum") "V1p")) (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V7m" "ty_2Enum_2Enum" (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V8n" "ty_2Enum_2Enum" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "ty_2Enum_2Enum") "V7m" "V8n")) (("c_2Epred__set_2EDISJOINT" "A_27a") ("V6f" "V7m") ("V6f" "V8n")))))))) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EBIGUNION" "A_27a") (("c_2Epred__set_2EIMAGE" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V6f" ("c_2Epred__set_2EUNIV" "ty_2Enum_2Enum"))) "V1p"))))))))) (("c_2Epred__set_2ESUBSET" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Emeasure_2Esubsets" "A_27a") (("c_2Emeasure_2Esigma" "A_27a") "V0sp" "V2a")) "V1p"))))))))))
; measure.SIGMA_PROPERTY_DISJOINT
; ∀sp p a.     algebra (sp,a) ∧ a ⊆ p ∧     (∀s. s ∈ p ∩ subsets (sigma sp a) ⇒ sp DIFF s ∈ p) ∧     (∀f.          f ∈ (𝕌(:num) -> p ∩ subsets (sigma sp a)) ∧ (f 0 = ∅) ∧          (∀n. f n ⊆ f (SUC n)) ⇒          BIGUNION (IMAGE f 𝕌(:num)) ∈ p) ∧     (∀f.          f ∈ (𝕌(:num) -> p ∩ subsets (sigma sp a)) ∧          (∀m n. m ≠ n ⇒ DISJOINT (f m) (f n)) ⇒          BIGUNION (IMAGE f 𝕌(:num)) ∈ p) ⇒     subsets (sigma sp a) ⊆ p
