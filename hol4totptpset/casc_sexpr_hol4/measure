; measure.closed_cdi_def
; ∀p.     closed_cdi p ⇔     subset_class (space p) (subsets p) ∧     (∀s. s ∈ subsets p ⇒ space p DIFF s ∈ subsets p) ∧     (∀f.          f ∈ (𝕌(:num) -> subsets p) ∧ (f 0 = ∅) ∧ (∀n. f n ⊆ f (SUC n)) ⇒          BIGUNION (IMAGE f 𝕌(:num)) ∈ subsets p) ∧     ∀f.         f ∈ (𝕌(:num) -> subsets p) ∧ (∀m n. m ≠ n ⇒ DISJOINT (f m) (f n)) ⇒         BIGUNION (IMAGE f 𝕌(:num)) ∈ subsets p
(axiom "thm_2Emeasure_2Eclosed__cdi__def" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emeasure_2Eclosed__cdi" "A_27a") "V0p") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Esubset__class" "A_27a") (("c_2Emeasure_2Espace" "A_27a") "V0p") (("c_2Emeasure_2Esubsets" "A_27a") "V0p")) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V1s" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V1s" (("c_2Emeasure_2Esubsets" "A_27a") "V0p")) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EDIFF" "A_27a") (("c_2Emeasure_2Espace" "A_27a") "V0p") "V1s") (("c_2Emeasure_2Esubsets" "A_27a") "V0p"))))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V2f" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) "V2f" (("c_2Epred__set_2EFUNSET" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("c_2Epred__set_2EUNIV" "ty_2Enum_2Enum") (("c_2Emeasure_2Esubsets" "A_27a") "V0p"))) ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("V2f" "c_2Enum_2E0") ("c_2Epred__set_2EEMPTY" "A_27a")) (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V3n" "ty_2Enum_2Enum" (("c_2Epred__set_2ESUBSET" "A_27a") ("V2f" "V3n") ("V2f" ("c_2Enum_2ESUC" "V3n"))))))) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EBIGUNION" "A_27a") (("c_2Epred__set_2EIMAGE" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V2f" ("c_2Epred__set_2EUNIV" "ty_2Enum_2Enum"))) (("c_2Emeasure_2Esubsets" "A_27a") "V0p"))))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V4f" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) "V4f" (("c_2Epred__set_2EFUNSET" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("c_2Epred__set_2EUNIV" "ty_2Enum_2Enum") (("c_2Emeasure_2Esubsets" "A_27a") "V0p"))) (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V5m" "ty_2Enum_2Enum" (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V6n" "ty_2Enum_2Enum" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "ty_2Enum_2Enum") "V5m" "V6n")) (("c_2Epred__set_2EDISJOINT" "A_27a") ("V4f" "V5m") ("V4f" "V6n")))))))) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EBIGUNION" "A_27a") (("c_2Epred__set_2EIMAGE" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V4f" ("c_2Epred__set_2EUNIV" "ty_2Enum_2Enum"))) (("c_2Emeasure_2Esubsets" "A_27a") "V0p")))))))))))))
; measure.smallest_closed_cdi_def
; ∀a.     smallest_closed_cdi a =     (space a,BIGINTER {b | subsets a ⊆ b ∧ closed_cdi (space a,b)})
(axiom "thm_2Emeasure_2Esmallest__closed__cdi__def" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V0a" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emin_2E_3D" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (("c_2Emeasure_2Esmallest__closed__cdi" "A_27a") "V0a") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Espace" "A_27a") "V0a") (("c_2Epred__set_2EBIGINTER" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EGSPEC" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (lambda "V1b" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") "V1b" ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2ESUBSET" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Emeasure_2Esubsets" "A_27a") "V0a") "V1b") (("c_2Emeasure_2Eclosed__cdi" "A_27a") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Espace" "A_27a") "V0a") "V1b")))))))))))))
; measure.measurable_def
; ∀a b.     measurable a b =     {f |      sigma_algebra a ∧ sigma_algebra b ∧ f ∈ (space a -> space b) ∧      ∀s. s ∈ subsets b ⇒ PREIMAGE f s ∩ space a ∈ subsets a}
(axiom "thm_2Emeasure_2Emeasurable__def" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V0a" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V1b" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "A_27b") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Emeasurable" "A_27a" "A_27b") "V0a" "V1b") (("c_2Epred__set_2EGSPEC" ("ty_2Emin_2Efun" "A_27a" "A_27b") ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V2f" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "A_27b") "ty_2Emin_2Ebool") "V2f" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Esigma__algebra" "A_27a") "V0a") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Esigma__algebra" "A_27b") "V1b") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27b")) "V2f" (("c_2Epred__set_2EFUNSET" "A_27a" "A_27b") (("c_2Emeasure_2Espace" "A_27a") "V0a") (("c_2Emeasure_2Espace" "A_27b") "V1b"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V3s" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) "V3s" (("c_2Emeasure_2Esubsets" "A_27b") "V1b")) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EINTER" "A_27a") (("c_2Epred__set_2EPREIMAGE" "A_27a" "A_27b") "V2f" "V3s") (("c_2Emeasure_2Espace" "A_27a") "V0a")) (("c_2Emeasure_2Esubsets" "A_27a") "V0a"))))))))))))))))))
; measure.measure_preserving_def
; ∀m1 m2.     measure_preserving m1 m2 =     {f |      f ∈      measurable (m_space m1,measurable_sets m1)        (m_space m2,measurable_sets m2) ∧ measure_space m1 ∧      measure_space m2 ∧      ∀s.          s ∈ measurable_sets m2 ⇒          (measure m1 (PREIMAGE f s ∩ m_space m1) = measure m2 s)}
(axiom "thm_2Emeasure_2Emeasure__preserving__def" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m1" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V1m2" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "A_27b") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Emeasure__preserving" "A_27a" "A_27b") "V0m1" "V1m2") (("c_2Epred__set_2EGSPEC" ("ty_2Emin_2Efun" "A_27a" "A_27b") ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V2f" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "A_27b") "ty_2Emin_2Ebool") "V2f" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "A_27b")) "V2f" (("c_2Emeasure_2Emeasurable" "A_27a" "A_27b") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Em__space" "A_27a") "V0m1") (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m1")) (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_