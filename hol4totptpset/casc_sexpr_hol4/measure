; measure.SPACE_SIGMA
; ∀sp a. space (sigma sp a) = sp
(conjecture "thm_2Emeasure_2ESPACE__SIGMA" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V0sp" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (lambda "V1a" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Emeasure_2Espace" "A_27a") (("c_2Emeasure_2Esigma" "A_27a") "V0sp" "V1a")) "V0sp")))))))
; measure.MONOTONE_CONVERGENCE
; ∀m s f.     measure_space m ∧ f ∈ (𝕌(:num) -> measurable_sets m) ∧     (∀n. f n ⊆ f (SUC n)) ∧ (s = BIGUNION (IMAGE f 𝕌(:num))) ⇒     measure m ∘ f --> measure m s
