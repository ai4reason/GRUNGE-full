; real_sigma.REAL_SUM_IMAGE_CROSS_SYM
; ∀f s1 s2.     FINITE s1 ∧ FINITE s2 ⇒     (SIGMA (λ(x,y). f (x,y)) (s1 × s2) = SIGMA (λ(y,x). f (x,y)) (s2 × s1))
(conjecture "thm_2Ereal__sigma_2EREAL__SUM__IMAGE__CROSS__SYM" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27a" "A_27b") "ty_2Erealax_2Ereal")) (lambda "V0f" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27a" "A_27b") "ty_2Erealax_2Ereal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V1s1" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V2s2" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27a") "V1s1") (("c_2Epred__set_2EFINITE" "A_27b") "V2s2")) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Ereal__sigma_2EREAL__SUM__IMAGE" ("ty_2Epair_2Eprod" "A_27a" "A_27b")) (("c_2Epair_2EUNCURRY" "A_27a" "A_27b" "ty_2Erealax_2Ereal") (lambda "V3x" "A_27a" (lambda "V4y" "A_27b" ("V0f" (("c_2Epair_2E_2C" "A_27a" "A_27b") "V3x" "V4y"))))) (("c_2Epred__set_2ECROSS" "A_27a" "A_27b") "V1s1" "V2s2")) (("c_2Ereal__sigma_2EREAL__SUM__IMAGE" ("ty_2Epair_2Eprod" "A_27b" "A_27a")) (("c_2Epair_2EUNCURRY" "A_27b" "A_27a" "ty_2Erealax_2Ereal") (lambda "V5y" "A_27b" (lambda "V6x" "A_27a" ("V0f" (("c_2Epair_2E_2C" "A_27a" "A_27b") "V6x" "V5y"))))) (("c_2Epred__set_2ECROSS" "A_27b" "A_27a") "V2s2" "V1s1"))))))))))))
