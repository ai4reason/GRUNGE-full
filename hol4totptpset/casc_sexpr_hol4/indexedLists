; indexedLists.findi_def
; (∀x. findi x [] = 0) ∧ ∀x h t. findi x (h::t) = if x = h then 0 else 1 + findi x t
(axiom "thm_2EindexedLists_2Efindi__def" (forall_tyvarl_term ("A_27a") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V0x" "A_27a" (("c_2Emin_2E_3D" "ty_2Enum_2Enum") (("c_2EindexedLists_2Efindi" "A_27a") "V0x" ("c_2Elist_2ENIL" "A_27a")) "c_2Enum_2E0"))) (("c_2Ebool_2E_21" "A_27a") (lambda "V1x" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V2h" "A_27a" (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V3t" ("ty_2Elist_2Elist" "A_27a") (("c_2Emin_2E_3D" "ty_2Enum_2Enum") (("c_2EindexedLists_2Efindi" "A_27a") "V1x" (("c_2Elist_2ECONS" "A_27a") "V2h" "V3t")) (("c_2Ebool_2ECOND" "ty_2Enum_2Enum") (("c_2Emin_2E_3D" "A_27a") "V1x" "V2h") "c_2Enum_2E0" ("c_2Earithmetic_2E_2B" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT1" "c_2Earithmetic_2EZERO")) (("c_2EindexedLists_2Efindi" "A_27a") "V1x" "V3t")))))))))))))
; indexedLists.MEM_findi
; MEM x l ⇒ findi x l < LENGTH l
(conjecture "thm_2EindexedLists_2EMEM__findi" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" "A_27a") (lambda "V0x" "A_27a" (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V1l" ("ty_2Elist_2Elist" "A_27a") ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27a") "V0x" (("c_2Elist_2ELIST__TO__SET" "A_27a") "V1l")) ("c_2Eprim__rec_2E_3C" (("c_2EindexedLists_2Efindi" "A_27a") "V0x" "V1l") (("c_2Elist_2ELENGTH" "A_27a") "V1l")))))))))
; indexedLists.findi_EL
; ∀l n. n < LENGTH l ∧ ALL_DISTINCT l ⇒ (findi (EL n l) l = n)
(conjecture "thm_2EindexedLists_2Efindi__EL" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V0l" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V1n" "ty_2Enum_2Enum" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" ("c_2Eprim__rec_2E_3C" "V1n" (("c_2Elist_2ELENGTH" "A_27a") "V0l")) (("c_2Elist_2EALL__DISTINCT" "A_27a") "V0l")) (("c_2Emin_2E_3D" "ty_2Enum_2Enum") (("c_2EindexedLists_2Efindi" "A_27a") (("c_2Elist_2EEL" "A_27a") "V1n" "V0l") "V0l") "V1n"))))))))
; indexedLists.EL_findi
; ∀l x. MEM x l ⇒ (EL (findi x l) l = x)
(conjecture "thm_2EindexedLists_2EEL__findi" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V0l" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" "A_27a") (lambda "V1x" "A_27a" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27a") "V1x" (("c_2Elist_2ELIST__TO__SET" "A_27a") "V0l")) (("c_2Emin_2E_3D" "A_27a") (("c_2Elist_2EEL" "A_27a") (("c_2EindexedLists_2Efindi" "A_27a") "V1x" "V0l") "V0l") "V1x"))))))))
; indexedLists.delN_def
; (∀i. delN i [] = []) ∧ ∀i h t. delN i (h::t) = if i = 0 then t else h::delN (i − 1) t
(axiom "thm_2EindexedLists_2EdelN__def" (forall_tyvarl_term ("A_27a") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V0i" "ty_2Enum_2Enum" (("c_2Emin_2E_3D" ("ty_2Elist_2Elist" "A_27a")) (("c_2EindexedLists_2EdelN" "A_27a") "V0i" ("c_2Elist_2ENIL" "A_27a")) ("c_2Elist_2ENIL" "A_27a")))) (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V1i" "ty_2Enum_2Enum" (("c_2Ebool_2E_21" "A_27a") (lambda "V2h" "A_27a" (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V3t" ("ty_2Elist_2Elist" "A_27a") (("c_2Emin_2E_3D" ("ty_2Elist_2Elist" "A_27a")) (("c_2EindexedLists_2EdelN" "A_27a") "V1i" (("c_2Elist_2ECONS" "A_27a") "V2h" "V3t")) (("c_2Ebool_2ECOND" ("ty_2Elist_2Elist" "A_27a")) (("c_2Emin_2E_3D" "ty_2Enum_2Enum") "V1i" "c_2Enum_2E0") "V3t" (("c_2Elist_2ECONS" "A_27a") "V2h" (("c_2EindexedLists_2EdelN" "A_27a") ("c_2Earithmetic_2E_2D" "V1i" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT1" "c_2Earithmetic_2EZERO"))) "V3t")))))))))))))
; indexedLists.delN_shortens
; ∀l i. i < LENGTH l ⇒ (LENGTH (delN i l) = LENGTH l − 1)
(conjecture "thm_2EindexedLists_2EdelN__shortens" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V0l" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V1i" "ty_2Enum_2Enum" ("c_2Emin_2E_3D_3D_3E" ("c_2Eprim__rec_2E_3C" "V1i" (("c_2Elist_2ELENGTH" "A_27a") "V0l")) (("c_2Emin_2E_3D" "ty_2Enum_2Enum") (("c_2Elist_2ELENGTH" "A_27a") (("c_2EindexedLists_2EdelN" "A_27a") "V1i" "V0l")) ("c_2Earithmetic_2E_2D" (("c_2Elist_2ELENGTH" "A_27a") "V0l") ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT1" "c_2Earithmetic_2EZERO")))))))))))
; indexedLists.EL_delN_BEFORE
; ∀l i j. i < j ∧ j < LENGTH l ⇒ (EL i (delN j l) = EL i l)
(conjecture "thm_2EindexedLists_2EEL__delN__BEFORE" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V0l" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V1i" "ty_2Enum_2Enum" (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V2j" "ty_2Enum_2Enum" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" ("c_2Eprim__rec_2E_3C" "V1i" "V2j") ("c_2Eprim__rec_2E_3C" "V2j" (("c_2Elist_2ELENGTH" "A_27a") "V0l"))) (("c_2Emin_2E_3D" "A_27a") (("c_2Elist_2EEL" "A_27a") "V1i" (("c_2EindexedLists_2EdelN" "A_27a") "V2j" "V0l")) (("c_2Elist_2EEL" "A_27a") "V1i" "V0l")))))))))))
; indexedLists.EL_delN_AFTER
; ∀l i j. j ≤ i ∧ i + 1 < LENGTH l ⇒ (EL i (delN j l) = EL (i + 1) l)
(conjecture "thm_2EindexedLists_2EEL__delN__AFTER" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V0l" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V1i" "ty_2Enum_2Enum" (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V2j" "ty_2Enum_2Enum" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" ("c_2Earithmetic_2E_3C_3D" "V2j" "V1i") ("c_2Eprim__rec_2E_3C" ("c_2Earithmetic_2E_2B" "V1i" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT1" "c_2Earithmetic_2EZERO"))) (("c_2Elist_2ELENGTH" "A_27a") "V0l"))) (("c_2Emin_2E_3D" "A_27a") (("c_2Elist_2EEL" "A_27a") "V1i" (("c_2EindexedLists_2EdelN" "A_27a") "V2j" "V0l")) (("c_2Elist_2EEL" "A_27a") ("c_2Earithmetic_2E_2B" "V1i" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT1" "c_2Earithmetic_2EZERO"))) "V0l")))))))))))
; indexedLists.fupdLast_ind
; ∀P.     (∀f. P f []) ∧ (∀f h. P f [h]) ∧     (∀f h v4 v5. P f (v4::v5) ⇒ P f (h::v4::v5)) ⇒     ∀v v1. P v v1
(conjecture "thm_2EindexedLists_2EfupdLast__ind" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "A_27a") ("ty_2Emin_2Efun" ("ty_2Elist_2Elist" "A_27a") "ty_2Emin_2Ebool"))) (lambda "V0P" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "A_27a") ("ty_2Emin_2Efun" ("ty_2Elist_2Elist" "A_27a") "ty_2Emin_2Ebool")) ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27a")) (lambda "V1f" ("ty_2Emin_2Efun" "A_27a" "A_27a") ("V0P" "V1f" ("c_2Elist_2ENIL" "A_27a")))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27a")) (lambda "V2f" ("ty_2Emin_2Efun" "A_27a" "A_27a") (("c_2Ebool_2E_21" "A_27a") (lambda "V3h" "A_27a" ("V0P" "V2f" (("c_2Elist_2ECONS" "A_27a") "V3h" ("c_2Elist_2ENIL" "A_27a"))))))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27a")) (lambda "V4f" ("ty_2Emin_2Efun" "A_27a" "A_27a") (("c_2Ebool_2E_21" "A_27a") (lambda "V5h" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V6v4" "A_27a" (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V7v5" ("ty_2Elist_2Elist" "A_27a") ("c_2Emin_2E_3D_3D_3E" ("V0P" "V4f" (("c_2Elist_2ECONS" "A_27a") "V6v4" "V7v5")) ("V0P" "V4f" (("c_2Elist_2ECONS" "A_27a") "V5h" (("c_2Elist_2ECONS" "A_27a") "V6v4" "V7v5")))))))))))))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27a")) (lambda "V8v" ("ty_2Emin_2Efun" "A_27a" "A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V9v1" ("ty_2Elist_2Elist" "A_27a") ("V0P" "V8v" "V9v1"))))))))))
; indexedLists.fupdLast_def
; (∀f. fupdLast f [] = []) ∧ (∀h f. fupdLast f [h] = [f h]) ∧ ∀v5 v4 h f. fupdLast f (h::v4::v5) = h::fupdLast f (v4::v5)
(conjecture "thm_2EindexedLists_2EfupdLast__def" (forall_tyvarl_term ("A_27a") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27a")) (lambda "V0f" ("ty_2Emin_2Efun" "A_27a" "A_27a") (("c_2Emin_2E_3D" ("ty_2Elist_2Elist" "A_27a")) (("c_2EindexedLists_2EfupdLast" "A_27a") "V0f" ("c_2Elist_2ENIL" "A_27a")) ("c_2Elist_2ENIL" "A_27a")))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V1h" "A_27a" (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27a")) (lambda "V2f" ("ty_2Emin_2Efun" "A_27a" "A_27a") (("c_2Emin_2E_3D" ("ty_2Elist_2Elist" "A_27a")) (("c_2EindexedLists_2EfupdLast" "A_27a") "V2f" (("c_2Elist_2ECONS" "A_27a") "V1h" ("c_2Elist_2ENIL" "A_27a"))) (("c_2Elist_2ECONS" "A_27a") ("V2f" "V1h") ("c_2Elist_2ENIL" "A_27a"))))))) (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V3v5" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" "A_27a") (lambda "V4v4" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V5h" "A_27a" (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27a")) (lambda "V6f" ("ty_2Emin_2Efun" "A_27a" "A_27a") (("c_2Emin_2E_3D" ("ty_2Elist_2Elist" "A_27a")) (("c_2EindexedLists_2EfupdLast" "A_27a") "V6f" (("c_2Elist_2ECONS" "A_27a") "V5h" (("c_2Elist_2ECONS" "A_27a") "V4v4" "V3v5"))) (("c_2Elist_2ECONS" "A_27a") "V5h" (("c_2EindexedLists_2EfupdLast" "A_27a") "V6f" (("c_2Elist_2ECONS" "A_27a") "V4v4" "V3v5"))))))))))))))))
; indexedLists.fupdLast_EQ_NIL
; ((fupdLast f x = []) ⇔ (x = [])) ∧ (([] = fupdLast f x) ⇔ (x = []))
(conjecture "thm_2EindexedLists_2EfupdLast__EQ__NIL" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27a")) (lambda "V0f" ("ty_2Emin_2Efun" "A_27a" "A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V1x" ("ty_2Elist_2Elist" "A_27a") ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Elist_2Elist" "A_27a")) (("c_2EindexedLists_2EfupdLast" "A_27a") "V0f" "V1x") ("c_2Elist_2ENIL" "A_27a")) (("c_2Emin_2E_3D" ("ty_2Elist_2Elist" "A_27a")) "V1x" ("c_2Elist_2ENIL" "A_27a"))) (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Elist_2Elist" "A_27a")) ("c_2Elist_2ENIL" "A_27a") (("c_2EindexedLists_2EfupdLast" "A_27a") "V0f" "V1x")) (("c_2Emin_2E_3D" ("ty_2Elist_2Elist" "A_27a")) "V1x" ("c_2Elist_2ENIL" "A_27a"))))))))))
; indexedLists.fupdLast_FRONT_LAST
; fupdLast f l = if l = [] then [] else FRONT l ++ [f (LAST l)]
(conjecture "thm_2EindexedLists_2EfupdLast__FRONT__LAST" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27a")) (lambda "V0f" ("ty_2Emin_2Efun" "A_27a" "A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V1l" ("ty_2Elist_2Elist" "A_27a") (("c_2Emin_2E_3D" ("ty_2Elist_2Elist" "A_27a")) (("c_2EindexedLists_2EfupdLast" "A_27a") "V0f" "V1l") (("c_2Ebool_2ECOND" ("ty_2Elist_2Elist" "A_27a")) (("c_2Emin_2E_3D" ("ty_2Elist_2Elist" "A_27a")) "V1l" ("c_2Elist_2ENIL" "A_27a")) ("c_2Elist_2ENIL" "A_27a") (("c_2Elist_2EAPPEND" "A_27a") (("c_2Elist_2EFRONT" "A_27a") "V1l") (("c_2Elist_2ECONS" "A_27a") ("V0f" (("c_2Elist_2ELAST" "A_27a") "V1l")) ("c_2Elist_2ENIL" "A_27a")))))))))))
; indexedLists.LIST_RELi_def
; LIST_RELi = (λR a0 a1.      ∀LIST_RELi'.          (∀a0 a1.               (a0 = []) ∧ (a1 = []) ∨               (∃h1 h2 l1 l2.                    (a0 = l1 ++ [h1]) ∧ (a1 = l2 ++ [h2]) ∧                    R (LENGTH l1) h1 h2 ∧ LIST_RELi' l1 l2) ⇒               LIST_RELi' a0 a1) ⇒          LIST_RELi' a0 a1)
(axiom "thm_2EindexedLists_2ELIST__RELi__def" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool"))) ("ty_2Emin_2Efun" ("ty_2Elist_2Elist" "A_27a") ("ty_2Emin_2Efun" ("ty_2Elist_2Elist" "A_27b") "ty_2Emin_2Ebool")))) ("c_2EindexedLists_2ELIST__RELi" "A_27a" "A_27b") (lambda "V0R" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool"))) (lambda "V1a0" ("ty_2Elist_2Elist" "A_27a") (lambda "V2a1" ("ty_2Elist_2Elist" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" ("ty_2Elist_2Elist" "A_27a") ("ty_2Emin_2Efun" ("ty_2Elist_2Elist" "A_27b") "ty_2Emin_2Ebool"))) (lambda "V3LIST__RELi_27" ("ty_2Emin_2Efun" ("ty_2Elist_2Elist" "A_27a") ("ty_2Emin_2Efun" ("ty_2Elist_2Elist" "A_27b") "ty_2Emin_2Ebool")) ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V4a0" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27b")) (lambda "V5a1" ("ty_2Elist_2Elist" "A_27b") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_5C_2F" ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Elist_2Elist" "A_27a")) "V4a0" ("c_2Elist_2ENIL" "A_27a")) (("c_2Emin_2E_3D" ("ty_2Elist_2Elist" "A_27b")) "V5a1" ("c_2Elist_2ENIL" "A_27b"))) (("c_2Ebool_2E_3F" "A_27a") (lambda "V6h1" "A_27a" (("c_2Ebool_2E_3F" "A_27b") (lambda "V7h2" "A_27b" (("c_2Ebool_2E_3F" ("ty_2Elist_2Elist" "A_27a")) (lambda "V8l1" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_3F" ("ty_2Elist_2Elist" "A_27b")) (lambda "V9l2" ("ty_2Elist_2Elist" "A_27b") ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Elist_2Elist" "A_27a")) "V4a0" (("c_2Elist_2EAPPEND" "A_27a") "V8l1" (("c_2Elist_2ECONS" "A_27a") "V6h1" ("c_2Elist_2ENIL" "A_27a")))) ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Elist_2Elist" "A_27b")) "V5a1" (("c_2Elist_2EAPPEND" "A_27b") "V9l2" (("c_2Elist_2ECONS" "A_27b") "V7h2" ("c_2Elist_2ENIL" "A_27b")))) ("c_2Ebool_2E_2F_5C" ("V0R" (("c_2Elist_2ELENGTH" "A_27a") "V8l1") "V6h1" "V7h2") ("V3LIST__RELi_27" "V8l1" "V9l2"))))))))))))) ("V3LIST__RELi_27" "V4a0" "V5a1")))))) ("V3LIST__RELi_27" "V1a0" "V2a1"))))))))))
; indexedLists.LIST_RELi_rules
; ∀R.     LIST_RELi R [] [] ∧     ∀h1 h2 l1 l2.         R (LENGTH l1) h1 h2 ∧ LIST_RELi R l1 l2 ⇒         LIST_RELi R (l1 ++ [h1]) (l2 ++ [h2])
(conjecture "thm_2EindexedLists_2ELIST__RELi__rules" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")))) (lambda "V0R" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool"))) ("c_2Ebool_2E_2F_5C" (("c_2EindexedLists_2ELIST__RELi" "A_27a" "A_27b") "V0R" ("c_2Elist_2ENIL" "A_27a") ("c_2Elist_2ENIL" "A_27b")) (("c_2Ebool_2E_21" "A_27a") (lambda "V1h1" "A_27a" (("c_2Ebool_2E_21" "A_27b") (lambda "V2h2" "A_27b" (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V3l1" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27b")) (lambda "V4l2" ("ty_2Elist_2Elist" "A_27b") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" ("V0R" (("c_2Elist_2ELENGTH" "A_27a") "V3l1") "V1h1" "V2h2") (("c_2EindexedLists_2ELIST__RELi" "A_27a" "A_27b") "V0R" "V3l1" "V4l2")) (("c_2EindexedLists_2ELIST__RELi" "A_27a" "A_27b") "V0R" (("c_2Elist_2EAPPEND" "A_27a") "V3l1" (("c_2Elist_2ECONS" "A_27a") "V1h1" ("c_2Elist_2ENIL" "A_27a"))) (("c_2Elist_2EAPPEND" "A_27b") "V4l2" (("c_2Elist_2ECONS" "A_27b") "V2h2" ("c_2Elist_2ENIL" "A_27b"))))))))))))))))))
; indexedLists.LIST_RELi_ind
; ∀R LIST_RELi'.     LIST_RELi' [] [] ∧     (∀h1 h2 l1 l2.          R (LENGTH l1) h1 h2 ∧ LIST_RELi' l1 l2 ⇒          LIST_RELi' (l1 ++ [h1]) (l2 ++ [h2])) ⇒     ∀a0 a1. LIST_RELi R a0 a1 ⇒ LIST_RELi' a0 a1
