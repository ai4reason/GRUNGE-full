; indexedLists.LIST_RELi_LENGTH
; ∀l1 l2. LIST_RELi R l1 l2 ⇒ (LENGTH l1 = LENGTH l2)
(conjecture "thm_2EindexedLists_2ELIST__RELi__LENGTH" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")))) (lambda "V0R" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool"))) (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V1l1" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27b")) (lambda "V2l2" ("ty_2Elist_2Elist" "A_27b") ("c_2Emin_2E_3D_3D_3E" (("c_2EindexedLists_2ELIST__RELi" "A_27a" "A_27b") "V0R" "V1l1" "V2l2") (("c_2Emin_2E_3D" "ty_2Enum_2Enum") (("c_2Elist_2ELENGTH" "A_27a") "V1l1") (("c_2Elist_2ELENGTH" "A_27b") "V2l2")))))))))))
; indexedLists.LIST_RELi_EL_EQN
; LIST_RELi R l1 l2 ⇔ (LENGTH l1 = LENGTH l2) ∧ ∀i. i < LENGTH l1 ⇒ R i (EL i l1) (EL i l2)
