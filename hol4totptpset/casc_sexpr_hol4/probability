; probability.joint_distribution_sum_mul1
; ∀p X Y f.     prob_space p ∧ FINITE (p_space p) ∧ (events p = POW (p_space p)) ⇒     (SIGMA (λ(x,y). joint_distribution p X Y {(x,y)} * f x)        (IMAGE X (p_space p) × IMAGE Y (p_space p)) =      SIGMA (λx. distribution p X {x} * f x) (IMAGE X (p_space p)))
(conjecture "thm_2Eprobability_2Ejoint__distribution__sum__mul1" (forall_tyvarl_term ("A_27a" "A_27b" "A_27c") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V1X" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27c")) (lambda "V2Y" ("ty_2Emin_2Efun" "A_27a" "A_27c") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Erealax_2Ereal")) (lambda "V3f" ("ty_2Emin_2Efun" "A_27b" "ty_2Erealax_2Ereal") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Eprob__space" "A_27a") "V0p") ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27a") (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Eprobability_2Eevents" "A_27a") "V0p") (("c_2Epred__set_2EPOW" "A_27a") (("c_2Eprobability_2Ep__space" "A_27a") "V0p"))))) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Ereal__sigma_2EREAL__SUM__IMAGE" ("ty_2Epair_2Eprod" "A_27b" "A_27c")) (("c_2Epair_2EUNCURRY" "A_27b" "A_27c" "ty_2Erealax_2Ereal") (lambda "V4x" "A_27b" (lambda "V5y" "A_27c" ("c_2Erealax_2Ereal__mul" (("c_2Eprobability_2Ejoint__distribution" "A_27b" "A_27c" "A_27a") "V0p" "V1X" "V2Y" (("c_2Epred__set_2EINSERT" ("ty_2Epair_2Eprod" "A_27b" "A_27c")) (("c_2Epair_2E_2C" "A_27b" "A_27c") "V4x" "V5y") ("c_2Epred__set_2EEMPTY" ("ty_2Epair_2Eprod" "A_27b" "A_27c")))) ("V3f" "V4x"))))) (("c_2Epred__set_2ECROSS" "A_27b" "A_27c") (("c_2Epred__set_2EIMAGE" "A_27a" "A_27b") "V1X" (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) (("c_2Epred__set_2EIMAGE" "A_27a" "A_27c") "V2Y" (("c_2Eprobability_2Ep__space" "A_27a") "V0p")))) (("c_2Ereal__sigma_2EREAL__SUM__IMAGE" "A_27b") (lambda "V6x" "A_27b" ("c_2Erealax_2Ereal__mul" (("c_2Eprobability_2Edistribution" "A_27b" "A_27a") "V0p" "V1X" (("c_2Epred__set_2EINSERT" "A_27b") "V6x" ("c_2Epred__set_2EEMPTY" "A_27b"))) ("V3f" "V6x"))) (("c_2Epred__set_2EIMAGE" "A_27a" "A_27b") "V1X" (("c_2Eprobability_2Ep__space" "A_27a") "V0p")))))))))))))))
