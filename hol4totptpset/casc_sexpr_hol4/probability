; probability.PROB_REAL_SUM_IMAGE
; ∀p s.     prob_space p ∧ s ∈ events p ∧ (∀x. x ∈ s ⇒ {x} ∈ events p) ∧ FINITE s ⇒     (prob p s = SIGMA (λx. prob p {x}) s)
(conjecture "thm_2Eprobability_2EPROB__REAL__SUM__IMAGE" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V1s" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Eprob__space" "A_27a") "V0p") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V1s" (("c_2Eprobability_2Eevents" "A_27a") "V0p")) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V2x" "A_27a" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27a") "V2x" "V1s") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EINSERT" "A_27a") "V2x" ("c_2Epred__set_2EEMPTY" "A_27a")) (("c_2Eprobability_2Eevents" "A_27a") "V0p"))))) (("c_2Epred__set_2EFINITE" "A_27a") "V1s")))) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Eprobability_2Eprob" "A_27a") "V0p" "V1s") (("c_2Ereal__sigma_2EREAL__SUM__IMAGE" "A_27a") (lambda "V3x" "A_27a" (("c_2Eprobability_2Eprob" "A_27a") "V0p" (("c_2Epred__set_2EINSERT" "A_27a") "V3x" ("c_2Epred__set_2EEMPTY" "A_27a")))) "V1s")))))))))
; probability.PROB_EQUIPROBABLE_FINITE_UNIONS
; ∀p s.     prob_space p ∧ s ∈ events p ∧ (∀x. x ∈ s ⇒ {x} ∈ events p) ∧ FINITE s ∧     (∀x y. x ∈ s ∧ y ∈ s ⇒ (prob p {x} = prob p {y})) ⇒     (prob p s = &CARD s * prob p {CHOICE s})
(conjecture "thm_2Eprobability_2EPROB__EQUIPROBABLE__FINITE__UNIONS" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V1s" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Eprob__space" "A_27a") "V0p") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V1s" (("c_2Eprobability_2Eevents" "A_27a") "V0p")) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V2x" "A_27a" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27a") "V2x" "V1s") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EINSERT" "A_27a") "V2x" ("c_2Epred__set_2EEMPTY" "A_27a")) (("c_2Eprobability_2Eevents" "A_27a") "V0p"))))) ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27a") "V1s") (("c_2Ebool_2E_21" "A_27a") (lambda "V3x" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V4y" "A_27a" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" "A_27a") "V3x" "V1s") (("c_2Ebool_2EIN" "A_27a") "V4y" "V1s")) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Eprobability_2Eprob" "A_27a") "V0p" (("c_2Epred__set_2EINSERT" "A_27a") "V3x" ("c_2Epred__set_2EEMPTY" "A_27a"))) (("c_2Eprobability_2Eprob" "A_27a") "V0p" (("c_2Epred__set_2EINSERT" "A_27a") "V4y" ("c_2Epred__set_2EEMPTY" "A_27a"))))))))))))) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Eprobability_2Eprob" "A_27a") "V0p" "V1s") ("c_2Erealax_2Ereal__mul" ("c_2Ereal_2Ereal__of__num" (("c_2Epred__set_2ECARD" "A_27a") "V1s")) (("c_2Eprobability_2Eprob" "A_27a") "V0p" (("c_2Epred__set_2EINSERT" "A_27a") (("c_2Epred__set_2ECHOICE" "A_27a") "V1s") ("c_2Epred__set_2EEMPTY" "A_27a"))))))))))))
; probability.PROB_REAL_SUM_IMAGE_FN
; ∀p f e s.     prob_space p ∧ e ∈ events p ∧ (∀x. x ∈ s ⇒ e ∩ f x ∈ events p) ∧     FINITE s ∧ (∀x y. x ∈ s ∧ y ∈ s ∧ x ≠ y ⇒ DISJOINT (f x) (f y)) ∧     (BIGUNION (IMAGE f s) ∩ p_space p = p_space p) ⇒     (prob p e = SIGMA (λx. prob p (e ∩ f x)) s)
(conjecture "thm_2Eprobability_2EPROB__REAL__SUM__IMAGE__FN" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V1f" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V2e" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V3s" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Eprob__space" "A_27a") "V0p") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V2e" (("c_2Eprobability_2Eevents" "A_27a") "V0p")) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27b") (lambda "V4x" "A_27b" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27b") "V4x" "V3s") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EINTER" "A_27a") "V2e" ("V1f" "V4x")) (("c_2Eprobability_2Eevents" "A_27a") "V0p"))))) ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27b") "V3s") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27b") (lambda "V5x" "A_27b" (("c_2Ebool_2E_21" "A_27b") (lambda "V6y" "A_27b" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" "A_27b") "V5x" "V3s") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" "A_27b") "V6y" "V3s") ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "A_27b") "V5x" "V6y")))) (("c_2Epred__set_2EDISJOINT" "A_27a") ("V1f" "V5x") ("V1f" "V6y"))))))) (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EINTER" "A_27a") (("c_2Epred__set_2EBIGUNION" "A_27a") (("c_2Epred__set_2EIMAGE" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V1f" "V3s")) (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) (("c_2Eprobability_2Ep__space" "A_27a") "V0p"))))))) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Eprobability_2Eprob" "A_27a") "V0p" "V2e") (("c_2Ereal__sigma_2EREAL__SUM__IMAGE" "A_27b") (lambda "V7x" "A_27b" (("c_2Eprobability_2Eprob" "A_27a") "V0p" (("c_2Epred__set_2EINTER" "A_27a") "V2e" ("V1f" "V7x")))) "V3s")))))))))))))
; probability.PROB_REAL_SUM_IMAGE_SPACE
; ∀p.     prob_space p ∧ (∀x. x ∈ p_space p ⇒ {x} ∈ events p) ∧ FINITE (p_space p) ⇒     (SIGMA (λx. prob p {x}) (p_space p) = 1)
