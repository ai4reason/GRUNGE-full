; probability.expectation_def
; expectation = integral
(axiom "thm_2Eprobability_2Eexpectation__def" (forall_tyvarl_term ("A_27a") (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") "ty_2Eextreal_2Eextreal"))) ("c_2Eprobability_2Eexpectation" "A_27a") ("c_2Elebesgue_2Eintegral" "A_27a"))))
; probability.conditional_expectation_def
; ∀p X s.     conditional_expectation p X s =     @f.         real_random_variable f p ∧         ∀g.             g ∈ s ⇒             (integral p (λx. f x * indicator_fn g x) =              integral p (λx. X x * indicator_fn g x))
(axiom "thm_2Eprobability_2Econditional__expectation__def" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1X" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (lambda "V2s" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (("c_2Eprobability_2Econditional__expectation" "A_27a") "V0p" "V1X" "V2s") (("c_2Emin_2E_40" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V3f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Ereal__random__variable" "A_27a") "V3f" "V0p") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V4g" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V4g" "V2s") (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") (("c_2Elebesgue_2Eintegral" "A_27a") "V0p" (lambda "V5x" "A_27a" ("c_2Eextreal_2Eextreal__mul" ("V3f" "V5x") (("c_2Emeasure_2Eindicator__fn" "A_27a") "V4g" "V5x")))) (("c_2Elebesgue_2Eintegral" "A_27a") "V0p" (lambda "V6x" "A_27a" ("c_2Eextreal_2Eextreal__mul" ("V1X" "V6x") (("c_2Emeasure_2Eindicator__fn" "A_27a") "V4g" "V6x"))))))))))))))))))))
; probability.conditional_prob_def
