; probability.distribution_prob_space
; ∀p X s.     random_variable X p s ⇒ prob_space (space s,subsets s,distribution p X)
(conjecture "thm_2Eprobability_2Edistribution__prob__space" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V1X" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V2s" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) ("c_2Emin_2E_3D_3D_3E" (("c_2Eprobability_2Erandom__variable" "A_27a" "A_27b") "V1X" "V0p" "V2s") (("c_2Eprobability_2Eprob__space" "A_27b") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Emeasure_2Espace" "A_27b") "V2s") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")) (("c_2Emeasure_2Esubsets" "A_27b") "V2s") (("c_2Eprobability_2Edistribution" "A_27b" "A_27a") "V0p" "V1X")))))))))))))
; probability.uniform_distribution_prob_space
; ∀X p s.     FINITE (p_space p) ∧ FINITE (space s) ∧ random_variable X p s ⇒     prob_space (space s,subsets s,uniform_distribution p X s)
(conjecture "thm_2Eprobability_2Euniform__distribution__prob__space" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V0X" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V1p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V2s" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27a") (("c_2Eprobability_2Ep__space" "A_27a") "V1p")) ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27b") (("c_2Emeasure_2Espace" "A_27b") "V2s")) (("c_2Eprobability_2Erandom__variable" "A_27a" "A_27b") "V0X" "V1p" "V2s"))) (("c_2Eprobability_2Eprob__space" "A_27b") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Emeasure_2Espace" "A_27b") "V2s") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")) (("c_2Emeasure_2Esubsets" "A_27b") "V2s") (("c_2Eprobability_2Euniform__distribution" "A_27b" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) ("ty_2Emin_2Efun" "A_27a" "A_27b") "A_27b") "V1p" "V0X" "V2s")))))))))))))
; probability.distribution_partition
; ∀p X.     prob_space p ∧ (∀x. x ∈ p_space p ⇒ {x} ∈ events p) ∧     FINITE (p_space p) ∧ random_variable X p Borel ⇒     (SIGMA (λx. distribution p X {x}) (IMAGE X (p_space p)) = 1)
(conjecture "thm_2Eprobability_2Edistribution__partition" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1X" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Eprob__space" "A_27a") "V0p") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V2x" "A_27a" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27a") "V2x" (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EINSERT" "A_27a") "V2x" ("c_2Epred__set_2EEMPTY" "A_27a")) (("c_2Eprobability_2Eevents" "A_27a") "V0p"))))) ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27a") (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) (("c_2Eprobability_2Erandom__variable" "A_27a" "ty_2Eextreal_2Eextreal") "V1X" "V0p" "c_2Emeasure_2EBorel")))) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Ereal__sigma_2EREAL__SUM__IMAGE" "ty_2Eextreal_2Eextreal") (lambda "V3x" "ty_2Eextreal_2Eextreal" (("c_2Eprobability_2Edistribution" "ty_2Eextreal_2Eextreal" "A_27a") "V0p" "V1X" (("c_2Epred__set_2EINSERT" "ty_2Eextreal_2Eextreal") "V3x" ("c_2Epred__set_2EEMPTY" "ty_2Eextreal_2Eextreal")))) (("c_2Epred__set_2EIMAGE" "A_27a" "ty_2Eextreal_2Eextreal") "V1X" (("c_2Eprobability_2Ep__space" "A_27a") "V0p"))) ("c_2Ereal_2Ereal__of__num" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT1" "c_2Earithmetic_2EZERO")))))))))))
; probability.distribution_lebesgue_thm1
; ∀X p s A.     random_variable X p s ∧ A ∈ subsets s ⇒     (Normal (distribution p X A) =      integral p (indicator_fn (PREIMAGE X A ∩ p_space p)))
(conjecture "thm_2Eprobability_2Edistribution__lebesgue__thm1" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V0X" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V1p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V2s" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V3A" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Erandom__variable" "A_27a" "A_27b") "V0X" "V1p" "V2s") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) "V3A" (("c_2Emeasure_2Esubsets" "A_27b") "V2s"))) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") ("c_2Eextreal_2ENormal" (("c_2Eprobability_2Edistribution" "A_27b" "A_27a") "V1p" "V0X" "V3A")) (("c_2Elebesgue_2Eintegral" "A_27a") "V1p" (("c_2Emeasure_2Eindicator__fn" "A_27a") (("c_2Epred__set_2EINTER" "A_27a") (("c_2Epred__set_2EPREIMAGE" "A_27a" "A_27b") "V0X" "V3A") (("c_2Eprobability_2Ep__space" "A_27a") "V1p"))))))))))))))))
; probability.distribution_lebesgue_thm2
; ∀X p s A.     random_variable X p s ∧ A ∈ subsets s ⇒     (Normal (distribution p X A) =      integral (space s,subsets s,distribution p X) (indicator_fn A))
(conjecture "thm_2Eprobability_2Edistribution__lebesgue__thm2" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V0X" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V1p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool"))) (lambda "V2s" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V3A" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Erandom__variable" "A_27a" "A_27b") "V0X" "V1p" "V2s") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) "V3A" (("c_2Emeasure_2Esubsets" "A_27b") "V2s"))) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") ("c_2Eextreal_2ENormal" (("c_2Eprobability_2Edistribution" "A_27b" "A_27a") "V1p" "V0X" "V3A")) (("c_2Elebesgue_2Eintegral" "A_27b") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Emeasure_2Espace" "A_27b") "V2s") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")) (("c_2Emeasure_2Esubsets" "A_27b") "V2s") (("c_2Eprobability_2Edistribution" "A_27b" "A_27a") "V1p" "V0X"))) (("c_2Emeasure_2Eindicator__fn" "A_27b") "V3A"))))))))))))))
; probability.finite_expectation1
; ∀p X.     FINITE (p_space p) ∧ real_random_variable X p ⇒     (expectation p X =      SIGMA (λr. r * Normal (prob p (PREIMAGE X {r} ∩ p_space p)))        (IMAGE X (p_space p)))
(conjecture "thm_2Eprobability_2Efinite__expectation1" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1X" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27a") (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) (("c_2Eprobability_2Ereal__random__variable" "A_27a") "V1X" "V0p")) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") (("c_2Eprobability_2Eexpectation" "A_27a") "V0p" "V1X") (("c_2Eextreal_2EEXTREAL__SUM__IMAGE" "ty_2Eextreal_2Eextreal") (lambda "V2r" "ty_2Eextreal_2Eextreal" ("c_2Eextreal_2Eextreal__mul" "V2r" ("c_2Eextreal_2ENormal" (("c_2Eprobability_2Eprob" "A_27a") "V0p" (("c_2Epred__set_2EINTER" "A_27a") (("c_2Epred__set_2EPREIMAGE" "A_27a" "ty_2Eextreal_2Eextreal") "V1X" (("c_2Epred__set_2EINSERT" "ty_2Eextreal_2Eextreal") "V2r" ("c_2Epred__set_2EEMPTY" "ty_2Eextreal_2Eextreal"))) (("c_2Eprobability_2Ep__space" "A_27a") "V0p")))))) (("c_2Epred__set_2EIMAGE" "A_27a" "ty_2Eextreal_2Eextreal") "V1X" (("c_2Eprobability_2Ep__space" "A_27a") "V0p")))))))))))
; probability.finite_expectation2
; ∀p X.     FINITE (IMAGE X (p_space p)) ∧ real_random_variable X p ⇒     (expectation p X =      SIGMA (λr. r * Normal (prob p (PREIMAGE X {r} ∩ p_space p)))        (IMAGE X (p_space p)))
