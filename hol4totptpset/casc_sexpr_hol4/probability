; probability.joint_distribution_sums_1: ∀V0p V1X V2Y.     prob_space V0p ∧ FINITE (p_space V0p) ∧ (events V0p = POW (p_space V0p)) ⇒     (SIGMA (λ(V3x,V4y). joint_distribution V0p V1X V2Y {(V3x,V4y)})        (IMAGE V1X (p_space V0p) × IMAGE V2Y (p_space V0p)) = 1)
(conjecture "thm_2Eprobability_2Ejoint__distribution__sums__1" (forall_tyvarl_term ("A_27a" "A_27b" "A_27c") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V1X" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27c")) (lambda "V2Y" ("ty_2Emin_2Efun" "A_27a" "A_27c") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Eprob__space" "A_27a") "V0p") ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27a") (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Eprobability_2Eevents" "A_27a") "V0p") (("c_2Epred__set_2EPOW" "A_27a") (("c_2Eprobability_2Ep__space" "A_27a") "V0p"))))) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Ereal__sigma_2EREAL__SUM__IMAGE" ("ty_2Epair_2Eprod" "A_27b" "A_27c")) (("c_2Epair_2EUNCURRY" "A_27b" "A_27c" "ty_2Erealax_2Ereal") (lambda "V3x" "A_27b" (lambda "V4y" "A_27c" (("c_2Eprobability_2Ejoint__distribution" "A_27b" "A_27c" "A_27a") "V0p" "V1X" "V2Y" (("c_2Epred__set_2EINSERT" ("ty_2Epair_2Eprod" "A_27b" "A_27c")) (("c_2Epair_2E_2C" "A_27b" "A_27c") "V3x" "V4y") ("c_2Epred__set_2EEMPTY" ("ty_2Epair_2Eprod" "A_27b" "A_27c"))))))) (("c_2Epred__set_2ECROSS" "A_27b" "A_27c") (("c_2Epred__set_2EIMAGE" "A_27a" "A_27b") "V1X" (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) (("c_2Epred__set_2EIMAGE" "A_27a" "A_27c") "V2Y" (("c_2Eprobability_2Ep__space" "A_