; probability.PROB_DISCRETE_EVENTS_COUNTABLE
; ∀p.     prob_space p ∧ COUNTABLE (p_space p) ∧     (∀x. x ∈ p_space p ⇒ {x} ∈ events p) ⇒     ∀s. s ⊆ p_space p ⇒ s ∈ events p
(conjecture "thm_2Eprobability_2EPROB__DISCRETE__EVENTS__COUNTABLE" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Eprob__space" "A_27a") "V0p") ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2Ecountable" "A_27a") (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) (("c_2Ebool_2E_21" "A_27a") (lambda "V1x" "A_27a" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27a") "V1x" (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EINSERT" "A_27a") "V1x" ("c_2Epred__set_2EEMPTY" "A_27a")) (("c_2Eprobability_2Eevents" "A_27a") "V0p"))))))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V2s" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" (("c_2Epred__set_2ESUBSET" "A_27a") "V2s" (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V2s" (("c_2Eprobability_2Eevents" "A_27a") "V0p"))))))))))
; probability.marginal_joint_zero
; ∀p X Y s t.     prob_space p ∧ (events p = POW (p_space p)) ∧     ((distribution p X s = 0) ∨ (distribution p Y t = 0)) ⇒     (joint_distribution p X Y (s × t) = 0)
(conjecture "thm_2Eprobability_2Emarginal__joint__zero" (forall_tyvarl_term ("A_27a" "A_27b" "A_27c") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V1X" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27c")) (lambda "V2Y" ("ty_2Emin_2Efun" "A_27a" "A_27c") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V3s" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool")) (lambda "V4t" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Eprob__space" "A_27a") "V0p") ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Eprobability_2Eevents" "A_27a") "V0p") (("c_2Epred__set_2EPOW" "A_27a") (("c_2Eprobability_2Ep__space" "A_27a") "V0p"))) ("c_2Ebool_2E_5C_2F" (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Eprobability_2Edistribution" "A_27b" "A_27a") "V0p" "V1X" "V3s") ("c_2Ereal_2Ereal__of__num" "c_2Enum_2E0")) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Eprobability_2Edistribution" "A_27c" "A_27a") "V0p" "V2Y" "V4t") ("c_2Ereal_2Ereal__of__num" "c_2Enum_2E0"))))) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Eprobability_2Ejoint__distribution" "A_27b" "A_27c" "A_27a") "V0p" "V1X" "V2Y" (("c_2Epred__set_2ECROSS" "A_27b" "A_27c") "V3s" "V4t")) ("c_2Ereal_2Ereal__of__num" "c_2Enum_2E0")))))))))))))))
; probability.marginal_joint_zero3
; ∀p X Y Z s t u.     prob_space p ∧ (events p = POW (p_space p)) ∧     ((distribution p X s = 0) ∨ (distribution p Y t = 0) ∨      (distribution p Z u = 0)) ⇒     (joint_distribution3 p X Y Z (s × (t × u)) = 0)
(conjecture "thm_2Eprobability_2Emarginal__joint__zero3" (forall_tyvarl_term ("A_27a" "A_27b" "A_27c" "A_27d") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V1X" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27c")) (lambda "V2Y" ("ty_2Emin_2Efun" "A_27a" "A_27c") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27d")) (lambda "V3Z" ("ty_2Emin_2Efun" "A_27a" "A_27d") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V4s" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool")) (lambda "V5t" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27d" "ty_2Emin_2Ebool")) (lambda "V6u" ("ty_2Emin_2Efun" "A_27d" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Eprob__space" "A_27a") "V0p") ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Eprobability_2Eevents" "A_27a") "V0p") (("c_2Epred__set_2EPOW" "A_27a") (("c_2Eprobability_2Ep__space" "A_27a") "V0p"))) ("c_2Ebool_2E_5C_2F" (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Eprobability_2Edistribution" "A_27b" "A_27a") "V0p" "V1X" "V4s") ("c_2Ereal_2Ereal__of__num" "c_2Enum_2E0")) ("c_2Ebool_2E_5C_2F" (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Eprobability_2Edistribution" "A_27c" "A_27a") "V0p" "V2Y" "V5t") ("c_2Ereal_2