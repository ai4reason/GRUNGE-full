; probability.finite_marginal_product_space_POW
; ∀p X Y.     (POW (p_space p) = events p) ∧     random_variable X p (IMAGE X (p_space p),POW (IMAGE X (p_space p))) ∧     random_variable Y p (IMAGE Y (p_space p),POW (IMAGE Y (p_space p))) ∧     FINITE (p_space p) ⇒     measure_space       (IMAGE X (p_space p) × IMAGE Y (p_space p),        POW (IMAGE X (p_space p) × IMAGE Y (p_space p)),        (λa. prob p (PREIMAGE (λx. (X x,Y x)) a ∩ p_space p)))
(conjecture "thm_2Eprobability_2Efinite__marginal__product__space__POW" (forall_tyvarl_term ("A_27a" "A_27b" "A_27c") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V1X" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27c")) (lambda "V2Y" ("ty_2Emin_2Efun" "A_27a" "A_27c") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Epred__set_2EPOW" "A_27a") (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) (("c_2Eprobability_2Eevents" "A_27a") "V0p")) ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Erandom__variable" "A_27a" "A_27b") "V1X" "V0p" (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Epred__set_2EIMAGE" "A_27a" "A_27b") "V1X" (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) (("c_2Epred__set_2EPOW" "A_27b") (("c_2Epred__set_2EIMAGE" "A_27a" "A_27b") "V1X" (("c_2Eprobability_2Ep__space" "A_27a") "V0p"))))) ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Erandom__variable" "A_27a" "A_27c") "V2Y" "V0p" (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Epred__set_2EIMAGE" "A_27a" "A_27c") "V2Y" (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) (("c_2Epred__set_2EPOW" "A_27c") (("c_2Epred__set_2EIMAGE" "A_27a" "A_27c") "V2Y" (("c_2Eprobability_2Ep__space" "A_27a") "V0p"))))) (("c_2Epred__set_2EFINITE" "A_27a") (("c_2Eprobability_2Ep__space" "A_27a") "V0p"))))) (("c_2Emeasure_2Emeasure__space" ("ty_2Epair_2Eprod" "A_27b" "A_27c")) (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" "A_27c") "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" "A_27c") "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" "A_27c") "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Epred__set_2ECROSS" "A_27b" "A_27c") (("c_2Epred__set_2EIMAGE" "A_27a" "A_27b") "V1X" (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) (("c_2Epred__set_2EIMAGE" "A_27a" "A_27c") "V2Y" (("c_2Eprobability_2Ep__space" "A_27a") "V0p"))) (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" "A_27c") "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" "A_27c") "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")) (("c_2Epred__set_2EPOW" ("ty_2Epair_2Eprod" "A_27b" "A_27c")) (("c_2Epred__set_2ECROSS" "A_27b" "A_27c") (("c_2Epred__set_2EIMAGE" "A_27a" "A_27b") "V1X" (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) (("c_2Epred__set_2EIMAGE" "A_27a" "A_27c") "V2Y" (("c_2Eprobability_2Ep__space" "A_27a") "V0p")))) (lambda "V3a" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" "A_27c") "ty_2Emin_2Ebool") (("c_2Eprobability_2Eprob" "A_27a") "V0p" (("c_2Epred__set_2EINTER" "A_27a") (("c_2Epred__set_2EPREIMAGE" "A_27a" ("ty_2Epair_2Eprod" "A_27b" "A_27c")) (lambda "V4x" "A_27a" (("c_2Epair_2E_2C" "A_27b" "A_27c") ("V1X" "V4x") ("V2Y" "V4x"))) "V3a") (("c_2Eprobability_2Ep__space" "A_27a") "V0p"))))))))))))))))
; probability.finite_marginal_product_space_POW2
; ∀p s1 s2 X Y.     (POW (p_space p) = events p) ∧ random_variable X p (s1,POW s1) ∧     random_variable Y p (s2,POW s2) ∧ FINITE (p_space p) ∧ FINITE s1 ∧     FINITE s2 ⇒     measure_space (s1 × s2,POW (s1 × s2),joint_distribution p X Y)
(conjecture "thm_2Eprobability_2Efinite__marginal__product__space__POW2" (forall_tyvarl_term ("A_27a" "A_27b" "A_27c") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V1s1" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool")) (lambda "V2s2" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V3X" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27c")) (lambda "V4Y" ("ty_2Emin_2Efun" "A_27a" "A_27c") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Epred__set_2EPOW" "A_27a") (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) (("c_2Eprobability_2Eevents" "A_27a") "V0p")) ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Erandom__variable" "A_27a" "A_27b") "V3X" "V0p" (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) "V1s1" (("c_2Epred__set_2EPOW" "A_27b") "V1s1"))) ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Erandom__variable" "A_27a" "A_27c") "V4Y" "V0p" (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) "V2s2" (("c_2Epred__set_2EPOW" "A_27c") "V2s2"))) ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27a") (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27b") "V1s1") (("c_2Epred__set_2EFINITE" "A_27c") "V2s2")))))) (("c_2Emeasure_2Emeasure__space" ("ty_2Epair_2Eprod" "A_27b" "A_27c")) (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" "A_27c") "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" "A_27c") "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" "A_27c") "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Epred__set_2ECROSS" "A_27b" "A_27c") "V1s1" "V2s2") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" "A_27c") "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" "A_27c") "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")) (("c_2Epred__set_2EPOW" ("ty_2Epair_2Eprod" "A_27b" "A_27c")) (("c_2Epred__set_2ECROSS" "A_27b" "A_27c") "V1s1" "V2s2")) (("c_2Eprobability_2Ejoint__distribution" "A_27b" "A_27c" "A_27a") "V0p" "V3X" "V4Y")))))))))))))))))
; probability.finite_marginal_product_space_POW3
; ∀p s1 s2 s3 X Y Z.     (POW (p_space p) = events p) ∧ random_variable X p (s1,POW s1) ∧     random_variable Y p (s2,POW s2) ∧ random_variable Z p (s3,POW s3) ∧     FINITE (p_space p) ∧ FINITE s1 ∧ FINITE s2 ∧ FINITE s3 ⇒     measure_space       (s1 × (s2 × s3),POW (s1 × (s2 × s3)),joint_distribution3 p X Y Z)
(conjecture "thm_2Eprobability_2Efinite__marginal__product__space__POW3" (forall_tyvarl_term ("A_27a" "A_27b" "A_27c" "A_27d") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V1s1" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool")) (lambda "V2s2" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27d" "ty_2Emin_2Ebool")) (lambda "V3s3" ("ty_2Emin_2Efun" "A_27d" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V4X" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27c")) (lambda "V5Y" ("ty_2Emin_2Efun" "A_27a" "A_27c") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27d")) (lambda "V6Z" ("ty_2Emin_2Efun" "A_27a" "A_27d") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Epred__set_2EPOW" "A_27a") (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) (("c_2Eprobability_2Eevents" "A_27a") "V0p")) ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Erandom__variable" "A_27a" "A_27b") "V4X" "V0p" (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) "V1s1" (("c_2Epred__set_2EPOW" "A_27b") "V1s1"))) ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Erandom__variable" "A_27a" "A_27c") "V5Y" "V0p" (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) "V2s2" (("c_2Epred__set_2EPOW" "A_27c") "V2s2"))) ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Erandom__variable" "A_27a" "A_27d") "V6Z" "V0p" (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27d" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27d" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) "V3s3" (("c_2Epred__set_2EPOW" "A_27d") "V3s3"))) ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27a") (("c_2Eprobability_2Ep__space" "A_27a") "V0p")) ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27b") "V1s1") ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27c") "V2s2") (("c_2Epred__set_2EFINITE" "A_27d") "V3s3")))))))) (("c_2Emeasure_2Emeasure__space" ("ty_2Epair_2Eprod" "A_27b" ("ty_2Epair_2Eprod" "A_27c" "A_27d"))) (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" ("ty_2Epair_2Eprod" "A_27c" "A_27d")) "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" ("ty_2Epair_2Eprod" "A_27c" "A_27d")) "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" ("ty_2Epair_2Eprod" "A_27c" "A_27d")) "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Epred__set_2ECROSS" "A_27b" ("ty_2Epair_2Eprod" "A_27c" "A_27d")) "V1s1" (("c_2Epred__set_2ECROSS" "A_27c" "A_27d") "V2s2" "V3s3")) (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" ("ty_2Epair_2Eprod" "A_27c" "A_27d")) "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" ("ty_2Epair_2Eprod" "A_27c" "A_27d")) "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")) (("c_2Epred__set_2EPOW" ("ty_2Epair_2Eprod" "A_27b" ("ty_2Epair_2Eprod" "A_27c" "A_27d"))) (("c_2Epred__set_2ECROSS" "A_27b" ("ty_2Epair_2Eprod" "A_27c" "A_27d")) "V1s1" (("c_2Epred__set_2ECROSS" "A_27c" "A_27d") "V2s2" "V3s3"))) (("c_2Eprobability_2Ejoint__distribution3" "A_27b" "A_27c" "A_27d" "A_27a") "V0p" "V4X" "V5Y" "V6Z")))))))))))))))))))))
; probability.prob_x_eq_1_imp_prob_y_eq_0
; ∀p x.     prob_space p ∧ {x} ∈ events p ∧ (prob p {x} = 1) ⇒     ∀y. {y} ∈ events p ∧ y ≠ x ⇒ (prob p {y} = 0)
(conjecture "thm_2Eprobability_2Eprob__x__eq__1__imp__prob__y__eq__0" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" "A_27a") (lambda "V1x" "A_27a" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Eprob__space" "A_27a") "V0p") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EINSERT" "A_27a") "V1x" ("c_2Epred__set_2EEMPTY" "A_27a")) (("c_2Eprobability_2Eevents" "A_27a") "V0p")) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Eprobability_2Eprob" "A_27a") "V0p" (("c_2Epred__set_2EINSERT" "A_27a") "V1x" ("c_2Epred__set_2EEMPTY" "A_27a"))) ("c_2Ereal_2Ereal__of__num" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT1" "c_2Earithmetic_2EZERO")))))) (("c_2Ebool_2E_21" "A_27a") (lambda "V2y" "A_27a" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EINSERT" "A_27a") "V2y" ("c_2Epred__set_2EEMPTY" "A_27a")) (("c_2Eprobability_2Eevents" "A_27a") "V0p")) ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "A_27a") "V2y" "V1x"))) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Eprobability_2Eprob" "A_27a") "V0p" (("c_2Epred__set_2EINSERT" "A_27a") "V2y" ("c_2Epred__set_2EEMPTY" "A_27a"))) ("c_2Ereal_2Ereal__of__num" "c_2Enum_2E0"))))))))))))
; probability.distribution_x_eq_1_imp_distribution_y_eq_0
; ∀X p x.     random_variable X p (IMAGE X (p_space p),POW (IMAGE X (p_space p))) ∧     (distribution p X {x} = 1) ⇒     ∀y. y ≠ x ⇒ (distribution p X {y} = 0)
(conjecture "thm_2Eprobability_2Edistribution__x__eq__1__imp__distribution__y__eq__0" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V0X" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V1p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" "A_27b") (lambda "V2x" "A_27b" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Erandom__variable" "A_27a" "A_27b") "V0X" "V1p" (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Epred__set_2EIMAGE" "A_27a" "A_27b") "V0X" (("c_2Eprobability_2Ep__space" "A_27a") "V1p")) (("c_2Epred__set_2EPOW" "A_27b") (("c_2Epred__set_2EIMAGE" "A_27a" "A_27b") "V0X" (("c_2Eprobability_2Ep__space" "A_27a") "V1p"))))) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Eprobability_2Edistribution" "A_27b" "A_27a") "V1p" "V0X" (("c_2Epred__set_2EINSERT" "A_27b") "V2x" ("c_2Epred__set_2EEMPTY" "A_27b"))) ("c_2Ereal_2Ereal__of__num" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT1" "c_2Earithmetic_2EZERO"))))) (("c_2Ebool_2E_21" "A_27b") (lambda "V3y" "A_27b" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "A_27b") "V3y" "V2x")) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Eprobability_2Edistribution" "A_27b" "A_27a") "V1p" "V0X" (("c_2Epred__set_2EINSERT" "A_27b") "V3y" ("c_2Epred__set_2EEMPTY" "A_27b"))) ("c_2Ereal_2Ereal__of__num" "c_2Enum_2E0"))))))))))))))
; probability.joint_distribution_sym
; ∀p X Y a b.     prob_space p ⇒     (joint_distribution p X Y (a × b) = joint_distribution p Y X (b × a))
(conjecture "thm_2Eprobability_2Ejoint__distribution__sym" (forall_tyvarl_term ("A_27a" "A_27b" "A_27c") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V1X" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27c")) (lambda "V2Y" ("ty_2Emin_2Efun" "A_27a" "A_27c") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V3a" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool")) (lambda "V4b" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" (("c_2Eprobability_2Eprob__space" "A_27a") "V0p") (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Eprobability_2Ejoint__distribution" "A_27b" "A_27c" "A_27a") "V0p" "V1X" "V2Y" (("c_2Epred__set_2ECROSS" "A_27b" "A_27c") "V3a" "V4b")) (("c_2Eprobability_2Ejoint__distribution" "A_27c" "A_27b" "A_27a") "V0p" "V2Y" "V1X" (("c_2Epred__set_2ECROSS" "A_27c" "A_27b") "V4b" "V3a"))))))))))))))))
; probability.joint_distribution_pos
; ∀p X Y a.     prob_space p ∧ (events p = POW (p_space p)) ⇒     0 ≤ joint_distribution p X Y a
(conjecture "thm_2Eprobability_2Ejoint__distribution__pos" (forall_tyvarl_term ("A_27a" "A_27b" "A_27c") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V1X" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27c")) (lambda "V2Y" ("ty_2Emin_2Efun" "A_27a" "A_27c") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" "A_27c") "ty_2Emin_2Ebool")) (lambda "V3a" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" "A_27c") "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Eprob__space" "A_27a") "V0p") (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Eprobability_2Eevents" "A_27a") "V0p") (("c_2Epred__set_2EPOW" "A_27a") (("c_2Eprobability_2Ep__space" "A_27a") "V0p")))) ("c_2Ereal_2Ereal__lte" ("c_2Ereal_2Ereal__of__num" "c_2Enum_2E0") (("c_2Eprobability_2Ejoint__distribution" "A_27b" "A_27c" "A_27a") "V0p" "V1X" "V2Y" "V3a")))))))))))))
; probability.joint_distribution_le_1
; ∀p X Y a.     prob_space p ∧ (events p = POW (p_space p)) ⇒     joint_distribution p X Y a ≤ 1
(conjecture "thm_2Eprobability_2Ejoint__distribution__le__1" (forall_tyvarl_term ("A_27a" "A_27b" "A_27c") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27b")) (lambda "V1X" ("ty_2Emin_2Efun" "A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "A_27c")) (lambda "V2Y" ("ty_2Emin_2Efun" "A_27a" "A_27c") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" "A_27c") "ty_2Emin_2Ebool")) (lambda "V3a" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27b" "A_27c") "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Eprob__space" "A_27a") "V0p") (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Eprobability_2Eevents" "A_27a") "V0p") (("c_2Epred__set_2EPOW" "A_27a") (("c_2Eprobability_2Ep__space" "A_27a") "V0p")))) ("c_2Ereal_2Ereal__lte" (("c_2Eprobability_2Ejoint__distribution" "A_27b" "A_27c" "A_27a") "V0p" "V1X" "V2Y" "V3a") ("c_2Ereal_2Ereal__of__num" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT1" "c_2Earithmetic_2EZERO")))))))))))))))
; probability.joint_distribution_le
; ∀p X Y a b.     prob_space p ∧ (events p = POW (p_space p)) ⇒     joint_distribution p X Y (a × b) ≤ distribution p X a
