(conjecture "thm_2Elebesgue_2Epos__fn__integral__cmul__indicator" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V1s" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" "ty_2Erealax_2Ereal") (lambda "V2c" "ty_2Erealax_2Ereal" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V1s" (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")) ("c_2Ereal_2Ereal__lte" ("c_2Ereal_2Ereal__of__num" "c_2Enum_2E0") "V2c"))) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") (("c_2Elebesgue_2Epos__fn__integral" "A_27a") "V0m" (lambda "V3x" "A_27a" ("c_2Eextreal_2Eextreal__mul" ("c_2Eextreal_2ENormal" "V2c") (("c_2Emeasure_2Eindicator__fn" "A_27a") "V1s" "V3x")))) ("c_2Eextreal_2ENormal" ("c_2Erealax_2Ereal__mul" "V2c" (("c_2Emeasure_2Emeasure" "A_27a") "V0m" "V1s")))))))))))))
; lebesgue.pos_fn_integral_sum_cmul_indicator
; ∀m s a x.     measure_space m ∧ FINITE s ∧ (∀i. i ∈ s ⇒ 0 ≤ x i) ∧     (∀i. i ∈ s ⇒ a i ∈ measurable_sets m) ⇒     (pos_fn_integral m        (λt. SIGMA (λi. Normal (x i) * indicator_fn (a i) t) s) =      Normal (SIGMA (λi. x i * measure m (a i)) s))
(conjecture "thm_2Elebesgue_2Epos__fn__integral__sum__cmul__indicator" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool")) (lambda "V1s" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V2a" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal")) (lambda "V3x" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "ty_2Enum_2Enum") "V1s") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V4i" "ty_2Enum_2Enum" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "ty_2Enum_2Enum") "V4i" "V1s") ("c_2Ereal_2Ereal__lte" ("c_2Ereal_2Ereal__of__num" "c_2Enum_2E0") ("V3x" "V4i"))))) (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V5i" "ty_2Enum_2Enum" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "ty_2Enum_2Enum") "V5i" "V1s") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("V2a" "V5i") (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")))))))) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") (("c_2Elebesgue_2Epos__fn__integral" "A_27a") "V0m" (lambda "V6t" "A_27a" (("c_2Eextreal_2EEXTREAL__SUM__IMAGE" "ty_2Enum_2Enum") (lambda "V7i" "ty_2Enum_2Enum" ("c_2Eextreal_2Eextreal__mul" ("c_2Eextreal_2ENormal" ("V3x" "V7i")) (("c_2Emeasure_2Eindicator__fn" "A_27a") ("V2a" "V7i") "V6t"))) "V1s"))) ("c_2Eextreal_2ENormal" (("c_2Ereal__sigma_2EREAL__SUM__IMAGE" "ty_2Enum_2Enum") (lambda "V8i" "ty_2Enum_2Enum" ("c_2Erealax_2Ereal__mul" ("V3x" "V8i") (("c_2Emeasure_2Emeasure" "A_27a") "V0m" ("V2a" "V8i")))) "V1s"))))))))))))))
; lebesgue.lebesgue_monotone_convergence_lemma
; ∀m f fi g r'.     measure_space m ∧     (∀i. fi i ∈ measurable (m_space m,measurable_sets m) Borel) ∧     (∀x. mono_increasing (λi. fi i x)) ∧     (∀x. x ∈ m_space m ⇒ (sup (IMAGE (λi. fi i x) 𝕌(:num)) = f x)) ∧     r' ∈ psfis m g ∧ (∀x. g x ≤ f x) ∧ (∀i x. 0 ≤ fi i x) ⇒     r' ≤ sup (IMAGE (λi. pos_fn_integral m (fi i)) 𝕌(:num))
