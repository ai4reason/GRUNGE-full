; lebesgue.psfis_pos: ∀V0m V1f V2a.     measure_space V0m ∧ V2a ∈ psfis V0m V1f ⇒     ∀V3x. V3x ∈ m_space V0m ⇒ 0 ≤ V1f V3x
(conjecture "thm_2Elebesgue_2Epsfis__pos" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Ebool_2E_21" "ty_2Eextreal_2Eextreal") (lambda "V2a" "ty_2Eextreal_2Eextreal" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") (("c_2Ebool_2EIN" "ty_2Eextreal_2Eextreal") "V2a" (("c_2Elebesgue_2Epsfis" "A_27a") "V0m" "V1f"))) (("c_2Ebool_2E_21" "A_27a") (lambda "V3x" "A_27a" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27a") "V3x" (("c_2Emeasure_2Em__space" "A_27a") "V0m")) ("c_2Eextreal_2Eextreal__le" ("c_2Eextreal_2Eextreal__of__num" "c_2Enum_2E0") ("V1f" "V3x"))))))))))))))
; lebesgue.pos_simple_fn_integral_zero: ∀V0m V1s V2a V3x.     measure_space V0m ∧ pos_simple_fn V0m (λV4t. 0) V1s V2a V3x ⇒     (pos_simple_fn_integral V0m V1s V2a V3x = 0)
