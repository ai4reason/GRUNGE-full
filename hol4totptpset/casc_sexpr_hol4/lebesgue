; lebesgue.fn_seq_def
; ∀m f.     fn_seq m f =     (λn x.          SIGMA            (λk.                 &k / 2 pow n *                 indicator_fn                   {x |                    x ∈ m_space m ∧ &k / 2 pow n ≤ f x ∧                    f x < (&k + 1) / 2 pow n} x) (count (4 ** n)) +          2 pow n * indicator_fn {x | x ∈ m_space m ∧ 2 pow n ≤ f x} x)
(axiom "thm_2Elebesgue_2Efn__seq__def" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal"))) (("c_2Elebesgue_2Efn__seq" "A_27a") "V0m" "V1f") (lambda "V2n" "ty_2Enum_2Enum" (lambda "V3x" "A_27a" ("c_2Eextreal_2Eextreal__add" (("c_2Eextreal_2EEXTREAL__SUM__IMAGE" "ty_2Enum_2Enum") (lambda "V4k" "ty_2Enum_2Enum" ("c_2Eextreal_2Eextreal__mul" ("c_2Eextreal_2Eextreal__div" ("c_2Eextreal_2Eextreal__of__num" "V4k") ("c_2Eextreal_2Eextreal__pow" ("c_2Eextreal_2Eextreal__of__num" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT2" "c_2Earithmetic_2EZERO"))) "V2n")) (("c_2Emeasure_2Eindicator__fn" "A_27a") (("c_2Epred__set_2EGSPEC" "A_27a" "A_27a") (lambda "V5x" "A_27a" (("c_2Epair_2E_2C" "A_27a" "ty_2Emin_2Ebool") "V5x" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" "A_27a") "V5x" (("c_2Emeasure_2Em__space" "A_27a") "V0m")) ("c_2Ebool_2E_2F_5C" ("c_2Eextreal_2Eextreal__le" ("c_2Eextreal_2Eextreal__div" ("c_2Eextreal_2Eextreal__of__num" "V4k") ("c_2Eextreal_2Eextreal__pow" ("c_2Eextreal_2Eextreal__of__num" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT2" "c_2Earithmetic_2EZERO"))) "V2n")) ("V1f" "V5x")) ("c_2Eextreal_2Eextreal__lt" ("V1f" "V5x") ("c_2Eextreal_2Eextreal__div" ("c_2Eextreal_2Eextreal__add" ("c_2Eextreal_2Eextreal__of__num" "V4k") ("c_2Eextreal_2Eextreal__of__num" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT1" "c_2Earithmetic_2EZERO")))) ("c_2Eextreal_2Eextreal__pow" ("c_2Eextreal_2Eextreal__of__num" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT2" "c_2Earithmetic_2EZERO"))) "V2n")))))))) "V3x"))) ("c_2Epred__set_2Ecount" ("c_2Earithmetic_2EEXP" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT2" ("c_2Earithmetic_2EBIT1" "c_2Earithmetic_2EZERO"))) "V2n"))) ("c_2Eextreal_2Eextreal__mul" ("c_2Eextreal_2Eextreal__pow" ("c_2Eextreal_2Eextreal__of__num" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT2" "c_2Earithmetic_2EZERO"))) "V2n") (("c_2Emeasure_2Eindicator__fn" "A_27a") (("c_2Epred__set_2EGSPEC" "A_27a" "A_27a") (lambda "V6x" "A_27a" (("c_2Epair_2E_2C" "A_27a" "ty_2Emin_2Ebool") "V6x" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" "A_27a") "V6x" (("c_2Emeasure_2Em__space" "A_27a") "V0m")) ("c_2Eextreal_2Eextreal__le" ("c_2Eextreal_2Eextreal__pow" ("c_2Eextreal_2Eextreal__of__num" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT2" "c_2Earithmetic_2EZERO"))) "V2n") ("V1f" "V6x")))))) "V3x"))))))))))))
; lebesgue.fn_seq_integral_def
; ∀m f.     fn_seq_integral m f =     (λn.          Normal            (SIGMA               (λk.                    &k / 2 pow n *                    measure m                      {x |                       x ∈ m_space m ∧ &k / 2 pow n ≤ f x ∧                       f x < (&k + 1) / 2 pow n}) (count (4 ** n)) +             2 pow n * measure m {x | x ∈ m_space m ∧ 2 pow n ≤ f x}))
