; lebesgue.pos_fn_integral_sum: ∀V0m V1f V2s.     FINITE V2s ∧ measure_space V0m ∧     (∀V3i. V3i ∈ V2s ⇒ ∀V4x. 0 ≤ V1f V3i V4x) ∧     (∀V5i.          V5i ∈ V2s ⇒          V1f V5i ∈ measurable (m_space V0m,measurable_sets V0m) Borel) ⇒     (pos_fn_integral V0m (λV6x. SIGMA (λV7i. V1f V7i V6x) V2s) =      SIGMA (λV8i. pos_fn_integral V0m (V1f V8i)) V2s)
(conjecture "thm_2Elebesgue_2Epos__fn__integral__sum" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal"))) (lambda "V1f" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V2s" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27b") "V2s") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27b") (lambda "V3i" "A_27b" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27b") "V3i" "V2s") (("c_2Ebool_2E_21" "A_27a") (lambda "V4x" "A_27a" ("c_2Eextreal_2Eextreal__le" ("c_2Eextreal_2Eextreal__of__num" "c_2Enum_2E0") ("V1f" "V3i" "V4x"))))))) (("c_2Ebool_2E_21" "A_27b") (lambda "V5i" "A_27b" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27b") "V5i" "V2s") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) ("V1f" "V5i") (("c_2Emeasure_2Emeasurable" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Em__space" "A_27a") "V0m") (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")) "c_2Emeasure_2EBorel")))))))) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") (("c_2Elebesgue_2Epos__fn__i