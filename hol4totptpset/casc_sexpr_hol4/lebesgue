; lebesgue.pos_simple_fn_integral_cmul: ∀V0m V1f V2s V3a V4x V5z.     measure_space V0m ∧ pos_simple_fn V0m V1f V2s V3a V4x ∧ 0 ≤ V5z ⇒     pos_simple_fn V0m (λV6x. Normal V5z * V1f V6x) V2s V3a       (λV7i. V5z * V4x V7i) ∧     (pos_simple_fn_integral V0m V2s V3a (λV8i. V5z * V4x V8i) =      Normal V5z * pos_simple_fn_integral V0m V2s V3a V4x)
(conjecture "thm_2Elebesgue_2Epos__simple__fn__integral__cmul" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool")) (lambda "V2s" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V3a" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal")) (lambda "V4x" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal") (("c_2Ebool_2E_21" "ty_2Erealax_2Ereal") (lambda "V5z" "ty_2Erealax_2Ereal" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Epos__simple__fn" "A_27a") "V0m" "V1f" "V2s" "V3a" "V4x") ("c_2Ereal_2Ereal__lte" ("c_2Ereal_2Ereal__of__num" "c_2Enum_2E0") "V5z"))) ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Epos__simple__fn" "A_27a") "V0m" (lambda "V6x" "A_27a" ("c_2Eextreal_2Eextreal__mul" ("c_2Eextreal_2ENormal" "V5z") ("V1f" "V6x"))) "V2s" "V3a" (lambda "V7i" "ty_2Enum_2Enum" ("c_2Erealax_2Ereal__mul" "V5z" ("V4x" "V7i")))) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") (("c_2Elebesgue_2Epos__simple__fn__integral" "A_27a") "V0m" "V2s" "V3a" (lambda "V8i" "ty_2Enum_