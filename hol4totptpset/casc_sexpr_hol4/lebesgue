; lebesgue.pos_fn_integral_sum
; ∀m f s.     FINITE s ∧ measure_space m ∧ (∀i. i ∈ s ⇒ ∀x. 0 ≤ f i x) ∧     (∀i. i ∈ s ⇒ f i ∈ measurable (m_space m,measurable_sets m) Borel) ⇒     (pos_fn_integral m (λx. SIGMA (λi. f i x) s) =      SIGMA (λi. pos_fn_integral m (f i)) s)
(conjecture "thm_2Elebesgue_2Epos__fn__integral__sum" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal"))) (lambda "V1f" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V2s" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27b") "V2s") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27b") (lambda "V3i" "A_27b" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27b") "V3i" "V2s") (("c_2Ebool_2E_21" "A_27a") (lambda "V4x" "A_27a" ("c_2Eextreal_2Eextreal__le" ("c_2Eextreal_2Eextreal__of__num" "c_2Enum_2E0") ("V1f" "V3i" "V4x"))))))) (("c_2Ebool_2E_21" "A_27b") (lambda "V5i" "A_27b" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27b") "V5i" "V2s") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) ("V1f" "V5i") (("c_2Emeasure_2Emeasurable" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Em__space" "A_27a") "V0m") (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")) "c_2Emeasure_2EBorel")))))))) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") (("c_2Elebesgue_2Epos__fn__integral" "A_27a") "V0m" (lambda "V6x" "A_27a" (("c_2Eextreal_2EEXTREAL__SUM__IMAGE" "A_27b") (lambda "V7i" "A_27b" ("V1f" "V7i" "V6x")) "V2s"))) (("c_2Eextreal_2EEXTREAL__SUM__IMAGE" "A_27b") (lambda "V8i" "A_27b" (("c_2Elebesgue_2Epos__fn__integral" "A_27a") "V0m" ("V1f" "V8i"))) "V2s")))))))))))
; lebesgue.pos_fn_integral_disjoint_sets
; ∀m f s t.     measure_space m ∧ DISJOINT s t ∧ s ∈ measurable_sets m ∧     t ∈ measurable_sets m ∧     f ∈ measurable (m_space m,measurable_sets m) Borel ∧ (∀x. 0 ≤ f x) ⇒     (pos_fn_integral m (λx. f x * indicator_fn (s ∪ t) x) =      pos_fn_integral m (λx. f x * indicator_fn s x) +      pos_fn_integral m (λx. f x * indicator_fn t x))
(conjecture "thm_2Elebesgue_2Epos__fn__integral__disjoint__sets" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V2s" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V3t" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EDISJOINT" "A_27a") "V2s" "V3t") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V2s" (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V3t" (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) "V1f" (("c_2Emeasure_2Emeasurable" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Em__space" "A_27a") "V0m") (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")) "c_2Emeasure_2EBorel")) (("c_2Ebool_2E_21" "A_27a") (lambda "V4x" "A_27a" ("c_2Eextreal_2Eextreal__le" ("c_2Eextreal_2Eextreal__of__num" "c_2Enum_2E0") ("V1f" "V4x"))))))))) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") (("c_2Elebesgue_2Epos__fn__integral" "A_27a") "V0m" (lambda "V5x" "A_27a" ("c_2Eextreal_2Eextreal__mul" ("V1f" "V5x") (("c_2Emeasure_2Eindicator__fn" "A_27a") (("c_2Epred__set_2EUNION" "A_27a") "V2s" "V3t") "V5x")))) ("c_2Eextreal_2Eextreal__add" (("c_2Elebesgue_2Epos__fn__integral" "A_27a") "V0m" (lambda "V6x" "A_27a" ("c_2Eextreal_2Eextreal__mul" ("V1f" "V6x") (("c_2Emeasure_2Eindicator__fn" "A_27a") "V2s" "V6x")))) (("c_2Elebesgue_2Epos__fn__integral" "A_27a") "V0m" (lambda "V7x" "A_27a" ("c_2Eextreal_2Eextreal__mul" ("V1f" "V7x") (("c_2Emeasure_2Eindicator__fn" "A_27a") "V3t" "V7x")))))))))))))))))
; lebesgue.pos_fn_integral_disjoint_sets_sum
; ∀m f s a.     FINITE s ∧ measure_space m ∧ (∀i. i ∈ s ⇒ a i ∈ measurable_sets m) ∧     (∀x. 0 ≤ f x) ∧ (∀i j. i ∈ s ∧ j ∈ s ∧ i ≠ j ⇒ DISJOINT (a i) (a j)) ∧     f ∈ measurable (m_space m,measurable_sets m) Borel ⇒     (pos_fn_integral m (λx. f x * indicator_fn (BIGUNION (IMAGE a s)) x) =      SIGMA (λi. pos_fn_integral m (λx. f x * indicator_fn (a i) x)) s)
(conjecture "thm_2Elebesgue_2Epos__fn__integral__disjoint__sets__sum" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V2s" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V3a" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27b") "V2s") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27b") (lambda "V4i" "A_27b" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27b") "V4i" "V2s") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("V3a" "V4i") (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m"))))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V5x" "A_27a" ("c_2Eextreal_2Eextreal__le" ("c_2Eextreal_2Eextreal__of__num" "c_2Enum_2E0") ("V1f" "V5x")))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27b") (lambda "V6i" "A_27b" (("c_2Ebool_2E_21" "A_27b") (lambda "V7j" "A_27b" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" "A_27b") "V6i" "V2s") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" "A_27b") "V7j" "V2s") ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "A_27b") "V6i" "V7