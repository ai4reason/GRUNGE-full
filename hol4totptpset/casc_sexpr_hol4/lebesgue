; lebesgue.finite_POW_prod_measure_reduce3
; ∀m0 m1 m2.     measure_space m0 ∧ measure_space m1 ∧ measure_space m2 ∧     FINITE (m_space m0) ∧ FINITE (m_space m1) ∧ FINITE (m_space m2) ∧     (POW (m_space m0) = measurable_sets m0) ∧     (POW (m_space m1) = measurable_sets m1) ∧     (POW (m_space m2) = measurable_sets m2) ⇒     ∀a0 a1 a2.         a0 ∈ measurable_sets m0 ∧ a1 ∈ measurable_sets m1 ∧         a2 ∈ measurable_sets m2 ⇒         (prod_measure3 m0 m1 m2 (a0 × (a1 × a2)) =          measure m0 a0 * measure m1 a1 * measure m2 a2)
(conjecture "thm_2Elebesgue_2Efinite__POW__prod__measure__reduce3" (forall_tyvarl_term ("A_27a" "A_27b" "A_27c") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m0" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V1m1" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V2m2" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27c" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m0") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27b") "V1m1") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27c") "V2m2") ("c_2Ebool_2E_2F_5C" (