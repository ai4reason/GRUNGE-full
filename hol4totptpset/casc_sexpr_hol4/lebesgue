; lebesgue.IN_psfis: ∀V0m V1r V2f.     V1r ∈ psfis V0m V2f ⇒     ∃V3s V4a V5x.         pos_simple_fn V0m V2f V3s V4a V5x ∧         (V1r = pos_simple_fn_integral V0m V3s V4a V5x)
(conjecture "thm_2Elebesgue_2EIN__psfis" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" "ty_2Eextreal_2Eextreal") (lambda "V1r" "ty_2Eextreal_2Eextreal" (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V2f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "ty_2Eextreal_2Eextreal") "V1r" (("c_2Elebesgue_2Epsfis" "A_27a") "V0m" "V2f")) (("c_2Ebool_2E_3F" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool")) (lambda "V3s" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool") (("c_2Ebool_2E_3F" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V4a" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Ebool_2E_3F" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal")) (lambda "V5x" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Epos__simple__fn" "A_27a") "V0m" "V2f" "V3s" "V4a" "V5x") (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") "V1r" (("c_2Elebesgue_2Epos__simple__fn__integral" "A_27a") "V0m" "V3s" "V4a" "V5x"))))))))))))))))))
; lebesgue.IN_psfis_eq: ∀V0m V1r V2f.     V1r ∈ psfis V0m V2f ⇔     ∃V3s V4a V5x.         pos_simple_fn V0m V2f V3s V4a V5x ∧         (V1r = pos_simple_fn_integral V0m V3s V4a V5x)
