; lebesgue.psfis_sum
; ∀m f a P.     measure_space m ∧ (∀i. i ∈ P ⇒ a i ∈ psfis m (f i)) ∧ FINITE P ⇒     SIGMA a P ∈ psfis m (λt. SIGMA (λi. f i t) P)
(conjecture "thm_2Elebesgue_2Epsfis__sum" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal"))) (lambda "V1f" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Eextreal_2Eextreal")) (lambda "V2a" ("ty_2Emin_2Efun" "A_27b" "ty_2Eextreal_2Eextreal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V3P" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27b") (lambda "V4i" "A_27b" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27b") "V4i" "V3P") (("c_2Ebool_2EIN" "ty_2Eextreal_2Eextreal") ("V2a" "V4i") (("c_2Elebesgue_2Epsfis" "A_27a") "V0m" ("V1f" "V4i")))))) (("c_2Epred__set_2EFINITE" "A_27b") "V3P"))) (("c_2Ebool_2EIN" "ty_2Eextreal_2Eextreal") (("c_2Eextreal_2EEXTREAL__SUM__IMAGE" "A_27b") "V2a" "V3P") (("c_2Elebesgue_2Epsfis" "A_27a") "V0m" (lambda "V5t" "A_27a" (("c_2Eextreal_2EEXTREAL__SUM__IMAGE" "A_27b") (lambda "V6i" "A_27b" ("V1f" "V6i" "V5t")) "V3P")))))))))))))))
; lebesgue.psfis_intro
; ∀m a x P.     measure_space m ∧ (∀i. i ∈ P ⇒ a i ∈ measurable_sets m) ∧     (∀i. i ∈ P ⇒ 0 ≤ x i) ∧ FINITE P ⇒     Normal (SIGMA (λi. x i * measure m (a i)) P) ∈     psfis m (λt. SIGMA (λi. Normal (x i) * indicator_fn (a i) t) P)
(conjecture "thm_2Elebesgue_2Epsfis__intro" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V1a" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Erealax_2Ereal")) (lambda "V2x" ("ty_2Emin_2Efun" "A_27b" "ty_2Erealax_2Ereal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V3P" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27b") (lambda "V4i" "A_27b" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27b") "V4i" "V3P") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("V1a" "V4i") (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m"))))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27b") (lambda "V5i" "A_27b" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27b") "V5i" "V3P") ("c_2Ereal_2Ereal__lte" ("c_2Ereal_2Ereal__of__num" "c_2Enum_2E0") ("V2x" "V5i"))))) (("c_2Epred__set_2EFINITE" "A_27b") "V3P")))) (("c_2Ebool_2EIN" "ty_2Eextreal_2Eextreal") ("c_2Eextreal_2ENormal" (("c_2Ereal__sigma_2EREAL__SUM__IMAGE" "A_27b") (lambda "V6i" "A_27b" ("c_2Erealax_2Ereal__mul" ("V2x" "V6i") (("c_2Emeasure_2Emeasure" "A_27a") "V0m" ("V1a" "V6i")))) "V3P")) (("c_2Elebesgue_2Epsfis" "A_27a") "V0m" (lambda "V7t" "A_27a" (("c_2Eextreal_2EEXTREAL__SUM__IMAGE" "A_27b") (lambda "V8i" "A_27b" ("c_2Eextreal_2Eextreal__mul" ("c_2Eextreal_2ENormal" ("V2x" "V8i")) (("c_2Emeasure_2Eindicator__fn" "A_27a") ("V1a" "V8i") "V7t"))) "V3P")))))))))))))))
; lebesgue.pos_simple_fn_integral_sub
; ∀m f s a x g s' b y.     measure_space m ∧ (∀x. g x ≤ f x) ∧ (∀x. g x ≠ PosInf) ∧     pos_simple_fn m f s a x ∧ pos_simple_fn m g s' b y ⇒     ∃s'' c z.         pos_simple_fn m (λx. f x − g x) s'' c z ∧         (pos_simple_fn_integral m s a x − pos_simple_fn_integral m s' b y =          pos_simple_fn_integral m s'' c z)
(conjecture "thm_2Elebesgue_2Epos__simple__fn__integral__sub" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool")) (lambda "V2s" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V3a" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal")) (lambda "V4x" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V5g" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool")) (lambda "V6s_27" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V7b" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal")) (lambda "V8y" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V9x" "A_27a" ("c_2Eextreal_2Eextreal__le" ("V5g" "V9x") ("V1f" "V9x")))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V10x" "A_27a" ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") ("V5g" "V10x") "c_2Eextreal_2EPosInf")))) ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Epos__simple__fn" "A_27a") "V0m" "V1f" "V2s" "V3a" "V4x") (("c_2Emeasure_2Epos__simple__fn" "A_27a") "V0m" "V5g" "V6s_27" "V7b" "V8y"))))) (("c_2Ebool_2E_3F" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool")) (lambda "V11s_27_27" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool") (("c_2Ebool_2E_3F" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V12c" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Ebool_2E_3F" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal")) (lambda "V13z" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Epos__simple__fn" "A_27a") "V0m" (lambda "V14x" "A_27a" ("c_2Eextreal_2Eextreal__sub" ("V1f" "V14x") ("V5g" "V14x"))) "V11s_27_27" "V12c" "V13z") (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") ("c_2Eextreal_2Eextreal__sub" (("c_2Elebesgue_2Epos__simple__fn__integral" "A_27a") "V0m" "V2s" "V3a" "V4x") (("c_2Elebesgue_2Epos__simple__fn__integral" "A_27a") "V0m" "V6s_27" "V7b" "V8y")) (("c_2Elebesgue_2Epos__simple__fn__integral" "A_27a") "V0m" "V11s_27_27" "V12c" "V13z"))))))))))))))))))))))))))))))
; lebesgue.psfis_sub
; ∀m f g a b.     measure_space m ∧ (∀x. g x ≤ f x) ∧ (∀x. g x ≠ PosInf) ∧ a ∈ psfis m f ∧     b ∈ psfis m g ⇒     a − b ∈ psfis m (λx. f x − g x)
(conjecture "thm_2Elebesgue_2Epsfis__sub" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V2g" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Ebool_2E_21" "ty_2Eextreal_2Eextreal") (lambda "V3a" "ty_2Eextreal_2Eextreal" (("c_2Ebool_2E_21" "ty_2Eextreal_2Eextreal") (lambda "V4b" "ty_2Eextreal_2Eextreal" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V5x" "A_27a" ("c_2Eextreal_2Eextreal__le" ("V2g" "V5x") ("V1f" "V5x")))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V6x" "A_27a" ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") ("V2g" "V6x") "c_2Eextreal_2EPosInf")))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" "ty_2Eextreal_2Eextreal") "V3a" (("c_2Elebesgue_2Epsfis" "A_27a") "V0m" "V1f")) (("c_2Ebool_2EIN" "ty_2Eextreal_2Eextreal") "V4b" (("c_2Elebesgue_2Epsfis" "A_27a") "V0m" "V2g")))))) (("c_2Ebool_2EIN" "ty_2Eextreal_2Eextreal") ("c_2Eextreal_2Eextreal__sub" "V3a" "V4b") (("c_2Elebesgue_2Epsfis" "A_27a") "V0m" (lambda "V7x" "A_27a" ("c_2Eextreal_2Eextreal__sub" ("V1f" "V7x") ("V2g" "V7x"))))))))))))))))))
; lebesgue.pos_fn_integral_pos_simple_fn
; ∀m f s a x.     measure_space m ∧ pos_simple_fn m f s a x ⇒     (pos_fn_integral m f = pos_simple_fn_integral m s a x)
(conjecture "thm_2Elebesgue_2Epos__fn__integral__pos__simple__fn" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool")) (lambda "V2s" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V3a" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal")) (lambda "V4x" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") (("c_2Emeasure_2Epos__simple__fn" "A_27a") "V0m" "V1f" "V2s" "V3a" "V4x")) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") (("c_2Elebesgue_2Epos__fn__integral" "A_27a") "V0m" "V1f") (("c_2Elebesgue_2Epos__simple__fn__integral" "A_27a") "V0m" "V2s" "V3a" "V4x")))))))))))))))
; lebesgue.pos_fn_integral_mspace
; ∀m f.     measure_space m ∧ (∀x. 0 ≤ f x) ⇒     (pos_fn_integral m f =      pos_fn_integral m (λx. f x * indicator_fn (m_space m) x))
(conjecture "thm_2Elebesgue_2Epos__fn__integral__mspace" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") (("c_2Ebool_2E_21" "A_27a") (lambda "V2x" "A_27a" ("c_2Eextreal_2Eextreal__le" ("c_2Eextreal_2Eextreal__of__num" "c_2Enum_2E0") ("V1f" "V2x"))))) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") (("c_2Elebesgue_2Epos__fn__integral" "A_27a") "V0m" "V1f") (("c_2Elebesgue_2Epos__fn__integral" "A_27a") "V0m" (lambda "V3x" "A_27a" ("c_2Eextreal_2Eextreal__mul" ("V1f" "V3x") (("c_2Emeasure_2Eindicator__fn" "A_27a") (("c_2Emeasure_2Em__space" "A_27a") "V0m") "V3x"))))))))))))
; lebesgue.pos_fn_integral_zero
; ∀m. measure_space m ⇒ (pos_fn_integral m (λx. 0) = 0)
(conjecture "thm_2Elebesgue_2Epos__fn__integral__zero" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) ("c_2Emin_2E_3D_3D_3E" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") (("c_2Elebesgue_2Epos__fn__integral" "A_27a") "V0m" (lambda "V1x" "A_27a" ("c_2Eextreal_2Eextreal__of__num" "c_2Enum_2E0"))) ("c_2Eextreal_2Eextreal__of__num" "c_2Enum_2E0")))))))
; lebesgue.pos_fn_integral_mono
; ∀f g. (∀x. 0 ≤ f x ∧ f x ≤ g x) ⇒ pos_fn_integral m f ≤ pos_fn_integral m g
