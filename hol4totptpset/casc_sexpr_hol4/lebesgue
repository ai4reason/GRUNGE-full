; lebesgue.psfis_intro: ∀V0m V1a V2x V3P.     measure_space V0m ∧ (∀V4i. V4i ∈ V3P ⇒ V1a V4i ∈ measurable_sets V0m) ∧     (∀V5i. V5i ∈ V3P ⇒ 0 ≤ V2x V5i) ∧ FINITE V3P ⇒     Normal (SIGMA (λV6i. V2x V6i * measure V0m (V1a V6i)) V3P) ∈     psfis V0m       (λV7t. SIGMA (λV8i. Normal (V2x V8i) * indicator_fn (V1a V8i) V7t) V3P)
(conjecture "thm_2Elebesgue_2Epsfis__intro" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V1a" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Erealax_2Ereal")) (lambda "V2x" ("ty_2Emin_2Efun" "A_27b" "ty_2Erealax_2Ereal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V3P" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27b") (lambda "V4i" "A_27b" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27b") "V4i" "V3P") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("V1a" "V4i") (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m"))))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27b") (lambda "V5i" "A_27b" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27b") "V5i" "V3P") ("c_2Ereal_2Ereal__lte" ("c_2Ereal_2Ereal__of__num" "c_2Enum_2E0") ("V2x" "V5i"))))) (("c_2Epred__set_2EFINITE" "A_27b") "V3P")))) (("c_2Ebool_2EIN" "ty_2Eextreal_2Eextreal") ("c_2Eextreal_2ENormal" (("c_2Ereal__sigma_2EREAL__SUM__IMAGE" "A_27b") (lambda "V6i" "A_27b" ("c_2Erealax_2Ereal__mul" ("V2x" "V6i") (("c_2Emeasure_2Emeasure" "A_27a") "V0m" ("V1a" "V6i")))) "V3P")) (("c_2Elebesgue_2Epsfis" "A_27a") "V0m" (lambda "V7t" "A_27a" (("c_2Eextrea