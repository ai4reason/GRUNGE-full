; lebesgue.RN_lemma1: ∀V0m V1v V2e.     measure_space V0m ∧ measure_space V1v ∧ 0 < V2e ∧     (m_space V1v = m_space V0m) ∧ (measurable_sets V1v = measurable_sets V0m) ⇒     ∃V3A.         V3A ∈ measurable_sets V0m ∧         measure V0m (m_space V0m) − measure V1v (m_space V0m) ≤         measure V0m V3A − measure V1v V3A ∧         ∀V4B.             V4B ∈ measurable_sets V0m ∧ V4B ⊆ V3A ⇒             -V2e < measure V0m V4B − measure V1v V4B
(conjecture "thm_2Elebesgue_2ERN__lemma1" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V1v" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" "ty_2Erealax_2Ereal") (lambda "V2e" "ty_2Erealax_2Ereal" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V1v") ("c_2Ebool_2E_2F_5C" ("c_2Erealax_2Ereal__lt" ("c_2Ereal_2Ereal__of__num" "c_2Enum_2E0") "V2e") ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Emeasure_2Em__space" "A_27a") "V1v") (("c_2Emeasure_2Em__space" "A_27a") "V0m")) (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V1v") (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")))))) (("c_2Ebool_2E_3F" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V3A" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("c_2