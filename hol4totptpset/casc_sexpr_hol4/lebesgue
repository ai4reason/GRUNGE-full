; lebesgue.lebesgue_monotone_convergence_subset
; ∀m f fi A.     measure_space m ∧     (∀i. fi i ∈ measurable (m_space m,measurable_sets m) Borel) ∧     (∀i x. 0 ≤ fi i x) ∧ (∀x. 0 ≤ f x) ∧     (∀x. x ∈ m_space m ⇒ (sup (IMAGE (λi. fi i x) 𝕌(:num)) = f x)) ∧     (∀x. mono_increasing (λi. fi i x)) ∧ A ∈ measurable_sets m ⇒     (pos_fn_integral m (λx. f x * indicator_fn A x) =      sup        (IMAGE (λi. pos_fn_integral m (λx. fi i x * indicator_fn A x)) 𝕌(:num)))
(conjecture "thm_2Elebesgue_2Elebesgue__monotone__convergence__subset" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal"))) (lambda "V2fi" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V3A" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V4i" "ty_2Enum_2Enum" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) ("V2fi" "V4i") (("c_2Emeasure_2Emeasurable" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Em__space" "A_27a") "V0m") (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")) "c_2Emeasure_2EBorel")))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V5i" "ty_2Enum_2Enum" (("c_2Ebool_2E_21" "A_27a") (lambda "V6x" "A_27a" ("c_2Eextreal_2Eextreal__le" ("c_2Eextreal_2Eextreal__of__num" "c_2Enum_2E0") ("V2fi" "V5i" "V6x")))))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A