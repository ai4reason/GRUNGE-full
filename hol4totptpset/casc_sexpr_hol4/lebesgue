; lebesgue.psfis_unique: ∀V0m V1f V2a V3b.     measure_space V0m ∧ V2a ∈ psfis V0m V1f ∧ V3b ∈ psfis V0m V1f ⇒     (V2a = V3b)
(conjecture "thm_2Elebesgue_2Epsfis__unique" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Ebool_2E_21" "ty_2Eextreal_2Eextreal") (lambda "V2a" "ty_2Eextreal_2Eextreal" (("c_2Ebool_2E_21" "ty_2Eextreal_2Eextreal") (lambda "V3b" "ty_2Eextreal_2Eextreal" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" "ty_2Eextreal_2Eextreal") "V2a" (("c_2Elebesgue_2Epsfis" "A_27a") "V0m" "V1f")) (("c_2Ebool_2EIN" "ty_2Eextreal_2Eextreal") "V3b" (("c_2Elebesgue_2Epsfis" "A_27a") "V0m" "V1f")))) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") "V2a" "V3b"))))))))))))
; lebesgue.pos_simple_fn_integral_indicator: ∀V0m V1A.     measure_space V0m ∧ V1A ∈ measurable_sets V0m ⇒     ∃V2s V3a V4x.         pos_simple_fn V0m (indicator_fn V1A) V2s V3a V4x ∧         (pos_simple_fn_integral V0m V2s V3a V4x = Normal (measure V0m V1A))
