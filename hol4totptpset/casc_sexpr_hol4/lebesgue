; lebesgue.lemma_fn_in_psfis
; ∀m f n.     (∀x. 0 ≤ f x) ∧ measure_space m ∧     f ∈ measurable (m_space m,measurable_sets m) Borel ⇒     fn_seq_integral m f n ∈ psfis m (fn_seq m f n)
(conjecture "thm_2Elebesgue_2Elemma__fn__in__psfis" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V2n" "ty_2Enum_2Enum" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V3x" "A_27a" ("c_2Eextreal_2Eextreal__le" ("c_2Eextreal_2Eextreal__of__num" "c_2Enum_2E0") ("V1f" "V3x")))) ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) "V1f" (("c_2Emeasure_2Emeasurable" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Em__space" "A_27a") "V0m") (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")) "c_2Emeasure_2EBorel")))) (("c_2Ebool_2EIN" "ty_2Eextreal_2Eextreal") (("c_2Elebesgue_2Efn__seq__integral" "A_27a") "V0m" "V1f" "V2n") (("c_2Elebesgue_2Epsfis" "A_27a") "V0m" (("c_2Elebesgue_2Efn__seq" "A_27a") "V0m" "V1f" "V2n"))))))))))))
; lebesgue.integral_sequence
; ∀m f.     (∀x. 0 ≤ f x) ∧ measure_space m ∧     f ∈ measurable (m_space m,measurable_sets m) Borel ⇒     (pos_fn_integral m f =      sup (IMAGE (λi. pos_fn_integral m (fn_seq m f i)) 𝕌(:num)))
(conjecture "thm_2Elebesgue_2Eintegral__sequence" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V2x" "A_27a" ("c_2Eextreal_2Eextreal__le" ("c_2Eextreal_2Eextreal__of__num" "c_2Enum_2E0") ("V1f" "V2x")))) ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) "V1f" (("c_2Emeasure_2Emeasurable" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Em__space" "A_27a") "V0m") (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")) "c_2Emeasure_2EBorel")))) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") (("c_2Elebesgue_2Epos__fn__integral" "A_27a") "V0m" "V1f") ("c_2Eextreal_2Eextreal__sup" (("c_2Epred__set_2EIMAGE" "ty_2Enum_2Enum" "ty_2Eextreal_2Eextreal") (lambda "V3i" "ty_2Enum_2Enum" (("c_2Elebesgue_2Epos__fn__integral" "A_27a") "V0m" (("c_2Elebesgue_2Efn__seq" "A_27a") "V0m" "V1f" "V3i"))) ("c_2Epred__set_2EUNIV" "ty_2Enum_2Enum")))))))))))
; lebesgue.measurable_sequence
; ∀m f.     measure_space m ∧ f ∈ measurable (m_space m,measurable_sets m) Borel ⇒     (∃fi ri.          (∀x. mono_increasing (λi. fi i x)) ∧          (∀x.               x ∈ m_space m ⇒               (sup (IMAGE (λi. fi i x) 𝕌(:num)) = fn_plus f x)) ∧          (∀i. ri i ∈ psfis m (fi i)) ∧ (∀i x. fi i x ≤ fn_plus f x) ∧          (∀i x. 0 ≤ fi i x) ∧          (pos_fn_integral m (fn_plus f) =           sup (IMAGE (λi. pos_fn_integral m (fi i)) 𝕌(:num)))) ∧     ∃gi vi.         (∀x. mono_increasing (λi. gi i x)) ∧         (∀x.              x ∈ m_space m ⇒              (sup (IMAGE (λi. gi i x) 𝕌(:num)) = fn_minus f x)) ∧         (∀i. vi i ∈ psfis m (gi i)) ∧ (∀i x. gi i x ≤ fn_minus f x) ∧         (∀i x. 0 ≤ gi i x) ∧         (pos_fn_integral m (fn_minus f) =          sup (IMAGE (λi. pos_fn_integral m (gi i)) 𝕌(:num)))
(conjecture "thm_2Elebesgue_2Emeasurable__sequence" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) "V1f" (("c_2Emeasure_2Emeasurable" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Em__space" "A_27a") "V0m") (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")) "c_2Emeasure_2EBorel"))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_3F" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal"))) (lambda "V2fi" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (("c_2Ebool_2E_3F" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Eextreal_2Eextreal")) (lambda "V3ri" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Eextreal_2Eextreal") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V4x" "A_27a" ("c_2Eextreal_2Eext__mono__increasing" (lambda "V5i" "ty_2Enum_2Enum" ("V2fi" "V5i" "V4x"))))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V6x" "A_27a" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27a") "V6x" (("c_2Emeasure_2Em__space" "A_27a") "V0m")) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") ("c_2Eextreal_2Eextreal__sup" (("c_2Epred__set_2EIMAGE" "ty_2Enum_2Enum" "ty_2Eextreal_2Eextreal") (lambda "V7i" "ty_2Enum_2Enum" ("V2fi" "V7i" "V6x")) ("c_2Epred__set_2EUNIV" "ty_2Enum_2Enum"))) (("c_2Emeasure_2Efn__plus" "A_27a") "V1f" "V6x"))))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V8i" "ty_2Enum_2Enum" (("c_2Ebool_2EIN" "ty_2Eextreal_2Eextreal") ("V3ri" "V8i") (("c_2Elebesgue_2Epsfis" "A_27a") "V0m" ("V2fi" "V8i"))))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V9i" "ty_2Enum_2Enum" (("c_2Ebool_2E_21" "A_27a") (lambda "V10x" "A_27a" ("c_2Eextreal_2Eextreal__le" ("V2fi" "V9i" "V10x") (("c_2Emeasure_2Efn__plus" "A_27a") "V1f" "V10x")))))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V11i" "ty_2Enum_2Enum" (("c_2Ebool_2E_21" "A_27a") (lambda "V12x" "A_27a" ("c_2Eextreal_2Eextreal__le" ("c_2Eextreal_2Eextreal__of__num" "c_2Enum_2E0") ("V2fi" "V11i" "V12x")))))) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") (("c_2Elebesgue_2Epos__fn__integral" "A_27a") "V0m" (("c_2Emeasure_2Efn__plus" "A_27a") "V1f")) ("c_2Eextreal_2Eextreal__sup" (("c_2Epred__set_2EIMAGE" "ty_2Enum_2Enum" "ty_2Eextreal_2Eextreal") (lambda "V13i" "ty_2Enum_2Enum" (("c_2Elebesgue_2Epos__fn__integral" "A_27a") "V0m" ("V2fi" "V13i"))) ("c_2Epred__set_2EUNIV" "ty_2Enum_2Enum"))))))))))))) (("c_2Ebool_2E_3F" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal"))) (lambda "V14gi" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (("c_2Ebool_2E_3F" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Eextreal_2Eextreal")) (lambda "V15vi" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Eextreal_2Eextreal") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V16x" "A_27a" ("c_2Eextreal_2Eext__mono__increasing" (lambda "V17i" "ty_2Enum_2Enum" ("V14gi" "V17i" "V16x"))))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V18x" "A_27a" ("c_2Emin_2