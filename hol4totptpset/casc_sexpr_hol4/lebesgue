; lebesgue.integrable_add_pos
; ∀m f g.     measure_space m ∧ integrable m f ∧ integrable m g ∧ (∀x. 0 ≤ f x) ∧     (∀x. 0 ≤ g x) ⇒     integrable m (λx. f x + g x)
(conjecture "thm_2Elebesgue_2Eintegrable__add__pos" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V2g" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Elebesgue_2Eintegrable" "A_27a") "V0m" "V1f") ("c_2Ebool_2E_2F_5C" (("c_2Elebesgue_2Eintegrable" "A_27a") "V0m" "V2g") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V3x" "A_27a" ("c_2Eextreal_2Eextreal__le" ("c_2Eextreal_2Eextreal__of__num" "c_2Enum_2E0") ("V1f" "V3x")))) (("c_2Ebool_2E_21" "A_27a") (lambda "V4x" "A_27a" ("c_2Eextreal_2Eextreal__le" ("c_2Eextreal_2Eextreal__of__num" "c_2Enum_2E0") ("V2g" "V4x")))))))) (("c_2Elebesgue_2Eintegrable" "A_27a") "V0m" (lambda "V5x" "A_27a" ("c_2Eextreal_2Eextreal__add" ("V1f" "V5x") ("V2g" "V5x")))))))))))))
; lebesgue.integrable_add_lemma
; ∀m f g.     measure_space m ∧ integrable m f ∧ integrable m g ⇒     integrable m (λx. fn_plus f x + fn_plus g x) ∧     integrable m (λx. fn_minus f x + fn_minus g x)
(conjecture "thm_2Elebesgue_2Eintegrable__add__lemma" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V2g" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Elebesgue_2Eintegrable" "A_27a") "V0m" "V1f") (("c_2Elebesgue_2Eintegrable" "A_27a") "V0m" "V2g"))) ("c_2Ebool_2E_2F_5C" (("c_2Elebesgue_2Eintegrable" "A_27a") "V0m" (lambda "V3x" "A_27a" ("c_2Eextreal_2Eextreal__add" (("c_2Emeasure_2Efn__plus" "A_27a") "V1f" "V3x") (("c_2Emeasure_2Efn__plus" "A_27a") "V2g" "V3x")))) (("c_2Elebesgue_2Eintegrable" "A_27a") "V0m" (lambda "V4x" "A_27a" ("c_2Eextreal_2Eextreal__add" (("c_2Emeasure_2Efn__minus" "A_27a") "V1f" "V4x") (("c_2Emeasure_2Efn__minus" "A_27a") "V2g" "V4x"))))))))))))))
; lebesgue.integrable_add
; ∀m f1 f2.     measure_space m ∧ integrable m f1 ∧ integrable m f2 ⇒     integrable m (λx. f1 x + f2 x)
