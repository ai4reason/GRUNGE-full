; lebesgue.pos_simple_fn_integral_sum
; ∀m f s a x P.     measure_space m ∧ (∀i. i ∈ P ⇒ pos_simple_fn m (f i) s a (x i)) ∧     FINITE P ∧ P ≠ ∅ ⇒     pos_simple_fn m (λt. SIGMA (λi. f i t) P) s a (λi. SIGMA (λj. x j i) P) ∧     (pos_simple_fn_integral m s a (λj. SIGMA (λi. x i j) P) =      SIGMA (λi. pos_simple_fn_integral m s a (x i)) P)
(conjecture "thm_2Elebesgue_2Epos__simple__fn__integral__sum" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal"))) (lambda "V1f" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool")) (lambda "V2s" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V3a" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal"))) (lambda "V4x" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V5P" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27b") (lambda "V6i" "A_27b" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27b") "V6i" "V5P") (("c_2Emeasure_2Epos__simple__fn" "A_27a") "V0m" ("V1f" "V6i") "V2s" "V3a" ("V4x" "V6i"))))) ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27b") "V5P") ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) "V5P" ("c_2Epred__set_2EEMPTY" "A_27b")))))) ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Epos__simple__fn" "A_27a") "V0m" (lambda "V7t" "A_27a" (("c_2Eextreal_2EEXTREAL__SUM__IMAGE" "A_27b") (lambda "V8i" "A_27b" ("V1f" "V8i" "V7t")) "V5P")) "V2s" "V3a" (lambda "V9i" "ty_2Enum_2Enum" (("c_2Ereal__sigma_2EREAL__SUM__IMAGE" "A_27b") (lambda "V10j" "A_27b" ("V4x" "V10j" "V9i")) "V5P"))) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") (("c_2Elebesgue_2Epos__simple__fn__integral" "A_27a") "V0m" "V2s" "V3a" (lambda "V11j" "ty_2Enum_2Enum" (("c_2Ereal__sigma_2EREAL__SUM__IMAGE" "A_27b") (lambda "V12i" "A_27b" ("V4x" "V12i" "V11j")) "V5P"))) (("c_2Eextreal_2EEXTREAL__SUM__IMAGE" "A_27b") (lambda "V13i" "A_27b" (("c_2Elebesgue_2Epos__simple__fn__integral" "A_27a") "V0m" "V2s" "V3a" ("V4x" "V13i"))) "V5P"))))))))))))))))))
; lebesgue.pos_simple_fn_integral_sum_alt
; ∀m f s a x P.     measure_space m ∧ (∀i. i ∈ P ⇒ pos_simple_fn m (f i) (s i) (a i) (x i)) ∧     FINITE P ∧ P ≠ ∅ ⇒     ∃c k z.         pos_simple_fn m (λt. SIGMA (λi. f i t) P) k c z ∧         (pos_simple_fn_integral m k c z =          SIGMA (λi. pos_simple_fn_integral m (s i) (a i) (x i)) P)
(conjecture "thm_2Elebesgue_2Epos__simple__fn__integral__sum__alt" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal"))) (lambda "V1f" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool"))) (lambda "V2s" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")))) (lambda "V3a" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal"))) (lambda "V4x" ("ty_2Emin_2Efun" "A_27b" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V5P" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27b") (lambda "V6i" "A_27b" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27b") "V6i" "V5P") (("c_2Emeasure_2Epos__simple__fn" "A_27a") "V0m" ("V1f" "V6i") ("V2s" "V6i") ("V3a" "V6i") ("V4x" "V6i"))))) ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27