; lebesgue.lemma_radon_max_in_F
; ∀f g m v.     measure_space m ∧ measure_space v ∧ (m_space v = m_space m) ∧     (measurable_sets v = measurable_sets m) ∧ f ∈ RADON_F m v ∧     g ∈ RADON_F m v ⇒     (λx. max (f x) (g x)) ∈ RADON_F m v
