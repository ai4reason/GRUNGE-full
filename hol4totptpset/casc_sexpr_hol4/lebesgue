; lebesgue.finite_space_POW_integral_reduce
; ∀m f.     measure_space m ∧ (POW (m_space m) = measurable_sets m) ∧     FINITE (m_space m) ∧ (∀x. x ∈ m_space m ⇒ f x ≠ NegInf ∧ f x ≠ PosInf) ⇒     (integral m f = SIGMA (λx. f x * Normal (measure m {x})) (m_space m))
(conjecture "thm_2Elebesgue_2Efinite__space__POW__integral__reduce" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal")) (lambda "V1f" ("ty_2Emin_2Efun" "A_27a" "ty_2Eextreal_2Eextreal") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Epred__set_2EPOW" "A_27a") (("c_2Emeasure_2Em__space" "A_27a") "V0m")) (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m")) ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27a") (("c_2Emeasure_2Em__space" "A_27a") "V0m")) (("c_2Ebool_2E_21" "A_27a") (lambda "V2x" "A_27a" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27a") "V2x" (("c_2Emeasure_2Em__space" "A_27a") "V0m")) ("c_2Ebool_2E_2F_5C" ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") ("V1f" "V2x") "c_2Eextreal_2ENegInf")) ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") ("V1f" "V2x") "c_2Eextreal_2EPosInf"))))))))) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") (("c_2Elebesgue_2Eintegral" "A_27a") "V0m" "V1f") (("c_2Eextreal_2EEXTREAL__SUM__IMAGE" "A_27a") (lambda "V3x" "A_27a" ("c_2Eextreal_2Eextreal__mul" ("V1f" "V3x") ("c_2Eextreal_2ENormal" (("c_2Emeasure_2Emeasure" "A_27a") "V0m" (("c_2Epred__set_2EINSERT" "A_27a") "V3x" ("c_2Epred__set_2EEMPTY" "A_27a")))))) (("c_2Emeasure_2Em__space" "A_27a") "V0m"))))))))))
; lebesgue.finite_POW_prod_measure_reduce
; ∀m0 m1.     measure_space m0 ∧ measure_space m1 ∧ FINITE (m_space m0) ∧     FINITE (m_space m1) ∧ (POW (m_space m0) = measurable_sets m0) ∧     (POW (m_space m1) = measurable_sets m1) ⇒     ∀a0 a1.         a0 ∈ measurable_sets m0 ∧ a1 ∈ measurable_sets m1 ⇒         (prod_measure m0 m1 (a0 × a1) = measure m0 a0 * measure m1 a1)
(conjecture "thm_2Elebesgue_2Efinite__POW__prod__measure__reduce" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m0" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V1m1" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m0") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27b") "V1m1") ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27a") (("c_2Emeasure_2Em__space" "A_27a") "V0m0")) ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27b") (("c_2Emeasure_2Em__space" "A_27b") "V1m1")) ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Epred__set_2EPOW" "A_27a") (("c_2Emeasure_2Em__space" "A_27a") "V0m0")) (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m0")) (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Epred__set_2EPOW" "A_27b") (("c_2Emeasure_2Em__space" "A_27b") "V1m1")) (("c_2Emeasure_2Emeasurable__sets" "A_27b") "V1m1"))))))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V2a0" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V3a1" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V2a0" (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m0")) (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) "V3a1" (("c_2Emeasure_2Emeasurable__sets" "A_27b") "V1m1"))) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Elebesgue_2Eprod__measure" "A_27a" "A_27b") "V0m0" "V1m1" (("c_2Epred__set_2ECROSS" "A_27a" "A_27b") "V2a0" "V3a1")) ("c_2Erealax_2Ereal__mul" (("c_2Emeasure_2Emeasure" "A_27a") "V0m0" "V2a0") (("c_2Emeasure_2Emeasure" "A_27b") "V1m1" "V3a1")))))))))))))))
; lebesgue.measure_space_finite_prod_measure_POW1
; ∀m0 m1.     measure_space m0 ∧ measure_space m1 ∧ FINITE (m_space m0) ∧     FINITE (m_space m1) ∧ (POW (m_space m0) = measurable_sets m0) ∧     (POW (m_space m1) = measurable_sets m1) ⇒     measure_space (prod_measure_space m0 m1)
(conjecture "thm_2Elebesgue_2Emeasure__space__finite__prod__measure__POW1" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m0" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V1m1" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m0") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27b") "V1m1") ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27a") (("c_2Emeasure_2Em__space" "A_27a") "V0m0")) ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27b") (("c_2Emeasure_2Em__space" "A_27b") "V1m1")) ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Epred__set_2EPOW" "A_27a") (("c_2Emeasure_2Em__space" "A_27a") "V0m0")) (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m0")) (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Epred__set_2EPOW" "A_27b") (("c_2Emeasure_2Em__space" "A_27b") "V1m1")) (("c_2Emeasure_2Emeasurable__sets" "A_27b") "V1m1"))))))) (("c_2Emeasure_2Emeasure__space" ("ty_2Epair_2Eprod" "A_27a" "A_27b")) (("c_2Elebesgue_2Eprod__measure__space" "A_27a" "A_27b") "V0m0" "V1m1")))))))))
; lebesgue.measure_space_finite_prod_measure_POW2
; ∀m0 m1.     measure_space m0 ∧ measure_space m1 ∧ FINITE (m_space m0) ∧     FINITE (m_space m1) ∧ (POW (m_space m0) = measurable_sets m0) ∧     (POW (m_space m1) = measurable_sets m1) ⇒     measure_space       (m_space m0 × m_space m1,POW (m_space m0 × m_space m1),        prod_measure m0 m1)
(conjecture "thm_2Elebesgue_2Emeasure__space__finite__prod__measure__POW2" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m0" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V1m1" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m0") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27b") "V1m1") ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27a") (("c_2Emeasure_2Em__space" "A_27a") "V0m0")) ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27b") (("c_2Emeasure_2Em__space" "A_27b") "V1m1")) ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Epred__set_2EPOW" "A_27a") (("c_2Emeasure_2Em__space" "A_27a") "V0m0")) (("c_2Emeasure_2Emeasurable__sets" "A_27a") "V0m0")) (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Epred__set_2EPOW" "A_27b") (("c_2Emeasure_2Em__space" "A_27b") "V1m1")) (("c_2Emeasure_2Emeasurable__sets" "A_27b") "V1m1"))))))) (("c_2Emeasure_2Emeasure__space" ("ty_2Epair_2Eprod" "A_27a" "A_27b")) (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27a" "A_27b") "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27a" "A_27b") "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27a" "A_27b") "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Epred__set_2ECROSS" "A_27a" "A_27b") (("c_2Emeasure_2Em__space" "A_27a") "V0m0") (("c_2Emeasure_2Em__space" "A_27b") "V1m1")) (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27a" "A_27b") "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27a" "A_27b") "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")) (("c_2Epred__set_2EPOW" ("ty_2Epair_2Eprod" "A_27a" "A_27b")) (("c_2Epred__set_2ECROSS" "A_27a" "A_27b") (("c_2Emeasure_2Em__space" "A_27a") "V0m0") (("c_2Emeasure_2Em__space" "A_27b") "V1m1"))) (("c_2Elebesgue_2Eprod__measure" "A_27a" "A_27b") "V0m0" "V1m1")))))))))))
; lebesgue.finite_prod_measure_space_POW
; ∀s1 s2 u v.     FINITE s1 ∧ FINITE s2 ⇒     (prod_measure_space (s1,POW s1,u) (s2,POW s2,v) =      (s1 × s2,POW (s1 × s2),prod_measure (s1,POW s1,u) (s2,POW s2,v)))
(conjecture "thm_2Elebesgue_2Efinite__prod__measure__space__POW" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V0s1" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (lambda "V1s2" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")) (lambda "V2u" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")) (lambda "V3v" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Epred__set_2EFINITE" "A_27a") "V0s1") (("c_2Epred__set_2EFINITE" "A_27b") "V1s2")) (("c_2Emin_2E_3D" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27a" "A_27b") "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27a" "A_27b") "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" ("ty_2Epair_2Eprod" "A_27a" "A_27b") "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (("c_2Elebesgue_2Eprod__measure__space" "A_27a" "A_27b") (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) "V0s1" (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")) (("c_2Epred__set_2EPOW" "A_27a") "V0s1") "V2u")) (("c_2Epair_2E_2C" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emi