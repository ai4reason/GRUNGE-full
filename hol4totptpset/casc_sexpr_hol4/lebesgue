; lebesgue.lemma_radon_seq_conv_sup
; ∀f m v.     measure_space m ∧ measure_space v ∧ (m_space v = m_space m) ∧     (measurable_sets v = measurable_sets m) ⇒     ∃f_n.         (∀n. f_n n ∈ RADON_F m v) ∧ (∀x n. f_n n x ≤ f_n (SUC n) x) ∧         (sup (IMAGE (λn. pos_fn_integral m (f_n n)) 𝕌(:num)) =          sup (RADON_F_integrals m v))
(conjecture "thm_2Elebesgue_2Elemma__radon__seq__conv__sup" (forall_tyvarl_term ("A_27a" "A_27b") (("c_2Ebool_2E_21" "A_27a") (lambda "V0f" "A_27a" (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V1m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V2v" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27b") "V1m") ("c_2Ebool_2E_2F_5C" (("c_2Emeasure_2Emeasure__space" "A_27b") "V2v") ("c_2Ebool_2E_2F_5C" (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool")) (("c_2Emeasure_2Em__space" "A_27b") "V2v") (("c_2Emeasure_2Em__space" "A_27b") "V1m")) (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27b" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool")) (("c_2Emeasure_2Emeasurable__sets" "A_27b") "V2v") (("c_2Emeasure_2Emeasurable__sets" "A_27b") "V1m"))))) (("c_2Ebool_2E_3F" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27b" "ty_2Eextreal_2Eextreal"))) (lambda "V3f__n" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27b" "ty_2Eextreal_2Eextreal")) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V4n" "ty_2Enum_2Enum" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27b" "ty_2Eextreal_2Eextreal")) ("V3f__n" "V4n") (("c_2Elebesgue_2ERADON__F" "A_27b") "V1m" "V2v")))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27b") (lambda "V5x" "A_27b" (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V6n" "ty_2Enum_2Enum" ("c_2Eextreal_2Eextreal__le" ("V3f__n" "V6n" "V5x") ("V3f__n" ("c_2Enum_2ESUC" "V6n") "V5x")))))) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") ("c_2Eextreal_2Eextreal__sup" (("c_2Epred__set_2EIMAGE" "ty_2Enum_2Enum" "ty_2Eextreal_2Eextreal") (lambda "V7n" "ty_2Enum_2Enum" (("c_2Elebesgue_2Epos__fn__integral" "A_27b") "V1m" ("V3f__n" "V7n"))) ("c_2Epred__set_2EUNIV" "ty_2Enum_2Enum"))) ("c_2Eextreal_2Eextreal__sup" (("c_2Elebesgue_2ERADON__F__integrals" "A_27b") "V1m" "V2v"))))))))))))))))
; lebesgue.RN_lemma1
; ∀m v e.     measure_space m ∧ measure_space v ∧ 0 < e ∧ (m_space v = m_space m) ∧     (measurable_sets v = measurable_sets m) ⇒     ∃A.         A ∈ measurable_sets m ∧         measure m (m_space m) − measure v (m_space m) ≤         measure m A − measure v A ∧         ∀B. B ∈ measurable_sets m ∧ B ⊆ A ⇒ -e < measure m B − measure v B
