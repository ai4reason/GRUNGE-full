; lebesgue.psfis_zero: ∀V0m V1a. measure_space V0m ⇒ (V1a ∈ psfis V0m (λV2x. 0) ⇔ (V1a = 0))
(conjecture "thm_2Elebesgue_2Epsfis__zero" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" "ty_2Eextreal_2Eextreal") (lambda "V1a" "ty_2Eextreal_2Eextreal" ("c_2Emin_2E_3D_3D_3E" (("c_2Emeasure_2Emeasure__space" "A_27a") "V0m") (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Ebool_2EIN" "ty_2Eextreal_2Eextreal") "V1a" (("c_2Elebesgue_2Epsfis" "A_27a") "V0m" (lambda "V2x" "A_27a" ("c_2Eextreal_2Eextreal__of__num" "c_2Enum_2E0")))) (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") "V1a" ("c_2Eextreal_2Eextreal__of__num" "c_2Enum_2E0"))))))))))
; lebesgue.pos_simple_fn_integral_not_infty: ∀V0m V1s V2a V3x.     pos_simple_fn_integral V0m V1s V2a V3x ≠ NegInf ∧     pos_simple_fn_integral V0m V1s V2a V3x ≠ PosInf
(conjecture "thm_2Elebesgue_2Epos__simple__fn__integral__not__infty" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0m" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool")) (lambda "V1s" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Emin_2Ebool") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V2a" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal")) (lambda "V3x" ("ty_2Emin_2Efun" "ty_2Enum_2Enum" "ty_2Erealax_2Ereal") ("c_2Ebool_2E_2F_5C" ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") (("c_2Elebesgue_2Epos__simple__fn__integral" "A_27a") "V0m" "V1s" "V2a" "V3x") "c_2Eextreal_2ENegInf")) ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "ty_2Eextreal_2Eextreal") (("c_2Elebesgue_2Epos__simple__fn__integral" "A_27a") "V0m" "V1s" "V2a" "V3x") "c_2Eextreal_2EPosInf")))))))))))))
; lebesgue.psfis_not_infty: ∀V0m V1f V2a. V2a ∈ psfis V0m V1f ⇒ V2a ≠ NegInf ∧ V2a ≠ PosInf
