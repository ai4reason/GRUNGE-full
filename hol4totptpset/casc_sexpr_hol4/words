; words.add_with_carry_def
; ∀x y carry_in.     add_with_carry (x,y,carry_in) =     (let        unsigned_sum = w2n x + w2n y + if carry_in then 1 else 0 ;        result = n2w unsigned_sum ;        carry_out = w2n result ≠ unsigned_sum and        overflow = (word_msb x ⇔ word_msb y) ∧ (word_msb x ⇎ word_msb result)      in        (result,carry_out,overflow))
