(axiom "thm_2Ewords_2Edimword__def" (forall_tyvarl_term ("A_27a") (("c_2Emin_2E_3D" "ty_2Enum_2Enum") (("c_2Ewords_2Edimword" "A_27a") ("c_2Ebool_2Ethe__value" "A_27a")) ("c_2Earithmetic_2EEXP" ("c_2Earithmetic_2ENUMERAL" ("c_2Earithmetic_2EBIT2" "c_2Earithmetic_2EZERO")) (("c_2Efcp_2Edimindex" "A_27a") ("c_2Ebool_2Ethe__value" "A_27a"))))))
