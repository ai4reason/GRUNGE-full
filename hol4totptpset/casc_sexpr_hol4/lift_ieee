; lift_ieee.float_div_finite
; ∀a b.     float_is_finite a ∧ float_is_finite b ∧ ¬float_is_zero b ∧     abs (float_to_real a / float_to_real b) < threshold (:τ # χ) ⇒     float_is_finite (SND ((roundTiesToEven float_div a) b))
(conjecture "thm_2Elift__ieee_2Efloat__div__finite" (forall_tyvarl_term ("A_27t" "A_27w") (("c_2Ebool_2E_21" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w")) (lambda "V0a" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w") (("c_2Ebool_2E_21" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w")) (lambda "V1b" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Ebinary__ieee_2Efloat__is__finite" "A_27t" "A_27w") "V0a") ("c_2Ebool_2E_2F_5C" (("c_2Ebinary__ieee_2Efloat__is__finite" "A_27t" "A_27w") "V1b") ("c_2Ebool_2E_2F_5C" ("c_2Ebool_2E_7E" (("c_2Ebinary__ieee_2Efloat__is__zero" "A_27t" "A_27w") "V1b")) ("c_2Erealax_2Ereal__lt" ("c_2Ereal_2Eabs" ("c_2Ereal_2E_2F" (("c_2Ebinary__ieee_2Efloat__to__real" "A_27t" "A_27w") "V0a") (("c_2Ebinary__ieee_2Efloat__to__real" "A_27t" "A_27w") "V1b"))) (("c_2Ebinary__ieee_2Ethreshold" "A_27t" "A_27w") ("c_2Ebool_2Ethe__value" ("ty_2Epair_2Eprod" "A_27t" "A_27w"))))))) (("c_2Ebinary__ieee_2Efloat__is__finite" "A_27t" "A_27w") (("c_2Epair_2ESND" "ty_2Ebinary__ieee_2Eflags" ("ty_2Ebinary__ieee_2Efloat" "A_27t" "A_27w")) (("c_2Ebinary__ieee_2Efloat__div" "A_27t" "A_27w") "c_2Ebinary__ieee_2EroundTiesToEven" "V0a" "V1b"))))))))))
; lift_ieee.float_sqrt_finite
; ∀a.     float_is_finite a ∧ (a.Sign = 0w) ∧     abs (sqrt (float_to_real a)) < threshold (:τ # χ) ⇒     float_is_finite (SND (float_sqrt roundTiesToEven a))
