(conjecture "thm_2Emergesort_2EmergesortN__perm" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V0R" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V1n" "ty_2Enum_2Enum" (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V2l" ("ty_2Elist_2Elist" "A_27a") (("c_2Esorting_2EPERM" "A_27a") (("c_2Elist_2ETAKE" "A_27a") "V1n" "V2l") (("c_2Emergesort_2EmergesortN" "A_27a") "V0R" "V1n" "V2l"))))))))))
; mergesort.mergesort_perm
