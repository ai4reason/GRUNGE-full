; mergesort.mergesortN_def
; (∀l R. mergesortN R 0 l = []) ∧ (∀x l R. mergesortN R 1 (x::l) = [x]) ∧ (∀R. mergesortN R 1 [] = []) ∧ (∀y x l R. mergesortN R 2 (x::y::l) = sort2 R x y) ∧ (∀x R. mergesortN R 2 [x] = [x]) ∧ (∀R. mergesortN R 2 [] = []) ∧ (∀z y x l R. mergesortN R 3 (x::y::z::l) = sort3 R x y z) ∧ (∀y x R. mergesortN R 3 [x; y] = sort2 R x y) ∧ (∀x R. mergesortN R 3 [x] = [x]) ∧ (∀R. mergesortN R 3 [] = []) ∧ ∀v4 l R.     mergesortN R v4 l =     if v4 = 0 then []     else if v4 = 1 then case l of [] => [] | x::l' => [x]     else if v4 = 2 then       case l of [] => [] | [x'] => [x'] | x'::y::l'' => sort2 R x' y     else if v4 = 3 then       case l of         [] => []       | [x''] => [x'']       | [x''; y'] => sort2 R x'' y'       | x''::y'::z::l'³' => sort3 R x'' y' z     else       (let          len1 = DIV2 v4        in          merge R (mergesortN R (DIV2 v4) l)            (mergesortN R (v4 − len1) (DROP len1 l)))
