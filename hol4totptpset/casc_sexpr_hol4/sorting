; sorting.PERM_CONG_2
; ∀l1 l1' l2 l2'. PERM l1 l1' ⇒ PERM l2 l2' ⇒ (PERM l1 l2 ⇔ PERM l1' l2')
(conjecture "thm_2Esorting_2EPERM__CONG__2" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V0l1" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V1l1_27" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V2l2" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V3l2_27" ("ty_2Elist_2Elist" "A_27a") ("c_2Emin_2E_3D_3D_3E" (("c_2Esorting_2EPERM" "A_27a") "V0l1" "V1l1_27") ("c_2Emin_2E_3D_3D_3E" (("c_2Esorting_2EPERM" "A_27a") "V2l2" "V3l2_27") (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Esorting_2EPERM" "A_27a") "V0l1" "V2l2") (("c_2Esorting_2EPERM" "A_27a") "V1l1_27" "V3l2_27"))))))))))))))
; sorting.PERM_CONG_APPEND_IFF
; ∀l l1 l1' l2 l2'.     PERM l1 (l ++ l1') ⇒ PERM l2 (l ++ l2') ⇒ (PERM l1 l2 ⇔ PERM l1' l2')
(conjecture "thm_2Esorting_2EPERM__CONG__APPEND__IFF" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V0l" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V1l1" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V2l1_27" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V3l2" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V4l2_27" ("ty_2Elist_2Elist" "A_27a") ("c_2Emin_2E_3D_3D_3E" (("c_2Esorting_2EPERM" "A_27a") "V1l1" (("c_2Elist_2EAPPEND" "A_27a") "V0l" "V2l1_27")) ("c_2Emin_2E_3D_3D_3E" (("c_2Esorting_2EPERM" "A_27a") "V3l2" (("c_2Elist_2EAPPEND" "A_27a") "V0l" "V4l2_27")) (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Esorting_2EPERM" "A_27a") "V1l1" "V3l2") (("c_2Esorting_2EPERM" "A_27a") "V2l1_27" "V4l2_27"))))))))))))))))
; sorting.PERM_CONG_APPEND_IFF2
; ∀l1 l1' l1'' l2 l2' l2''.     PERM l1 (l1' ++ l1'') ⇒     PERM l2 (l2' ++ l2'') ⇒     PERM l1' l2' ⇒     (PERM l1 l2 ⇔ PERM l1'' l2'')
