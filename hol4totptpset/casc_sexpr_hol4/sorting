; sorting.SORTED_DEF
; (∀R. SORTED R [] ⇔ T) ∧ (∀x R. SORTED R [x] ⇔ T) ∧ ∀y x rst R. SORTED R (x::y::rst) ⇔ R x y ∧ SORTED R (y::rst)
(conjecture "thm_2Esorting_2ESORTED__DEF" (forall_tyvarl_term ("A_27a") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V0R" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Esorting_2ESORTED" "A_27a") "V0R" ("c_2Elist_2ENIL" "A_27a")) "c_2Ebool_2ET"))) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V1x" "A_27a" (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V2R" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Esorting_2ESORTED" "A_27a") "V2R" (("c_2Elist_2ECONS" "A_27a") "V1x" ("c_2Elist_2ENIL" "A_27a"))) "c_2Ebool_2ET"))))) (("c_2Ebool_2E_21" "A_27a") (lambda "V3y" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V4x" "A_27a" (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V5rst" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V6R" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Esorting_2ESORTED" "A_27a") "V6R" (("c_2Elist_2ECONS" "A_27a") "V4x" (("c_2Elist_2ECONS" "A_27a") "V3y" "V5rst"))) ("c_2Ebool_2E_2F_5C" ("V6R" "V4x" "V3y") (("c_2Esorting_2ESORTED" "A_27a") "V6R" (("c_2Elist_2ECONS" "A_27a") "V3y" "V5rst"))))))))))))))))
; sorting.SORTS_DEF
; ∀f R. SORTS f R ⇔ ∀l. PERM l (f R l) ∧ SORTED R (f R l)
