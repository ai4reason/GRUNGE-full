; sorting.PERM_TO_APPEND_SIMPS
; (PERM (x::l) (x::r1 ++ r2) ⇔ PERM l (r1 ++ r2)) ∧ (PERM (x::l) (r1 ++ x::r2) ⇔ PERM l (r1 ++ r2)) ∧ (PERM (xs ++ ys ++ zs) r ⇔ PERM (xs ++ (ys ++ zs)) r) ∧ (PERM (x::ys ++ zs) r ⇔ PERM (x::(ys ++ zs)) r) ∧ (PERM ([] ++ l) r ⇔ PERM l r) ∧ (PERM (xs ++ l) (xs ++ r1 ++ r2) ⇔ PERM l (r1 ++ r2)) ∧ (PERM (xs ++ l) (r1 ++ (xs ++ r2)) ⇔ PERM l (r1 ++ r2)) ∧ (PERM [] ([] ++ []) ⇔ T) ∧ (PERM xs (xs ++ [] ++ []) ⇔ T) ∧ (PERM xs ([] ++ (xs ++ [])) ⇔ T)
