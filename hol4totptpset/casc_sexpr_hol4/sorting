; sorting.STABLE_DEF
; ∀sort r.     STABLE sort r ⇔     SORTS sort r ∧     ∀p. (∀x y. p x ∧ p y ⇒ r x y) ⇒ ∀l. FILTER p l = FILTER p (sort r l)
(axiom "thm_2Esorting_2ESTABLE__DEF" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("ty_2Emin_2Efun" ("ty_2Elist_2Elist" "A_27a") ("ty_2Elist_2Elist" "A_27a")))) (lambda "V0sort" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) ("ty_2Emin_2Efun" ("ty_2Elist_2Elist" "A_27a") ("ty_2Elist_2Elist" "A_27a"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V1r" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") (("c_2Esorting_2ESTABLE" "A_27a") "V0sort" "V1r") ("c_2Ebool_2E_2F_5C" (("c_2Esorting_2ESORTS" "A_27a") "V0sort" "V1r") (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V2p" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2E_21" "A_27a") (lambda "V3x" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V4y" "A_27a" ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" ("V2p" "V3x") ("V2p" "V4y")) ("V1r" "V3x" "V4y")))))) (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V5l" ("ty_2Elist_2Elist" "A_27a") (("c_2Emin_2E_3D" ("ty_2Elist_2Elist" "A_27a")) (("c_2Elist_2EFILTER" "A_27a") "V2p" "V5l") (("c_2Elist_2EFILTER" "A_27a") "V2p" ("V0sort" "V1r" "V5l"))))))))))))))))
; sorting.PART3_DEF
; (∀R h. PART3 R h [] = ([],[],[])) ∧ ∀R h hd tl.     PART3 R h (hd::tl) =     if R h hd ∧ R hd h then (I ## (CONS hd ## I)) (PART3 R h tl)     else if R hd h then (CONS hd ## (I ## I)) (PART3 R h tl)     else (I ## (I ## CONS hd)) (PART3 R h tl)
(axiom "thm_2Esorting_2EPART3__DEF" (forall_tyvarl_term ("A_27a") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V0R" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" "A_27a") (lambda "V1h" "A_27a" (("c_2Emin_2E_3D" ("ty_2Epair_2Eprod" ("ty_2Elist_2Elist" "A_27a") ("ty_2Epair_2Eprod" ("ty_2Elist_2Elist" "A_27a") ("ty_2Elist_2Elist" "A_27a")))) (("c_2Esorting_2EPART3" "A_27a") "V0R" "V1h" ("c_2Elist_2ENIL" "A_27a")) (("c_2Epair_2E_2C" ("ty_2Elist_2Elist" "A_27a") ("ty_2Epair_2Eprod" ("ty_2Elist_2Elist" "A_27a") ("ty_2Elist_2Elist" "A_27a"))) ("c_2Elist_2ENIL" "A_27a") (("c_2Epair_2E_2C" ("ty_2Elist_2Elist" "A_27a") ("ty_2Elist_2Elist" "A_27a")) ("c_2Elist_2ENIL" "A_27a") ("c_2Elist_2ENIL" "A_27a")))))))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool"))) (lambda "V2R" ("ty_2Emin_2Efun" "A_27a" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Ebool_2E_21" "A_27a") (lambda "V3h" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V4hd" "A_27a" (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V5tl" ("ty_2Elist_2Elist" "A_27a") (("c_2Emin_2E_3D" ("ty_2Epair_2Eprod" ("ty_2Elist_2Elist" "A_27a") ("ty_2Epair_2Eprod" ("ty_2Elist_2Elist" "A_27a") ("ty_2Elist_2Elist" "A_27a")))) (("c_2Esorting_2EPART3" "A_27a") "V2R" "V3h" (("c_2Elist_2ECONS" "A_27a") "V4hd" "V5tl")) (("c_2Ebool_2ECOND" ("ty_2Epair_2Eprod" ("ty_2Elist_2Elist" "A_27a") ("ty_2Epair_2Eprod" ("ty_2Elist_2Elist" "A_27a") ("ty_2Elist_2Elist" "A_27a")))) ("c_2Ebool_2E_2F_5C" ("V2R" "V3h" "V4hd") ("V2R" "V4hd" "V3h")) (("c_2Epair_2E_23_23" ("ty_2Elist_2Elist" "A_27a") ("ty_2Epair_2Eprod" ("ty_2Elist_2Elist" "A_27a") ("ty_2Elist_2Elist" "A_27a")) ("ty_2Elist_2Elist" "A_27a") ("ty_2Epair_2Eprod" ("ty_2Elist_2Elist" "A_27a") ("ty_2Elist_2Elist" "A_27a"))) ("c_2Ecombin_2EI" ("ty_2Elist_2Elist" "A_27a")) (("c_2Epair_2E_23_23" ("ty_2Elist_2Elist" "A_27a") ("ty_2El