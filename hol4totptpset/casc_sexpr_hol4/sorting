; sorting.PERM_FUN_APPEND_CONS
; ∀x l1 l2. PERM (l1 ++ x::l2) = PERM (x::l1 ++ l2)
(conjecture "thm_2Esorting_2EPERM__FUN__APPEND__CONS" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" "A_27a") (lambda "V0x" "A_27a" (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V1l1" ("ty_2Elist_2Elist" "A_27a") (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V2l2" ("ty_2Elist_2Elist" "A_27a") (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Elist_2Elist" "A_27a") "ty_2Emin_2Ebool")) (("c_2Esorting_2EPERM" "A_27a") (("c_2Elist_2EAPPEND" "A_27a") "V1l1" (("c_2Elist_2ECONS" "A_27a") "V0x" "V2l2"))) (("c_2Esorting_2EPERM" "A_27a") (("c_2Elist_2EAPPEND" "A_27a") (("c_2Elist_2ECONS" "A_27a") "V0x" "V1l1") "V2l2")))))))))))
; sorting.PERM_FUN_SWAP_AT_FRONT
; ∀x y l. PERM (x::y::l) = PERM (y::x::l)
(conjecture "thm_2Esorting_2EPERM__FUN__SWAP__AT__FRONT" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" "A_27a") (lambda "V0x" "A_27a" (("c_2Ebool_2E_21" "A_27a") (lambda "V1y" "A_27a" (("c_2Ebool_2E_21" ("ty_2Elist_2Elist" "A_27a")) (lambda "V2l" ("ty_2Elist_2Elist" "A_27a") (("c_2Emin_2E_3D" ("ty_2Emin_2Efun" ("ty_2Elist_2Elist" "A_27a") "ty_2Emin_2Ebool")) (("c_2Esorting_2EPERM" "A_27a") (("c_2Elist_2ECONS" "A_27a") "V0x" (("c_2Elist_2ECONS" "A_27a") "V1y" "V2l"))) (("c_2Esorting_2EPERM" "A_27a") (("c_2Elist_2ECONS" "A_27a") "V1y" (("c_2Elist_2ECONS" "A_27a") "V0x" "V2l"))))))))))))
; sorting.PERM_FUN_CONS_11_SWAP_AT_FRONT
