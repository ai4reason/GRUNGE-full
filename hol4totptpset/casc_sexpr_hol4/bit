(conjecture "thm_2Ebit_2EADD__BIT0" (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V0m" "ty_2Enum_2Enum" (("c_2Ebool_2E_21" "ty_2Enum_2Enum") (lambda "V1n" "ty_2Enum_2Enum" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") ("c_2Ebit_2EBIT" "c_2Enum_2E0" ("c_2Earithmetic_2E_2B" "V0m" "V1n")) ("c_2Ebool_2E_7E" (("c_2Emin_2E_3D" "ty_2Emin_2Ebool") ("c_2Ebit_2EBIT" "c_2Enum_2E0" "V0m") ("c_2Ebit_2EBIT" "c_2Enum_2E0" "V1n")))))))))
; bit.ADD_BITS_SUC
; ∀n a b.     BITS (SUC n) (SUC n) (a + b) =     (BITS (SUC n) (SUC n) a + BITS (SUC n) (SUC n) b +      BITS (SUC n) (SUC n) (BITS n 0 a + BITS n 0 b)) MOD 2
