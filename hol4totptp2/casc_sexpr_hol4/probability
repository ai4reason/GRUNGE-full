; probability.PROB_REAL_SUM_IMAGE
; âˆ€p s.     prob_space p âˆ§ s âˆˆ events p âˆ§ (âˆ€x. x âˆˆ s â‡’ {x} âˆˆ events p) âˆ§ FINITE s â‡’     (prob p s = SIGMA (Î»x. prob p {x}) s)
(conjecture "thm_2Eprobability_2EPROB__REAL__SUM__IMAGE" (forall_tyvarl_term ("A_27a") (("c_2Ebool_2E_21" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal")))) (lambda "V0p" ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("ty_2Epair_2Eprod" ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Emin_2Ebool") ("ty_2Emin_2Efun" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") "ty_2Erealax_2Ereal"))) (("c_2Ebool_2E_21" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (lambda "V1s" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool") ("c_2Emin_2E_3D_3D_3E" ("c_2Ebool_2E_2F_5C" (("c_2Eprobability_2Eprob__space" "A_27a") "V0p") ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) "V1s" (("c_2Eprobability_2Eevents" "A_27a") "V0p")) ("c_2Ebool_2E_2F_5C" (("c_2Ebool_2E_21" "A_27a") (lambda "V2x" "A_27a" ("c_2Emin_2E_3D_3D_3E" (("c_2Ebool_2EIN" "A_27a") "V2x" "V1s") (("c_2Ebool_2EIN" ("ty_2Emin_2Efun" "A_27a" "ty_2Emin_2Ebool")) (("c_2Epred__set_2EINSERT" "A_27a") "V2x" ("c_2Epred__set_2EEMPTY" "A_27a")) (("c_2Eprobability_2Eevents" "A_27a") "V0p"))))) (("c_2Epred__set_2EFINITE" "A_27a") "V1s")))) (("c_2Emin_2E_3D" "ty_2Erealax_2Ereal") (("c_2Eprobability_2Eprob" "A_27a") "V0p" "V1s") (("c_2Ereal__sigma_2EREAL__SUM__IMAGE" "A_27a") (lambda "V3x" "A_27a" (("c_2Eprobability_2Eprob" "A_27a") "V0p" (("c_2Epred__set_2EINSERT" "A_27a") "V3x" ("c_2Epred__set_2EEMPTY" "A_27a")))) "V1s")))))))))
; probability.PROB_EQUIPROBABLE_FINITE_UNIONS
; âˆ€p s.     prob_space p âˆ§ s âˆˆ events p âˆ§ (âˆ€x. x âˆˆ s â‡’ {x} âˆˆ events p) âˆ§ FINITE s âˆ§     (âˆ€x y. x âˆˆ s âˆ§ y âˆˆ s â‡’ (prob p {x} = prob p {y})) â‡’     (prob p s = &CARD s * prob p {CHOICE s})
